<?xml version="1.0" encoding="utf-8"?>
<xsd:schema targetNamespace="http://xmlns.oracle.com/apps/gtm/transmission/v6.4"
            xmlns="http://xmlns.oracle.com/apps/gtm/transmission/v6.4"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns:otm="http://xmlns.oracle.com/apps/otm/transmission/v6.4"
            attributeFormDefault="unqualified"
            elementFormDefault="qualified">
   <xsd:annotation>
      <xsd:documentation>
         Release 6.4.2 GTM Schema. Last modified Nov, 2016.
         Contains element, attribute and type definitions common to all GTM interfaces.
      </xsd:documentation>
   </xsd:annotation>

   <xsd:import namespace="http://xmlns.oracle.com/apps/otm/transmission/v6.4" schemaLocation="TransmissionCommon.xsd"/>
   <xsd:import namespace="http://xmlns.oracle.com/apps/otm/transmission/v6.4" schemaLocation="LocationContact.xsd"/>
   <xsd:import namespace="http://xmlns.oracle.com/apps/otm/transmission/v6.4" schemaLocation="Shipment.xsd"/>
   <xsd:import namespace="http://xmlns.oracle.com/apps/otm/transmission/v6.4" schemaLocation="Order.xsd"/>
   <xsd:import namespace="http://xmlns.oracle.com/apps/otm/transmission/v6.4" schemaLocation="Document.xsd"/>
   <xsd:import namespace="http://xmlns.oracle.com/apps/otm/transmission/v6.4" schemaLocation="Item.xsd"/>

   <xsd:element name="GtmContact" type="GtmContactType" substitutionGroup="otm:GLogXMLTransaction">
      <xsd:annotation>
         <xsd:documentation>
            Interface to create or exchange a Trading Party and it's details.
         </xsd:documentation>
      </xsd:annotation>
   </xsd:element>

   <xsd:element name="GtmRegistration" type="GtmRegistrationType" substitutionGroup="otm:GLogXMLTransaction">
      <xsd:annotation>
         <xsd:documentation>
            Represents the registrations existing in external systems that are relevant for trade purposes.
            GTM supports different type of registrations: for parties (e.g. legal entities, company branches,
            customers, carriers, etc.) and for items.
         </xsd:documentation>
      </xsd:annotation>
   </xsd:element>

   <xsd:element name="GtmTransaction" type="GtmTransactionType" substitutionGroup="otm:GLogXMLTransaction">
      <xsd:annotation>
         <xsd:documentation>
            Represents a Global Trade Management trade transaction for the screening of shipment and/or order release information.
         </xsd:documentation>
      </xsd:annotation>
   </xsd:element>

   <xsd:element name="GtmTransactionLine" type="GtmTransactionLineType" substitutionGroup="otm:GLogXMLTransaction">
      <xsd:annotation>
         <xsd:documentation>
            Represents a Global Trade Management trade transaction line.
         </xsd:documentation>
      </xsd:annotation>
   </xsd:element>

   <xsd:element name="GtmStructure" type="GtmStructureType" substitutionGroup="otm:GLogXMLTransaction">
      <xsd:annotation>
         <xsd:documentation>
            Represents Trade Item Structure (bill of material) to identify components that make up a single sellable product or item.
         </xsd:documentation>
      </xsd:annotation>
   </xsd:element>

   <xsd:element name="GtmDeclaration" type="GtmDeclarationType" substitutionGroup="otm:GLogXMLTransaction">
      <xsd:annotation>
         <xsd:documentation>
            Represents a Global Trade Management Customs Declaration. Examples could be: import shipment, export shipment,
            inter-state shipment. The interface can be also used to pass the shipments information including charges and
            relevant information to an external system such as an ERP Inventory Receiving or Landed Cost Management application.
         </xsd:documentation>
      </xsd:annotation>
   </xsd:element>

   <xsd:element name="GtmDeclarationMessage" type="GtmDeclarationMessageType" substitutionGroup="otm:GLogXMLTransaction">
      <xsd:annotation>
         <xsd:documentation>
            Represents information related to messages exchanged between different parties involved in the process of Customs Declaration.
         </xsd:documentation>
      </xsd:annotation>
   </xsd:element>

   <xsd:element name="GtmBond" type="GtmBondType" substitutionGroup="otm:GLogXMLTransaction">
      <xsd:annotation>
         <xsd:documentation>
            Represents bond or guarantee related information.
         </xsd:documentation>
      </xsd:annotation>
   </xsd:element>

   <xsd:element name="ServiceRequest" type="ServiceRequestType" substitutionGroup="otm:GLogXMLTransaction">
      <xsd:annotation>
         <xsd:documentation>
            Request/Reply style interface to request the execution one of a number of services:-

            - Compliance Rule Screening
            - License Determination
            - Item Classification Screening
            - Restricted Party Screening
            - Sanctioned Territory Screening
         </xsd:documentation>
      </xsd:annotation>
   </xsd:element>

   <xsd:element name="ServiceResponse" type="ServiceResponseType" substitutionGroup="otm:GLogXMLTransaction">
      <xsd:annotation>
         <xsd:documentation>
            Request/Reply style interface reply to the ServiceRequest.
         </xsd:documentation>
      </xsd:annotation>
   </xsd:element>

   <xsd:complexType name="ServiceRequestType">
      <xsd:annotation>
         <xsd:documentation>
            Request/Reply style interface request structure.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="otm:OTMTransactionIn">
            <xsd:sequence>
               <xsd:element name="ServiceName" type="xsd:string">
                  <xsd:annotation>
                     <xsd:documentation>
                        Contains the name of the Service to be executed.
                        Following are the values accepted.
                        COMPLIANCE RULE SCREEN, LICENSE DETERMINATION,
                        CLASSIFY ITEM, EMBARGOED COUNTRY SCREEN, RESTRICTED PARTY SCREEN
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="RefId" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        The reference identification for the request from other source system
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Source" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        The source system sending the request
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Attribute" type="AttributeType" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element name="GtmServicePreferenceId" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        The service preference or configuration to be used for performing services
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:choice>
                  <xsd:element name="RestrictedParty" type="RestrictedPartyType">
                     <xsd:annotation>
                        <xsd:documentation>
                           The service input for Restricted Party Screening
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="SanctionedTerritory" type="SanctionedTerritoryType">
                     <xsd:annotation>
                        <xsd:documentation>
                           The service input for Sanctioned Territory Screening
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Classification" type="ClassificationType">
                     <xsd:annotation>
                        <xsd:documentation>
                           The service input for Item Classification Screening
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="ComplianceRule" type="ComplianceRuleType">
                     <xsd:annotation>
                        <xsd:documentation>
                           The service input for Compliance Rule Screening
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="LicenseDetermination" type="LicenseDeterminationType">
                     <xsd:annotation>
                        <xsd:documentation>
                           The service input for License Determination
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:choice>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="ServiceResponseType">
      <xsd:annotation>
         <xsd:documentation>
            Request/Reply style interface reply to the ServiceRequest.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="otm:OTMTransactionOut">
            <xsd:sequence>
               <xsd:element name="ServiceRequest" type="ServiceRequestType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Identifies the inbound service request.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>       
               <xsd:choice>
                  <xsd:element name="RestrictedPartyResponse" type="RestrictedPartyResponseType" minOccurs="0" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation>
                           The service response for Restricted Party Screening request.
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>                     
                  <xsd:element name="SanctionedTerritoryResponse" type="SanctionedTerritoryResponseType" minOccurs="0" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation>
                           The service response for Sanctioned Territory Screening request.
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>                        
                  <xsd:element name="ClassificationResponse" type="ClassificationResponseType" minOccurs="0" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation>
                           The service response for Item Classification Screening request.
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>                        
                  <xsd:element name="ComplianceRuleResponse" type="ComplianceRuleResponseType" minOccurs="0" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation>
                           The service response for Compliance Rule Screening request.
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>                  
                  <xsd:element name="LicenseDeterminationResponse" type="LicenseDeterminationResponseType" minOccurs="0" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation>
                           The service response for License Determination request.
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>  
               </xsd:choice>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="GtmContactType">
      <xsd:annotation>
         <xsd:documentation>
            The Trading Party and it's details.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="otm:OTMTransactionInOut">
            <xsd:sequence>
               <xsd:element name="Contact" type="otm:ContactType">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Details of the Contact.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>       
               <xsd:element name="IntSavedQuery" type="otm:IntSavedQueryType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Inbound) Integration Saved Query to identify the relevant party to be updated.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="TransactionCode" type="otm:TransactionCodeType" />
               <xsd:element name="ReplaceChildren" type="otm:ReplaceChildrenType" minOccurs="0" />
               <xsd:element name="LocationRef" type="otm:LocationRefType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Location Referenced by party.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>            
               <xsd:element name="ContactRegistration" type="ContactRegistrationType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Registration applicable for the party.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>            
               <xsd:element name="UserDefinedClassification" type="UserDefinedClassificationType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Outbound) User Defined Classification applicable for the party.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>            
               <xsd:element name="PartyScreeningResult" type="PartyScreeningResultType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Outbound) Denied Party matches for the Party.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>            
               <xsd:element name="PartyRefnum" type="otm:RefnumType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Reference Number applicable for the party.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>            
               <xsd:element name="PartyRemark" type="otm:RemarkType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Remark applicable for the party.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="GtmRegistrationType">
      <xsd:annotation>
         <xsd:documentation>
            GtmRegistration interface represents the registration taken from any governmental jurisdiction or regimes.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="otm:OTMTransactionInOut">
            <xsd:sequence>
               <xsd:element name="GtmRegistrationGid" type="otm:GLogXMLGidType">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) GTM Identifier for the Registration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="TransactionCode" type="otm:TransactionCodeType" minOccurs="0" />
               <xsd:element name="GtmJurisdictionGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Indicates Jurisdiction of Registration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="GtmRegistrationCategoryGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Indicates Category of Registration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="GtmRegistrationTypeGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Indicates Type of Registration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="GtmRegistrationCodeGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Indicates Registration Code of Registration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="RegisteredName" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Indicates Name of Registration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="RegistrationNumber" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Indicates Registration Number obtained from governmental agency.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="RegisteredAddress" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>(Both) Indicates Address of Registration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="EffectiveDate" type="otm:GLogDateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Indicates Effective Date of Registration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="ExpirationDate" type="otm:GLogDateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Indicates Expiration Date of Registration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="RequestedDate" type="otm:GLogDateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Indicates date when the Registration was requested.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="ApprovalDate" type="otm:GLogDateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Indicates date when the Registration was approved.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="RenewalDate" type="otm:GLogDateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Indicates date when the Registration was renewed.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="GtmProdClassTypeGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Product Classification Type applicable for the Registration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="ClassificationCode" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Product Classification Code applicable for the Registration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="Indicator" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>(Both) Indicator.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="UserDefIconInfo" type="otm:UserDefIconInfoType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) User Defined Icon information.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="IsActive" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>(Both) Indicates if the Registration is still active.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="IsTemplate" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Indicates if the Registration is a template.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="GtmRegistrationDate" type="GtmRegistrationDateType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Qualified Date of the Registration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="GtmRegRelatedItem" type="GtmRegRelatedItemType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Item for which Registration is applicable.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="GtmRegRelatedCommodity" type="GtmRegRelatedCommodityType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Commodity for which Registration is applicable.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="GtmRegRelatedUDCommodity" type="GtmRegRelatedUDCommodityType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) User Defined Commodity for which Registration is applicable.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="GtmRegRelatedLicense" type="GtmRegRelatedLicenseType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Licenses related to the Registration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="GtmRegRelatedRegistration" type="GtmRegRelatedRegistrationType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Other Registrations related to the Registration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Quantity" type="QuantityType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Quantity data associated with the Registration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="Currency" type="CurrencyType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Currency data associated with the Registration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="Status" type="otm:StatusType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>(Outbound) Status of the Registration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="Remark" type="otm:RemarkType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Registration Remark data.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="InvolvedParty" type="otm:InvolvedPartyType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Registration Involved Parties.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="Refnum" type="otm:RefnumType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>(Both) Reference Number of the Registration
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="RegimeGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Regime for which the Registration is applicable
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="GtmTransactionType">
      <xsd:annotation>
         <xsd:documentation>
            Trade Transaction and it's details. Trade Transaction includes Order Release or Shipment.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="otm:OTMTransactionInOut">
            <xsd:sequence>
               <xsd:element name="GtmTransactionGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Identifies the Trade Transaction.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="GtmTransactionLine" type="GtmTransactionLineType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Identifies the Line Item.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="IntSavedQuery" type="otm:IntSavedQueryType" minOccurs="0" />
               <xsd:element name="TransactionCode" type="otm:TransactionCodeType" minOccurs="0" />
               <xsd:element name="ReplaceChildren" type="otm:ReplaceChildrenType" minOccurs="0" />
               <xsd:element name="DataQueryTypeGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Identifies the whether it is based on Order Release or Shipment.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="ObjectGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Identifies the Order Release or Shipment ID.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:choice minOccurs="0">
                  <xsd:sequence>
                     <xsd:element name="Release" type="otm:ReleaseType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>
                              (Both) Order Release data, optional if refers to existing object and no update is required.
                           </xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
                  <xsd:sequence>
                     <xsd:element name="Shipment" type="otm:ShipmentType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>
                              (Both) Shipment data, optional if refers to existing object and no update is required.
                           </xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:choice>         
               <xsd:element name="IncoTermGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Identifies the IncoTerm for the Trade Transaction.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="IncoTermLocation" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Identifies the IncoTerm Location for the Trade Transaction.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="IsHazardousCargo" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Identifies if the Trade Transaction cargo is hazardous.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="GtmTransactionType" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) This will always be TRANSACTION.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>            
               <xsd:element name="GtmTransactionTypeGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Identifies the type of trade transaction For eg, SALES ORDER, SHIPMENT, DELIVERY.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="IsShipmentCreated" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Outbound) Deprecated (6.4.2) No longer used.
                        Any value will be ignored and the element will be removed in a future release.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="Remark" type="otm:RemarkType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Remark data associated with the business Transaction.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="InvolvedParty" type="GtmInvolvedPartyType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>(Both) Details of the Party involved associated with the business Transaction.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="TransactionInvLocation" type="TransactionInvLocationType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Details of the Location involved associated with the business Transaction.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="Refnum" type="otm:RefnumType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Reference Number data associated with the business Transaction.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="Status" type="otm:StatusType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Outbound) Business Status of the Trade Transaction.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="TransDate" type="TransDateType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Dates on which the business transaction was handled.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="UserDefinedClassification" type="UserDefinedClassificationType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) User Defined Classification associated to the business Transaction.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="CarrierCode" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Carrier Code of the business Transaction.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="TransactionQuantity" type="QuantityType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Quantity data associated with business Transaction.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="TransactionCurrency" type="CurrencyType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Currency data associated with business Transaction.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="TransactionPolicy" type="TransactionPolicyType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Policy (list of services) to be performed on the Trade Transaction.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="TransactionRequiredDocument" type="TransactionRequiredDocumentType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Outbound) Document required for the business transaction.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="PortInfo" type="PortInfoType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Details of the Port through which the goods were handled.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="FlexFieldStrings" type="otm:FlexFieldStringType" minOccurs="0" />
               <xsd:element name="FlexFieldNumbers" type="otm:FlexFieldNumberType" minOccurs="0" />
               <xsd:element name="FlexFieldDates" type="otm:FlexFieldDateType" minOccurs="0" />
               <xsd:element name="BorderTransportModeGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Transport details for movement of goods across the border.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="BorderConveyanceName" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Conveyance used for movement of goods across the border.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="BorderConveyanceFlag" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Conveyance Flag for for movement of goods across the border.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="TariffPreferenceType" type="TariffPreferenceTypeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Tariff Preference Type under which the goods are imported or exported.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Conveyance" type="ConveyanceType" minOccurs="0" maxOccurs="unbounded" >
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Conveyances of the Transaction.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="GtmTransactionLineType">
      <xsd:annotation>
         <xsd:documentation>
            Trade transaction information for an line item.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="otm:OTMTransactionInOut">
            <xsd:sequence>
               <xsd:element name="GtmTransactionLineGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Identifies the Trade Transaction Line in GTM.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="IntSavedQuery" type="otm:IntSavedQueryType" minOccurs="0" />
               <xsd:element name="TransactionCode" type="otm:TransactionCodeType" minOccurs="0" />
               <xsd:element name="GtmTransactionGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Identifies the Trade Transaction.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="ItemGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Identifies the Item present on the line item.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="LineNumber" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Outbound) Identifies the Order Release or Ship Unit Line.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="LineObjectGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Identifies the Order Release or Ship Unit Line ID.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="ParentTransactionLineGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Identifies the parent lines for the Trade Transaction Line.
                        For the transaction to be created via integration, parent transaction line should 
                        be created before the child lines and in_sequence should be marked as yes.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="TransLineType" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        This is always TRANSACTION_LINE.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="CountryOfOrigin" type="otm:CountryCodeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Identifies the country where the goods originate from.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="ManufacturingCountry" type="otm:CountryCodeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Identifies the country where the goods are manufactured.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="IncoTermGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) IncoTerm associated with the line item (Sales order or fullfilment line or shipment line).
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="IncoTermLocation" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) IncoTerm Location associated with the line item (Sales order or fullfilment line or shipment line).
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="Remark" type="otm:RemarkType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>(Both) Remark data associated with the line item.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="InvolvedParty" type="GtmInvolvedPartyType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Detail of the Party involved associated with the line item.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="Refnum" type="otm:RefnumType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Reference Number associated with the line item.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="ProductClassification" type="ProductClassificationType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Product Classification associated with the line item.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="UserDefinedClassification" type="UserDefinedClassificationType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) User Defined Classification associated with the line item.             </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="ControlScreeningResult" type="ControlScreeningResultType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Outbound) License Controls applicable for the line item.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="ReportingResult" type="ReportingResultType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Outbound) Reporting Controls applicable for the Trade Transaction Line
                        indicating whether reporting needs to be done for any regime.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="RestrictedPartyScreeningResult" type="RestrictedPartyScreeningResultType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Outbound) Indicates the Pass or Fail status of the Contact during Restricted Party Screening.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="Quantity" type="QuantityType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Quantity data applicable for the line item.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="ReportingQuantity" type="ReportingQuantityType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Quantity to be reported in the appropriate UOM for the line item.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="Currency" type="CurrencyType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Currency data applicable for the line item.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="Percentage" type="PercentageType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Outbound) Percentage share of the current Trade Transaction Line in the Bill of Materials Item Structure.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="Status" type="otm:StatusType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Outbound) Business Status of the line item.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="GtmTransLineLicense" type="GtmTransLineLicenseType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Outbound) License assigned to the line item.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="GtmTransactionLineRequiredDocument" type="GtmTransactionLineRequiredDocumentType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Outbound) Document required for the movement or compliance of goods.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="GtmTransactionLineRequiredText" type="RequiredTextType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Outbound) Text Template required for the movement or compliance of goods.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="CalculatedValueResult" type="CalculatedValueResultType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        Deprecated (6.4.3): This element won't be populated from 6.4 onwards and will be removed in a future release.
                        (Outbound) Base amount applicable for the Trade Transaction Line.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="SanctionControlScreeningResult" type="SanctionControlScreeningResultType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Outbound) Sanction embargo screening result of the Trade Transaction Line.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="FlexFieldStrings" type="otm:FlexFieldStringType" minOccurs="0" />
               <xsd:element name="FlexFieldNumbers" type="otm:FlexFieldNumberType" minOccurs="0" />
               <xsd:element name="FlexFieldDates" type="otm:FlexFieldDateType" minOccurs="0" />
               <xsd:element name="GtmTrItemStructureGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Outbound) The (Bill of Materials) structure assigned to the Trade Transaction Line
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="LineDescription" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Description for the line item.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>         
               <xsd:element name="TariffPreferenceType" type="TariffPreferenceTypeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Tariff Preference Type under which the goods are imported or exported.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="TransLineDate" type="TransDateType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Dates when the transaction line was handled/created or shipment line was imported/exported etc.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="OtherControlScreeningResult" type="ControlScreeningResultGenericType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Other controls for the transaction line.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="GtmStructureType">
      <xsd:annotation>
         <xsd:documentation>
            Represents Trade Item Structure (bill of material) to identify
            components that make up a single sellable product or item.

            Deprecated (6.4.3): TradeDirection. Functionality removed. Value will be ignored and field will be removed in a future release.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="otm:OTMTransactionIn">
            <xsd:sequence>
               <xsd:element name="GtmStructureGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Identifier for the bill of material.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="TransactionCode" type="otm:TransactionCodeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Indicates the purpose of integration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="GtmStructureName" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Name of the bill of material.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Description" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Description for the bill of material.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="IsActive" type="xsd:string">
                  <xsd:annotation>
                     <xsd:documentation>(Both) Indicates whether the bill of material is active.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="EffectiveDate" type="otm:GLogDateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Date from which the bill of material will be valid.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ExpirationDate" type="otm:GLogDateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Date till which the bill of material will be valid.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ItemGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Item applicable for the bill of material.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="TradeDirection" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>(Both) Trade Direction applicable for the bill of material.
                        Deprecated (6.4.3): Functionality removed. Value will be ignored and field will be removed in a future release.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="GtmProdClassTypeGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) The product classification type applicable for the bill of material is applicable.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="DefaultCountryOfOriginGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) The country which can be considered as default country of origin for the bill of material.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="IntSavedQuery" type="otm:IntSavedQueryType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Inbound) The integration saved query identifier to identify the relevant bill of material to be updated.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="GtmStructureComponent" type="GtmStructureComponentType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Represent the components applicable for current bill of material.
                     </xsd:documentation>
                  </xsd:annotation>     
               </xsd:element>
               <xsd:element name="Remark" type="otm:RemarkType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Remark data of the bill of material.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="InvolvedParty" type="otm:InvolvedPartyType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Involved party of the bill of material.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Refnum" type="otm:RefnumType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Reference number of the bill of material.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="GtmProdClassCodeGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        The identifier for the product classification code applicable to the bill of material.
                        Deprecated (6.4.2): Use ClassificationCode instead of this element
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="UserDefinedClassification" type="UserDefinedClassificationType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) The user defined classification applicable for the bill of material.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ClassificationCode" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) The classification code applicable for the bill of material.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="GtmDeclarationType">
      <xsd:annotation>
         <xsd:documentation>
            Global Trade Management Declaration and it's details.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="otm:OTMTransactionInOut">
            <xsd:sequence>
               <xsd:element name="GtmDeclarationGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Identifies the declaration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="GtmDeclarationLine" type="GtmDeclarationLineType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) line items to be declared.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="IntSavedQuery" type="otm:IntSavedQueryType" minOccurs="0" />
               <xsd:element name="TransactionCode" type="otm:TransactionCodeType" minOccurs="0" />
               <xsd:element name="ReplaceChildren" type="otm:ReplaceChildrenType" minOccurs="0" />
               <xsd:element name="IncoTermGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Identifies the Inco term applicable for the declaration to be filed.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="IncoTermLocation" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Identifies the Inco term location applicable for the declaration to be filed.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="IsHazardousCargo" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Denotes whether there is any hazardous goods on the declaration to be filed.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="GtmTransactionType" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>(Both) Will be always SHIPMENT.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>                           
               <xsd:element name="Remark" type="otm:RemarkType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Remark data of the declaration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="InvolvedParty" type="GtmInvolvedPartyType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Details of the party involved in the declaration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="TransactionInvLocation" type="TransactionInvLocationType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Details of the location involved in the declaration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>                  
               <xsd:element name="Refnum" type="otm:RefnumType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Reference number associated to the declaration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Status" type="otm:StatusType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Outbound) business Status of the declaration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="UserDefinedClassification" type="UserDefinedClassificationType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) User Defined Classification associated to the declaration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="TransDate" type="TransDateType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Dates when the declaration was handled/created or shipments were imported/exported etc..
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>                
               <xsd:element name="CarrierCode" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Carrier code associated to the declaration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="TransactionQuantity" type="QuantityType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>(Both) Quantity data associated to the declaration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="TransactionCurrency" type="CurrencyType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Values data associated to the declaration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="TransactionPolicy" type="TransactionPolicyType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        Policy (list of services) to be performed on the declaration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="TransactionRequiredDocument" type="TransactionRequiredDocumentType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Document required for the business transaction to be carried on.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="PortInfo" type="PortInfoType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Port through which the goods are handled.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="FlexFieldStrings" type="otm:FlexFieldStringType" minOccurs="0" />
               <xsd:element name="FlexFieldNumbers" type="otm:FlexFieldNumberType" minOccurs="0" />
               <xsd:element name="FlexFieldDates" type="otm:FlexFieldDateType" minOccurs="0" />
               <xsd:element name="BorderTransportModeGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Transport mode through which the goods movement is carried on across Border.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="BorderConveyanceName" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Conveyance name for the goods movement is carried on across Border.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="BorderConveyanceFlag" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Conveyance flag for the goods movement is carried on across Border.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="InlandTransportModeGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Transport mode through which the goods movement is carried on across inland.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="InlandConveyanceName" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Conveyance name for the goods movement is carried on across inland.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="InlandConveyanceFlag" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Both) Conveyance flag for the goods movement is carried on across inland.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="DepartureTransportModeGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Transport mode through which the goods movement is carried on while departure.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="DepartureConveyanceName" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Conveyance name for the goods movement is carried on while departure.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="DepartureConveyanceFlag" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Conveyance flag for the goods movement is carried on while departure.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="DeclarationType" type="DeclarationTypeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Type of declaration. For eg, US EXPORT, EU IMPORT.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="DeclarationSubType" type="DeclarationSubTypeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) declaration Sub Type under which the goods is being declared.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="TariffPreferenceType" type="TariffPreferenceTypeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Tariff preference type under which the goods are imported or exported.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Procedure" type="ProcedureType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Details of procedure under which the goods is being declared.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="PriorProcedure" type="ProcedureType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Details of previous procedure under which the goods was earlier declared.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ProcedureDetail" type="ProcedureDetailType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Details of procedure under which the goods is being declared.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="GtmBond" type="GtmBondType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Bond utilized for the declaration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Document" type="otm:DocumentType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Documents related to the declaration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ExchangeRateInfo" type="otm:ExchangeRateInfoType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Exchange rate used for all the value calculations.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="CurrencyGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Currency of the regime for which the declaration is performed
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="RequestStatusGroupGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Indicates whether the declaration is being filed.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="RequestStatusCodeGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Indicates whether the request should be sent for new declaration or an update to existing declaration or cancellation of earlier declaration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ResponseStatusGroupGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Identifies the stage of the declaration whether it is still being verified or release or it has exit the country.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ResponseStatusCodeGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Identifies whether the declaration is accepted, rejected, released.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ValuationMethod" type="ValuationMethodType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Valuation method applicable to declaration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Conveyance" type="ConveyanceType" minOccurs="0" maxOccurs="unbounded" >
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Conveyances of the Declaration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="GtmDeclarationMessageType">
      <xsd:annotation>
         <xsd:documentation>This Element maintains the information related to messages exchanged between different parties involved in the process of Declaration.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="otm:OTMTransactionInOut">
            <xsd:sequence>
               <xsd:element name="DeclarationMessageGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Declaration Message GID is a unique key for declaration message across all domains.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="TransactionCode" type="otm:TransactionCodeType" minOccurs="0" />
               <xsd:element name="ReplaceChildren" type="otm:ReplaceChildrenType" minOccurs="0" />
               <xsd:element name="MessageDate" type="otm:GLogDateTimeType">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Date on which message is received or sent.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Description" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Description for the message.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="SenderID" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) ID of the sender of the message.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ReceiverID" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) ID of the receiver of the message.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="CountryCode" type="otm:CountryCodeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Country code of the filling country.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="StatusGroup" type="otm:StatusGroupType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) This indicates two things.
                        For Oubound message, it indicates whether the declaration is being filed.
                        For Inbound message, it indicates whether the declaration is still being verified or released or it has exit the country.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="StatusGroupExternalCode" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) This indicates the code that the external system understands for the status group.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="StatusCode" type="otm:StatusCodeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Message Function Code (original, additional, accepted, cancelled etc.).
                        For Oubound message, it indicates whether the request should be sent for new declaration or an update to existing declaration or cancellation of earlier declaration.
                        For Inbound message, it indicates whether the declaration is accepted, rejected, released.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="StatusCodeExternalCode" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) This indicates the code that the external system understands for the status group.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>  
               <xsd:element name="ReasonCode" type="otm:ReasonCodeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Status Reason code GID, eg. Reason Code or Error.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="TestIndicator" type="xsd:string">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Indicates whether the message sent for filing is a test message or a production message.
                        Government will take necessary actions for the production messages.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Action" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) The declaration action performed.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="DeclarationGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Global identifier for the declaration.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="GtmDeclaration" type="GtmDeclarationType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Identifies the type of the declaration (BE Import, US Export, etc
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="FlexFieldStrings" type="otm:FlexFieldStringType" minOccurs="0" />
               <xsd:element name="FlexFieldNumbers" type="otm:FlexFieldNumberType" minOccurs="0" />
               <xsd:element name="FlexFieldDates" type="otm:FlexFieldDateType" minOccurs="0" />
               <xsd:element name="ResponseInfo" type="ResponseInfoType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Inbound) Identifies the type of response information.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Remark" type="otm:RemarkType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Remark associated to the declaration message.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Refnum" type="otm:RefnumType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Reference Number associated to the declaration message.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="GtmBondType">
      <xsd:annotation>
         <xsd:documentation>
            This element maintains bond or guarantee related information.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="otm:OTMTransactionInOut">
            <xsd:sequence>
               <xsd:element name="BondGid" type="otm:GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Global identifier for the bond.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Description" type="xsd:string" minOccurs="0" >
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Description of the bond or guarantee
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="IntSavedQuery" type="otm:IntSavedQueryType" minOccurs="0" />
               <xsd:element name="TransactionCode" type="otm:TransactionCodeType" minOccurs="0" />
               <xsd:element name="ReplaceChildren" type="otm:ReplaceChildrenType" minOccurs="0" />
               <xsd:element name="BondNumber" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Represents the bond number.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="PriorBondNumber" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Represents the bond number associated with a prior bond.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>   
               <xsd:element name="BondType1" type="UserDefinedClassificationType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Identifies the type of bond.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="BondType2" type="UserDefinedClassificationType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Identifies additional information about the type of bond.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="IsSupplemental" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Identifies if the bond is a supplemental bond.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="IsExtension" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Identifies if the bond is an extension of a bond.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="EffectiveDate" type="otm:GLogDateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Date from when the bond will be effective.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ExpirationDate" type="otm:GLogDateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Date till when the bond will be effective.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ExecutionDate" type="otm:GLogDateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) The date on which the bond is executed.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="LiabilityLimit" type="otm:GLogXMLFinancialAmountType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Identifies the liability limit on the bond.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Remark" type="otm:RemarkType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Remark associated to the bond.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Refnum" type="otm:RefnumType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Reference number associated to the bond.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="InvolvedParty" type="GtmInvolvedPartyType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Identifies the involved parties associated with the bond.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="PortInfo" type="PortInfoType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Identifies the ports associated with the bond.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="ResponseInfoType">
      <xsd:annotation>
         <xsd:documentation>
            This element maintains additional information related to the messages exchanged between the different parties
            involved in the process of Declaration. 
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Sequence" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Identifies the sequence in which we get back response.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Type" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Identifies the type of the response.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Code" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Identifies the code for the error or warning kind of response.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Severity" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Identifies the importance of the response message.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AddtionalText" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Additional information on the response.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="DeclarationTypeType">
      <xsd:annotation>
         <xsd:documentation>
            This element identifies the type of declaration. For example, BE IMPORT, US EXPORT, EU TRANSIT ARRIVAL.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="DeclarationTypeGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Global identifier for the type of the declaration (BE Import, US Export, etc)
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Code" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Identifies the code associated with the declaration type
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RegimeId" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Identifies the regime related to the declaration type
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Name" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Contains the name of the declaration type
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Description" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Contains the description of the declaration type
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="DeclarationSubTypeType">
      <xsd:annotation>
         <xsd:documentation>
            The sub type of declaration applicable for custom filing. For example, MIXED CONSIGNMENTS COVERED BY ARTICLE 351,
            SUPPLEMENTARY DECLARATION - TYPE B
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="DeclarationSubTypeGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Global identifier for the sub type of the declaration
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Code" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Identifies the code associated with the declaration sub type
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Name" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Contains the name associated with the declaration sub type
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Description" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Contains the description associated with the declaration sub type
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ValuationMethodType">
      <xsd:annotation>
         <xsd:documentation>
            This element is used for identification of different methods of custom valuation.
            For example, TRANSACTION VALUE OF IDENTICAL GOODS, DEDUCTIVE METHOD
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ValuationMethodGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Global identifier for the valuation method
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Code" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Identifies the code associated with the valuation method
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Name" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Contains the name associated with the valuation method
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Description" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Contains the description associated with the valuation method
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="TariffPreferenceTypeType">
      <xsd:annotation>
         <xsd:documentation>
            (Both) This element represents the trade preference to be used for duty calculation.
            For example, GSP TARIFF QUOTAS, PREFERENTIAL TARIFF QUOTAS.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="TariffPreferenceTypeGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Global identifier for the tariff treatment of goods
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Code" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Identifies the code associated with the tariff preference
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Name" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Contains the name associated with the tariff preference
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Description" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Contains the description associated with the tariff preference
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ProcedureType">
      <xsd:annotation>
         <xsd:documentation>
            (Both) This element maintains the Customs Procedure associated with Customs Filing.
            For example, NO PRIOR PROCEDURE, PERMANENT EXPORT.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ProcedureGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Global identifier for the procedure
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Code" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Identifies the code associated with the procedure
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Name" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Contains the name associated with the procedure
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Description" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Contains the description associated with the procedure
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ProcedureCategory" type="ProcedureCategoryType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Identifies the procedure type associated with the procedure
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ProcedureCategoryType">
      <xsd:annotation>
         <xsd:documentation>
            (Both) The type of custom procedure applicable for the filing. For example, EXPORT / DISPATCH, TRANSIT, OUTWARD PROCESSING.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ProcedureCategoryGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Global identifier for the procedure type
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Code" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Identifies the code associated with the procedure type
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Name" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Contains the name associated with the procedure type
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Description" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Contains the description associated with the procedure type
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ProcedureDetailType">
      <xsd:annotation>
         <xsd:documentation>
            (Both) This element maintains Customs Procedure Details associated with Customs Filing.
            For example, DISASTER RELIEF MATERIAL, PUBLICITY MATERIAL.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ProcedureDetailGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Global identifier for the procedure detail
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Code" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Identifies the code associated with the procedure detail
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Name" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Contains the name associated with the procedure detail
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Description" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Contains the description associated with the procedure detail
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="UserDefinedClassificationType">
      <xsd:annotation>
         <xsd:documentation>
            (Both) General User Defined Code pertaining to business objects, including but not limited to: end use code,
            end user code, product type code, transaction code, etc.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="UserDefinedCategoryGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Global identifier for the user-defined category.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="UserDefinedType" type="UserDefinedTypeType">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Contains the user-defined type related to the category.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="UserDefinedCode" type="UserDefinedCodeType">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Contains the user-defined code related to the type.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="UserDefinedTypeType">
      <xsd:annotation>
         <xsd:documentation>
            (Both) General User Defined Type pertaining to business objects
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="TypeGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) GTM identifier for user defined type.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Code" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) External code for the user defined type.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="UserDefinedCodeType">
      <xsd:annotation>
         <xsd:documentation>
            General User Defined Type pertaining to business objects
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="GtmCode" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) GTM identifier for user defined code.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Code" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) External code for the user defined code.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="RestrictedPartyType">
      <xsd:annotation>
         <xsd:documentation>
            Restricted Party is RPLS screening request of party via ServiceRequest integration interface.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="FirstName" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  First name of the party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="LastName" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Last name of the party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CompanyName" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Company name of the party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="MatchEngine" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Match Engine to be used for screening.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AddressLine1" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Address Line1 of the party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AddressLine2" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Address Line2 of the party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AddressLine3" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Address Line3 of the party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="City" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  City of the party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Province" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Province of the party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Country" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Country of the party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PostalCode" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Postal code of the party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="RestrictedPartyResponseType">
      <xsd:annotation>
         <xsd:documentation>
            Restricted Party Response for the screening triggered via ServiceRequest integration interface.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="GtmDeniedPartyGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Identifier of the restricted party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="FirstName" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) First Name of the restricted party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="LastName" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Last Name of the restricted party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CompanyName" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Company Name of the restricted party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="VesselName" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Vessel Name of the restricted party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AddressLine1" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Address line 1 of the restricted party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AddressLine2" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Address line 2 of the restricted party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AddressLine3" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Address line 3 of the restricted party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="City" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) City of the restricted party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Province" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Province of the restricted party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Country" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Country of the restricted party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PostalCode" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Postal Code of the restricted party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="EffectiveDate" type="otm:GLogDateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Dates from which restriction on the party applies.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ExpirationDate" type="otm:GLogDateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Dates till which restriction on the party applies.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DateOfBirth" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Date of birth of the restricted party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="FederalRegulationUrl" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) FederalRegulationUrl. DataType: A20
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PlaceOfBirth" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Place of birth of the restricted party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PassportNumber" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Passport number of the restricted party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PassportIssuingCountry" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Country where the Passport was issued to the restricted party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PassportIssuingDate" type="otm:GLogDateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Date on which the Passport was issued to the restricted party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DriverLicenseNumber" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) License number of the restricted party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DriverLicenseAuthority" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Authority which has issued the License.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Citizenship" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Citizenship of the restricted party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Nationality" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Nationality of the restricted party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Ssn" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Ssn of the restricted party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Nit" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Nit of the restricted party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Cedula" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Cedula of the restricted party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DenialCode" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Denial Code of the restricted party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="EntryDate" type="otm:GLogDateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Date when the details of the restricted party was entered by agencies.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="EntryId" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Entry Identifier of the restricted party as entered by agencies.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AgencyCode" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Agency which has classified this party as restricted party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RulingVolume" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Ruling Volume detail of the restricted party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RulingPageNumber" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Ruling Page Number detail of the restricted party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="FederalRegulationDate" type="otm:GLogDateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Federal Regulation Date detail of the restricted party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Notes" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) More information of the restricted party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="MatchPer" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Percentage of matches the restricted party had with the input party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="SanctionedTerritoryType">
      <xsd:annotation>
         <xsd:documentation>
            (Inbound) SanctionedTerritory is the embargo screening request triggered via ServiceRequest integration interface.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="RuleGroupGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Inbound) Rule group to be used for screening</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="LocationInfo" type="LocationInfoType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Inbound) Location from or to which the goods are exported or imported from.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TransactionDate" type="otm:GLogDateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Inbound) Date of transaction.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="InvolvedPartyRefnumDetail" type="InvolvedPartyRefnumDetailType" minOccurs="0" maxOccurs="unbounded" >
            <xsd:annotation>
               <xsd:documentation>(Inbound) Reference numbers of the parties involved.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="InvolvedPartyRemarkDetail" type="InvolvedPartyRemarkDetailType" minOccurs="0" maxOccurs="unbounded" >
            <xsd:annotation>
               <xsd:documentation>(Inbound) Remarks of the parties involved.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="SanctionedTerritoryResponseType">
      <xsd:annotation>
         <xsd:documentation>(Outbound) SanctionedTerritoryResponse is the embargo screening response sent for the inbound SanctionedTerritory screening request.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="RuleControl" type="RuleControlType">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Sanction controls applicable for input sanction screening request.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   
   <xsd:complexType name="ClassificationType">
      <xsd:annotation>
         <xsd:documentation>(Inbound) Classification is the lookup classification request for the item description sent via the ServiceRequest interface.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ClassificationCode" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Inbound) It supports wild card lookup to search for classification in any particulat classification chapter.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Description" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Inbound) Description of the goods based on which the classification is required.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CountryName" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Inbound) Country whose locale should be used for the classification.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Language" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Inbound) Language in which the the classification should be performed.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ProdClassType" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Inbound) product classification type for which lookup should be performed.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ClassificationCategory" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Inbound) Classification category in which the classification should be performed.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ClassificationResponseType">
      <xsd:annotation>
         <xsd:documentation>(Outbound) Classification is the lookup classification response for the synchronous ServiceRequest interface.
            This interface is for outbound only.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ClassCode" type="ClassCodeType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Outbound) The classification code matching to the inbound classification request.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ClassCodeType">
      <xsd:annotation>
         <xsd:documentation>(Outbound) Classification is the lookup classification response for the synchronous ServiceRequest interface.
            This interface is for outbound only.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ClassificationCode" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Outbound) The classification code matching to the inbound classification request.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Description" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Description of the classification code matched.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ClassCode" type="ClassCodeType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Outbound) The child classification codes matching to the inbound classification request.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ComplianceRuleType">
      <xsd:annotation>
         <xsd:documentation>(Inbound) ComplianceRule is request for the compliance screening. This interface is for inbound only.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="RuleGroupGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>Identifies the compliance rule group based on which the compliance screening will be performed.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TransactionDate" type="otm:GLogDateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>The date of the business transaction.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CommodityGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Commodity id for the goods transacted.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="UserDefinedCommodityGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>User defined Commodity id for the goods transacted.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ProductInfo" type="ProductInfoType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>Product classification information for the goods transacted.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="InvolvedPartyDetail" type="InvolvedPartyDetailType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>Details of the party involved in business transaction.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CategoryTypeCode" type="CategoryTypeCodeType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>User defined code details for the goods transacted.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>     
         <xsd:element name="Condition" type="ConditionType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>This field is not used any more.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TerritoryInfo" type="TerritoryInfoType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>Identifies the country details from or to which the business transaction is being performed.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RegionInfo" type="RegionInfoType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>Identifies the region details from or to which the business transaction is being performed.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="InvolvedPartyRefnumDetail" type="InvolvedPartyRefnumDetailType" minOccurs="0" maxOccurs="unbounded" >
            <xsd:annotation>
               <xsd:documentation>Identified the reference number for the party involved.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="InvolvedPartyRemarkDetail" type="InvolvedPartyRemarkDetailType" minOccurs="0" maxOccurs="unbounded" >
            <xsd:annotation>
               <xsd:documentation>Identified the remark for the party involved.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ComplianceRuleResponseType">
      <xsd:annotation>
         <xsd:documentation>(Outbound) ComplianceRuleResponse is response for the compliance screening triggered via ServiceRequest integration interface.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="RuleControl" type="RuleControlType">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Controls applicable for the input service request.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="LicenseDeterminationType">
      <xsd:annotation>
         <xsd:documentation>(Inbound) LicenseDetermination is request for the license screening.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ItemGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Item Id of the input line.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CommodityGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Commodity Id of the input line.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="UserDefinedCommodityGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>User defined commodity of the input line.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ControlTypeCode" type="ControlTypeCodeType" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>Identifies the controls for which the license needs to be determined.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PortInfo" type="PortInfoType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>Port where the goods were handled.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="InvolvedPartyDetail" type="InvolvedPartyDetailType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>Details of the party involved.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RegionCountryInfo" type="RegionCountryInfoType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>Region or country where the business transaction is occuring.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CategoryTypeCode" type="CategoryTypeCodeType" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>User defined type code applicable for the business goods.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TransDateInfo" type="TransDateType" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>Date on which the business transaction is handled.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ProductClassification" type="ProductClassificationType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>Product classification under which the goods is classified.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ItemRemarks" type="GtmRemarksType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>Remarks of the item.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TransLineRemarks" type="GtmRemarksType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>Remarks of the transaction line (order line or shipment line).</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="LicenseDeterminationResponseType">
      <xsd:annotation>
         <xsd:documentation>(Outbound) LicenseDeterminationResponse is response for the license screening triggered via ServiceRequest integration interface.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="LicenseGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>License Id matching to the input data.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="LicenseNumber" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>License number matching to the input data.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="LicenseLineGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>License Line id matching to the input data.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ControlTypeGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>License Control type applicable to the goods to be transacted.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ControlCode" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>License Control code applicable to the goods to be transacted.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ExpirationDate" type="otm:GLogDateTimeType">
            <xsd:annotation>
               <xsd:documentation>Date on which the license will expire.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="QuantityTypeGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>Total quantity permitted on the license.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="BalanceQuantityStr" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Balance quantity available on the license for utilization.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CurrencyTypeGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>Total value permitted on the license.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="BalanceValueStr" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Balance value available on the license for utilization.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="RuleControlType">
      <xsd:annotation>
         <xsd:documentation>(Outbound) RuleControl represents the controls applicable for the input data based on compliance screening  
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="RuleId" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Compliance rule identifier.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ControlType" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>License, sanction or reporting control types applicable.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ControlCode" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>License, sanction or reporting controls applicable.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ControlCategory" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Type of compliance rule. For example, SANCTION, LICENSE</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Precedence" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Precedence of the controls applicable by matching compliance rule.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DocumentDefGid" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Document required for the business transaction.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RegimeId" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Regime under which the controls are applicable.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="InvolvedPartyQualGid" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Parties involved for the controls.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>       
         <xsd:element name="ScreeningStatus" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Whether the input business transaction is pass or failed in compliance check.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ExceptionControlTypeGid" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>License or reporting exception control types applicable.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ExceptionControlCode" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>License or reporting exception controls applicable.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PassFail" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Returns FAIL when there are license controls obtained by making compliance check.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Text" type="otm:TextType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>Text required for the business transaction.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ContactRegistrationType">
      <xsd:annotation>
         <xsd:documentation>(Both) Represents the registrations related to the contact.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="TransactionCode" type="otm:TransactionCodeType" minOccurs="0" />
         <xsd:choice>
            <xsd:element name="GtmRegistrationGid" type="otm:GLogXMLGidType">
               <xsd:annotation>
                  <xsd:documentation>(Both) Identifier for registration.</xsd:documentation>
               </xsd:annotation>
            </xsd:element>
            <xsd:element name="GtmRegistration" type="GtmRegistrationType">
               <xsd:annotation>
                  <xsd:documentation>(Both) Details of registration.</xsd:documentation>
               </xsd:annotation>
            </xsd:element>
         </xsd:choice>
         <xsd:element name="InvolvedPartyQualifierGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Party qualifier.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ComMethodGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Preferred communication method for the party.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="GtmRegistrationDateType">
      <xsd:annotation>
         <xsd:documentation>(Both) This tag will hold dates other then EFFECTIVE_DATE ,EXPIRATION_DATE ,REQUESTED_DATE
            ,APPROVAL_DATE ,RENEWAL_DATE.  
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="GtmRegistrationGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Identifier for registration.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DateQualifierGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Date qualifier.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RegistrationDate" type="otm:GLogDateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Date on which the registration was handled.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="AttributeType">
      <xsd:annotation>
         <xsd:documentation>(Both) Specifies unqualified attribute name and values. Not used any more.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="AttrName" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Both) Specifies attribute name.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Value" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Both) Specifies attribute value.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ConditionType">
      <xsd:annotation>
         <xsd:documentation>(Inbound) This is not used any more.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ConditionName" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>It was used earlier to pass type of data </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ConditionValue" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>It was used earlier to pass value for data.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ProductInfoType">
      <xsd:annotation>
         <xsd:documentation>(Both) This represent the product classification type and code.   
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ProductType" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Product classfication type.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ProductCode" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Both) Product classfication code.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="InvolvedPartyDetailType">
      <xsd:annotation>
         <xsd:documentation>(Both) This tag will hold details of party involved.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="InvolvedPartyQualGid" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Both) Party qualifier.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ContactGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Party involved.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="CategoryTypeCodeType">
      <xsd:annotation>
         <xsd:documentation>(Both) This represents user defined type and code for a business object
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="GtmCategory" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) User defined category.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="GtmType" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Both) User defined type applicable for the business transaction.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="GtmCode" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Both) User defined code applicable for the business transaction.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="LocationInfoType">
      <xsd:annotation>
         <xsd:documentation>(Both) This tag will hold Location Info.  
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="InvolvedCountryQualifier" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Qualifier for the location invovled in the business transaction.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CountryCode" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Country of the location invovled in the business transaction.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="GtmRegRelatedItemType">
      <xsd:annotation>
         <xsd:documentation>(Both) Items Related to Registration
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="GtmRegistrationGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Identifier for registration.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ItemGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Item related to the registration.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="GtmRegRelatedCommodityType">
      <xsd:annotation>
         <xsd:documentation>(Both) Commodities Related to Registration
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="GtmRegistrationGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Identifier for registration.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CommodityGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Commodity related to the registration.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="GtmRegRelatedUDCommodityType">
      <xsd:annotation>
         <xsd:documentation>(Both) User Defined Commodities Related to Registration
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="GtmRegistrationGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Identifier for registration.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="UserDefinedCommodityGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) User defined commodity related to the registration.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="GtmRegRelatedLicenseType">
      <xsd:annotation>
         <xsd:documentation>(Both) Licenses Related to Registration
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="GtmRegistrationGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Identifier for registration.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="LicenseGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) License related to the registration.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="GtmRegRelatedRegistrationType">
      <xsd:annotation>
         <xsd:documentation>(Both) Registrations related to another Registration.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="GtmRegistrationGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Identifier for registration.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="GtmRelatedRegistrationGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Identifier for the related registration.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="QuantityType">
      <xsd:annotation>
         <xsd:documentation>(Both) Quantity element with a numeric value, UOM code and quantity type.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="SequenceNumber" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Sequence of the quantities present on the business object.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="QuantityTypeGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) The quantity qualifier on the business object.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="QuantityValue" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Both) The quantity on the business object.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="UOM" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Both) Unit of measure for the quantity on the business transaction.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ReportingQuantityType">
      <xsd:annotation>
         <xsd:documentation>(Both) Reporting Quantity represents the quantity in external unit of measure as needed by different regimes.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="SequenceNumber" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Both) Sequence of the reporting quantities present on the line item.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="GtmProdClassTypeGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Product classification based on which the reporting quantity is calculated or to be reported.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="QuantityTypeGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Qualifier for the quantity on the business transaction.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="QuantityValue" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Both) The quantity on the business transaction registration.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AttrName" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Both) The type of reporting quantity.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="UOM" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Both) External Unit of measure for the quantity on the line item.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="CurrencyType">
      <xsd:annotation>
         <xsd:documentation>(Both) Currency on the business object.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="GtmCurrencyTypeGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Qualifier of currency value present on the business object.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CurrencyValue" type="otm:GLogXMLFinancialAmountType">
            <xsd:annotation>
               <xsd:documentation>(Both) Currency value present on the business object.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="GtmFormulaExpressionGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Formula expression used for calculating the currency value.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ValueNote" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) More information for the currency value.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="MethodOfCalculation" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Identifies whether the currency is manually entered or calculated by formula or external engine.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsFixed" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) The currency value assigned once cannot be overwritten by formula engine.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PaymentMethodGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Method of the payment for the currency.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ReportingCurrencyCode" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Unit of measure for reporting purpose.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ReportingMonetaryAmount" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Currency value for reporting purpose.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RateToReporting" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Conversion factor used for converting from entered currency to reporting currency.</xsd:documentation>
            </xsd:annotation>
         </xsd:element> 
         <xsd:element name="ExchangeRateInfo" type="otm:ExchangeRateInfoType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Exchange rate used for calculation of the currency value.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="TransactionPolicyType">
      <xsd:annotation>
         <xsd:documentation>(Both) Policy details of business transaction
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="GtmPolicyQualifierGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Kind of policy applied on the business transaction.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="GtmComplianceTemplateGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Policy applied on the business transaction.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ComplianceRuleGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Compliance rule used for policy determination.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RuleGroupGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Compliance rule group used for policy determination.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="TransactionRequiredDocumentType">
      <xsd:annotation>
         <xsd:documentation>(Outbound) Documents Required for the conduct of business transaction
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="DocumentDefGid" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Document required for the business transaction.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TransactionRequiredText" type="RequiredTextType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Text required for the business transaction.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TransactionDocumentReviewer" type="InvolvedPartyDetailType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Reviewer of the document.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TransactionDocumentSubscriber" type="InvolvedPartyDetailType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Subscriber of the document.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ComplianceRuleGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Compliance rule used for document determination.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ComplianceRuleGroupGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Compliance rule group used for document determination.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ReviewStatus" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Status of the document review.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="PartyScreeningResultType">
      <xsd:annotation>
         <xsd:documentation>(Outbound) Represents Party Screen Result
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="SequenceNumber" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Sequence of matching denied parties.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="GtmDeniedPartyGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Matching denied party.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ScreeningStatus" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Status of the document review.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ApprovedBy" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Reviewer of the denied party match.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Comments" type="CommentsType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>Comments provided by reviewer.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="CommentsType">
      <xsd:annotation>
         <xsd:documentation>(Outbound) Comments about the approval.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="SequenceNumber" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Sequence of comments provided by reviewer.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CommentText" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Comments provided by reviewer.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CommentBy" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Outbound) The reviewer who has provided the comments.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="GtmTransLineLicenseType">
      <xsd:annotation>
         <xsd:documentation>(Outbound) License Line assigned to line item.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="SequenceNumber" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Sequence of license applied.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="LicenseLineGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>License line assigned to the business transaction line.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="LicenseNumber" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>License number assigned to the business transaction line.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Quantity" type="QuantityType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Quantity of the license line assigned to the business transaction line.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Currency" type="CurrencyType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Currency value of the license line assigned to the business transaction line.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="GlUserGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>User who has assigned the license to the business transaction line.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="LicenseAssignedDate" type="otm:GLogDateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>The date on which the license assignment was made.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ControlTypeGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>The controls of the business transaction line for which the license is applied.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ControlCode" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>The controls of the business transaction line for which the license is applied.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="LicenseExpirationDate" type="otm:GLogDateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Date of expiration for the license.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="LicenseCode" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>License code assigned to the business transaction line.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="GtmTransactionLineRequiredDocumentType">
      <xsd:annotation>
         <xsd:documentation>(Outbound) Required Documents of transaction line item or declaration line item.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="GtmTransactionLineDocumentRequiredText" type="RequiredTextType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>Required text needed on the line item.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DocumentDefGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>Document type for the required text needed on the line item.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ComplianceRuleGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Compliance rule used for document and text template determination.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ComplianceRuleGroupGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Compliance rule group used for document and text template determination.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="RequiredTextType">
      <xsd:annotation>
         <xsd:documentation>(Outbound) Required Text of transaction
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Text" type="otm:TextType">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Required text needed on the business transaction line.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ComplianceRuleGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Compliance rule used for document and text template determination.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ComplianceRuleGroupGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Compliance rule group used for document and text template determination.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ProductClassificationType">
      <xsd:annotation>
         <xsd:documentation>(Both) Product classification code.

            Deprecated (6.4.3): TradeDirection. Functionality removed. Value will be ignored and field will be removed in a future release.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ClassificationCategory" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Classification category for the product classification.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="GtmProdClassTypeGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Product classification type.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ClassificationCode" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Both) Product classification code.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TradeDirection" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Both) Trade direction for the product classification on a goods transaction.
                  Deprecated (6.4.3): Functionality removed. Value will be ignored and field will be removed in a future release.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Attribute" type="AttributeType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Both) Attribute applicable for the product classification.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Note" type="ProdClassCodeNoteType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Both) Notes applicable for the product classification.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="UOM" type="ProdClassCodeUOMType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Both) Reporting uoms applicable for the product classification
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ClassificationDescription" type="ProdClassCodeDescType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Both) Description/comments applicable for the product classification.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ProdClassCodeNoteType">
      <xsd:annotation>
         <xsd:documentation>(Both) Notes on the Product classification code.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Type" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Both) Type of the notes on the product classification.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Value" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Both) Value of the notes on the product classification.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ProdClassCodeUOMType">
      <xsd:annotation>
         <xsd:documentation>Unit of measure related to product classification code.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Name" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Both) Name of the unit of measure on the product classification.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Code" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Value of the unit of measure present in GTM on the product classification.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ReportingUOM" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) The unit of measure for the particular country where reporting must be done
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ProdClassCodeDescType">
      <xsd:annotation>
         <xsd:documentation>(Both) Description of the Product classification code in different languages.
         </xsd:documentation>
      </xsd:annotation>  
      <xsd:sequence>
         <xsd:element name="LanguageGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Language for the product classification.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Description" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Both) Description applicable for the product classification in particular language.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ControlScreeningResultGenericType">
      <xsd:annotation>
         <xsd:documentation>Generic Trade control screening result information.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ControlTypeGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Control type applicable to the business transaction lines or declaration lines.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ControlCode" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Control code applicable to the business transaction lines or declaration lines.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ComplianceRuleGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Compliance rule used for assigning the control to the business transaction lines or declaration lines.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ComplianceRuleGroupGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Compliance rule group used for assigning the control to the business transaction lines or declaration lines.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Authorization" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Manually authorizing or overriding the control with some comments.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AuthorizationAddedBy" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Outbound) The user who has assigned manually authorized or overriden the control.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AuthorizationAddedOn" type="otm:GLogDateTimeType">
            <xsd:annotation>
               <xsd:documentation>(Outbound) The date on which the control was manually authorized or overriden.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ControlScreeningResultType">
      <xsd:annotation>
         <xsd:documentation>(Outbound) Trade control screening result.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="GtmControlTypeGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Outbound) License controls applicable to the business transaction lines.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ControlCode" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Outbound) License controls applicable to the business transaction lines.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ComplianceRuleGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Compliance rule group used for assigning the license controls to the business transaction lines.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ComplianceRuleGroupGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Regulation reference for the liccense controls assigned to the business transaction lines.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="GtmregulationRefGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Regulation reference for the liccense controls assigned to the business transaction lines.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="GtmExceptionControlTypeGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Exception controls applicable to the business transaction lines.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="GtmExceptionControlCode" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Exception controls applicable to the business transaction lines.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Authorization" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Manually authorizing or overriding the license controls with some comments.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="LicenseQuantity" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>
                   Deprecated since v6.4.3: Element is no longer required
                   
                   (Outbound) Quantity of external license assigned to the controls.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="LicenseUom" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>
                   Deprecated since v6.4.3: Element is no longer required
                   
                   (Outbound) UOM of external license assigned to the controls.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="LicenseAddedBy" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Outbound) The user who has assigned the external license assigned to the controls.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="LicenseAddedOn" type="otm:GLogDateTimeType">
            <xsd:annotation>
               <xsd:documentation>(Outbound) The date on which has assigned the external license assigned to the controls.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="InvolvedPartyQualifierGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Party involved in the controls assignment.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ControlCodeDescription" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Description/comments for the license controls assigned.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RegimeId" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Regime Id of the license controls assigned.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="SanctionControlScreeningResultType">
      <xsd:annotation>
         <xsd:documentation>(Outbound) Sanction control screening result.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="GtmControlTypeGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Sanction controls applicable to the business transaction lines.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ControlCode" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Sanction controls applicable to the business transaction lines.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ComplianceRuleGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Compliance rule used for assigning the sanction controls to the business transaction lines.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ComplianceRuleGroupGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Compliance rule group used for assigning the sanction controls to the business transaction lines.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Authorization" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Manually authorizing or overriding the sanction controls with some comments.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AuthorizationAddedBy" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Outbound) The user who has manually authorized or overriden the controls.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AuthorizationAddedOn" type="otm:GLogDateTimeType">
            <xsd:annotation>
               <xsd:documentation>(Outbound) The date on which the sanction controls was manually authorized or overriden      
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ControlCodeDescription" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Description/comments for the license controls assigned.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RegimeId" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Regime Id of the license controls assigned.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ReportingResultType">
      <xsd:annotation>
         <xsd:documentation>(Outbound) Controls applicable to line items for reporting purpose.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="GtmControlTypeGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Reporting control Type applicable to the business transaction line.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ControlCode" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Reporting control code applicable to the business transaction line.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ComplianceRuleGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Compliance rule used for assigning the reporting controls to the business transaction line.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ComplianceRuleGroupGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Compliance rule group used for assigning the reporting controls to the business transaction line.      
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="GtmExceptionControlTypeGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Exception controls applicable to the business transaction line.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="GtmExceptionControlCode" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Exception controls applicable to the business transaction line.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="RestrictedPartyScreeningResultType">
      <xsd:annotation>
         <xsd:documentation>(Outbound) Restricted party matches to the line party involved in business transaction.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="GtmContact" type="GtmContactType">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Party involvedin business transaction. 
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsPassed" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Indicates whether the party has successfully passed the restricted party screening.      
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="TerritoryInfoType">
      <xsd:annotation>
         <xsd:documentation>(Both) Region Information.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="InvolvedCountryQualifier" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Qualifier for the country of the business transaction.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CountryCode" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Country code of the business transaction.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="TransactionInvLocationType">
      <xsd:annotation>
         <xsd:documentation>(Both) An TransactionInvLocation specifies a location that is in some way involved with a TransOrder,
            Shipment, or Release.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="TransactionCode" type="otm:TransactionCodeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Indicates the purpose of the integration.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="InvolvedLocationQualifierGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Information to qualify the location involved in business transaction.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="LocationGid" minOccurs="0" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) The location involved in business transaction.     
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="RegionInfoType">
      <xsd:annotation>
         <xsd:documentation>(Both) Region Information.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="RegionQualifier" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Qualifier for the region of the business transaction.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RegionId" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Region of the business transaction.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="PortInfoType">
      <xsd:annotation>
         <xsd:documentation>(Both) Details of the port through which the business transaction is carried. 
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="PortQualifier" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Qualifier for the port of the business transaction.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PortId" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Qualifier for the port of the business transaction.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PortLocation" type="otm:LocationRefType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Details of the port location.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ControlTypeCodeType">
      <xsd:annotation>
         <xsd:documentation>(Both) License or reporting controls
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ControlType" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Both) Control type.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ControlCode" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Both) Control code.    
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="RegionCountryInfoType">
      <xsd:annotation>
         <xsd:documentation>(Both) Region or Country Information.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="RegionQualifier" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Qualifier for the region of the business transaction.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RegionId" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Region of the business transaction.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CountryCode" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Country of the business transaction.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="TransDateType">
      <xsd:annotation>
         <xsd:documentation>(Both) Date on the business transaction.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="DateQualifierGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Qualifier for the date of the business transaction.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TransactionDate" type="otm:GLogDateTimeType">
            <xsd:annotation>
               <xsd:documentation>(Both) Date of the business transaction.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="CalculatedValueResultType">
      <xsd:annotation>
         <xsd:documentation>
            Deprecated (6.4.3): This element won't be populated from 6.4 onwards and will be removed in a future release.
            (Outbound) Base amounts calulated and assigned to the business transaction.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="CalculatedValueGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Base amount value qualifier.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ComplianceRuleGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Compliance rule used for assigning the base amount.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RuleGroupGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Compliance rule group used for assigning the base amount.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="GtmStructureComponentType">
      <xsd:annotation>
         <xsd:documentation>(Both) Component present on the bill of material.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="GtmStructureCompSeq" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Both) Sequence for the component on the bill of material.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TransactionCode" type="otm:TransactionCodeType" minOccurs="0" />
         <xsd:element name="ItemGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Item on the component on the bill of material.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Description" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Description for the component on the bill of material.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="GtmProdClassCodeGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Product classification code for the goods on the component on the bill of material.
                  Deprecated (6.4.2): GtmProdClassCodeGid - Use ClassificationCode
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="GtmProdClassTypeGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Product classification code for the goods on the component on the bill of material.     
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CountryOfOriginGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Country of origin for the goods on the component on the bill of material.      
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Quantity" type="QuantityType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Both) Quantity data on the structure component.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Currency" type="CurrencyType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Both) Values data on the strcuture component.     
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Percentage" type="PercentageType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Both) Percentage share of the component on the bill of material.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Remark" type="otm:RemarkType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Both) Remark data of the strcuture component.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Refnum" type="otm:RefnumType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Both) Refnum number of the strcuture component.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ClassificationCode" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Product classification code for the goods on the component on the bill of material.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="PercentageType">
      <xsd:annotation>
         <xsd:documentation>(Both) Percentage information for an object. It consists of a qualifier and a value.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="PercentageQualifierGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Qualifier for the Percentage information.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PercentageValue" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Both) Percentage value for an object.      
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   
   <xsd:complexType name="GtmDeclarationLineType">
      <xsd:annotation>
         <xsd:documentation>
            (Both) Gtm Declaration information for an line item. Element ProductClassification
            is now supported both for inbound and outbound. ControlScreeningResult, ReportingResult
            RestrictedPartyScreeningResult, GtmTrItemStructureGid would be supported for outbound only.
            GtmDeclarationGid is supported only for outbound intiated on declaration line.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="GtmDeclarationLineGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Identifies the declaration line.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IntSavedQuery" type="otm:IntSavedQueryType" minOccurs="0" />
         <xsd:element name="TransactionCode" type="otm:TransactionCodeType" minOccurs="0" />
         <xsd:element name="GtmDeclarationGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Identifies the declaration.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ItemRef" type="otm:ItemRefType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Item details.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PrecedingTransactionLineGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Identifies the transaction line from which the declaration line is created.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ParentDeclarationLineGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Parent lines for the same declaration line.
                  For the declaration to be created via integration, parent declaration line should 
                  be created before the child lines and in_sequence should be marked as yes.	
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CountryOfOrigin" type="otm:CountryCodeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Countries where goods are originated and manufactured from.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ManufacturingCountry" type="otm:CountryCodeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Countries where goods are originated and manufactured from.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Remark" type="otm:RemarkType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Both) Remark data of the line item.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="InvolvedParty" type="GtmInvolvedPartyType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Both) Involved party of the line item.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Refnum" type="otm:RefnumType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Both) Reference number of the line item.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ProductClassification" type="ProductClassificationType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Both) Product classification applicable for the line item.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="UserDefinedClassification" type="UserDefinedClassificationType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Both) User Defined Classification of the line item.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ControlScreeningResult" type="ControlScreeningResultType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Outbound) The license controls applicable for the line item.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ReportingResult" type="ReportingResultType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Both) ReportingResult - the reporting controls applicable for the line item indicating 
                  whether the line needs to be reported to any regime.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RestrictedPartyScreeningResult" type="RestrictedPartyScreeningResultType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Both) Indicates the passed or failure status of the contact during restricted party screening.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Quantity" type="QuantityType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Both) Quantity data on the line item.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ReportingQuantity" type="ReportingQuantityType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Both) Quantity to be reported in the external uom.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Currency" type="CurrencyType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Both) Values data on the line item.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Percentage" type="PercentageType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Both) Percentage share of the current line in the bill of material structure.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Status" type="otm:StatusType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Business Status of the line item.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="GtmTransLineLicense" type="GtmTransLineLicenseType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Both) License assigned to the Line item.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="GtmTransactionLineRequiredDocument" type="GtmTransactionLineRequiredDocumentType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Both) Documents required for the declaration line.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="GtmTransactionLineRequiredText" type="RequiredTextType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Both) Texts required for the declaration line.   
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CalculatedValueResult" type="CalculatedValueResultType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>
                    Deprecated (6.4.3): This element won't be populated from 6.4 onwards and will be removed in a future release.
                    (Both) Base amount for the line item.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SanctionControlScreeningResult" type="SanctionControlScreeningResultType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Outbound) Sanction embargo screening result the line item.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="FlexFieldStrings" type="otm:FlexFieldStringType" minOccurs="0" />
         <xsd:element name="FlexFieldNumbers" type="otm:FlexFieldNumberType" minOccurs="0" />
         <xsd:element name="FlexFieldDates" type="otm:FlexFieldDateType" minOccurs="0" />
         <xsd:element name="GtmTrItemStructureGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) The (bill of material) structure assigned to the line item.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IncoTermGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Identifies the Inco term detail for the line item.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IncoTermLocation" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Identifies the Inco term Location for the line item.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="LineDescription" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) This field is used to specify the description on a line item.      
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ValuationMethod" type="ValuationMethodType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Valuation method applicable for the line item.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TariffPreferenceType" type="TariffPreferenceTypeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Tariff preference type under which the goods are imported or exported.      
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Procedure" type="ProcedureType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Details of procedure under which the goods is being declared.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PriorProcedure" type="ProcedureType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Details of previous procedure under which the goods was earlier declared.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ProcedureDetail" type="ProcedureDetailType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Details of procedure under which the goods is being declared.      
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="GtmBond" type="GtmBondType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Text template required for the movement or compliance of goods.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AdditionalInfo" type="AdditionalInfoType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Both) Text template required for the movement or compliance of goods.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ProducedDocument" type="ProducedDocumentType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>(Both) Sanction embargo screening result the line item.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TransLineDate" type="TransDateType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Dates when the declaration line was handled/created or shipment line was imported/exported etc.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OtherControlScreeningResult" type="ControlScreeningResultGenericType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Other controls for the declaration line.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="AdditionalInfoType">
      <xsd:annotation>
         <xsd:documentation>(Both) This element maintains the additional information master data.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="AdditionalInfoGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Global identifier for the additional information.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Code" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Identifies the code associated with the additional information.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Text" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Contains the text associated with the additional information.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ExportFrom" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Identifies the export from country details.      
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ProducedDocumentType">
      <xsd:sequence>
         <xsd:element name="ProducedDocumentTypeGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Global identifier for the produced document.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Name" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Identifies the name associated with the produced document</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Code" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Identifies the external code associated with the produced document
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ReferenceNumber" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Reference number.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Date" type="otm:GLogDateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Identifies the date of the produced document.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ValidationLocation" type="otm:LocationRefType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Identifies the location where the produced document was validated.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ArchiveLocation" type="otm:LocationRefType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Identifies the archive location associate with the produced document.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ArchiveFormat" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Identifies the archive format associated with the produced document.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ArchiveDetail" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Identifies additional archive detail associated with the produced document.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <!-- Types redefined to be in the GTM namespace should all be below here -->

   <xsd:complexType name="GtmInvolvedPartyType">
      <xsd:sequence>
         <xsd:element name="TransactionCode" type="otm:TransactionCodeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Identifies the purpose of integration.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="InvolvedPartyQualifierGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Both) Global identifier for the involved party qualifier.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:choice>
            <xsd:element name="GtmContactGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Global identifier for the party involved.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
            <xsd:element name="GtmContact" type="GtmContactType" minOccurs="0">
               <xsd:annotation>
                  <xsd:documentation>(Both) Details of the party involved.
                  </xsd:documentation>
               </xsd:annotation>
            </xsd:element>
         </xsd:choice>
         <xsd:element name="ComMethodGid" type="otm:GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>(Both) Global identifier for the communication method associated with the involved party
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="LocationOverrideInfo" type="otm:LocationOverrideInfoType" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="InvolvedPartyRefnumDetailType">
      <xsd:annotation>
         <xsd:documentation>(Inbound) Reference numbers for the party involved
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="PartyQualifierGidForRefnum" type="otm:GLogXMLGidType" >
            <xsd:annotation>
               <xsd:documentation>(Inbound) Qualifier for the party.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PartyRefnumQualifierGid" type="otm:GLogXMLGidType" >
            <xsd:annotation>
               <xsd:documentation>(Inbound) Qualifier for the reference number on the party.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PartyRefnumValue" type="xsd:string" >
            <xsd:annotation>
               <xsd:documentation>(Inbound) Reference number value.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="InvolvedPartyRemarkDetailType">
      <xsd:annotation>
         <xsd:documentation>(Inbound) Remarks for the party involved.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="PartyQualifierGidForRemark" type="otm:GLogXMLGidType" >
            <xsd:annotation>
               <xsd:documentation>(Inbound) Qualifier for the party.     
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PartyRemarkQualifierGid" type="otm:GLogXMLGidType" >
            <xsd:annotation>
               <xsd:documentation>(Both) Remark qualifier for the party.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PartyRemarkText" type="xsd:string" >
            <xsd:annotation>
               <xsd:documentation>(Both) Remark text for the party.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ConveyanceType">
      <xsd:annotation>
         <xsd:documentation>(Both) Defines equipment and seal details for a conveyance.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="SequenceNumber" type="xsd:string" >
            <xsd:annotation>
               <xsd:documentation>(Both) Sequence Number for the conveyance used in transaction.      
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="EquipmentInitial" type="xsd:string" >
            <xsd:annotation>
               <xsd:documentation>(Both) Equipment Initial of the conveyance used in transaction.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="EquipmentNumber" type="xsd:string" >
            <xsd:annotation>
               <xsd:documentation>(Both) Equipment Number of the conveyance used in transaction.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SealNumber" type="xsd:string" >
            <xsd:annotation>
               <xsd:documentation>(Both) Seal Number of the conveyance used in transaction.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <!-- Items that need code modification before they can be removed -->

   <xsd:complexType name="GtmRemarksType">
      <xsd:annotation>
         <xsd:documentation>(Inbound) Remarks for the item.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="RemarkQualifierGid" type="otm:GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>(Inbound) Remark qualifier for the item on the business transaction line.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RemarkText" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>(Inbound) Remark for the the item on the business transaction line.       
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

</xsd:schema>