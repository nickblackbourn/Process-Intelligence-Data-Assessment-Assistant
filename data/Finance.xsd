   <xsd:complexType name="WorkInvoiceType">
      <xsd:annotation>
         <xsd:documentation>
            A WorkInvoiceType represents driver pay information for a transportation shipment. It includes list of
            activities performed by the driver at different stops (like loading, unloading etc) or transit
            (deadhead or bobtail) for a transportation shipment.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="OTMTransactionInOut">
            <xsd:sequence>
               <xsd:element name="IntSavedQuery" type="IntSavedQueryType" minOccurs="0"/>
               <xsd:element name="WorkInvoiceGid" type="GLogXMLGidType">
                  <xsd:annotation>
                     <xsd:documentation>
                        Unique identifier for an Driver Work Invoice. Validation: References WORK_INVOICE table.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="TransactionCode" type="TransactionCodeType"/>
               <xsd:element name="ReplaceChildren" type="ReplaceChildrenType" minOccurs="0"/>
               <xsd:element name="WorkInvoiceNumber" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Represents work invoice number. This a number known to the external party and can e used to identify the OTM work invoice id.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:choice>
                  <xsd:element name="DriverGid" type="GLogXMLGidType">
                     <xsd:annotation>
                        <xsd:documentation>
                           ID of Primary Driver.
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Driver" type="DriverType">
                     <xsd:annotation>
                        <xsd:documentation>
                           Primary Driver Details. It include driver details like name, address, driver type, calendar,
                           HOS contact information, refnum, remarks etc.
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:choice>
               <xsd:choice minOccurs="0">
                  <xsd:element name="SecondaryDriverGid" type="GLogXMLGidType" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation>
                           ID of Secondary Driver.
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="SecondaryDriver" minOccurs="0">
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Driver" type="DriverType">
                              <xsd:annotation>
                                 <xsd:documentation>
                                    Secondary Driver Details. It include driver details like name, address, driver type,
                                    calendar, HOS contact information, refnum, remarks etc.
                                 </xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:choice>
               <xsd:choice>
                  <xsd:element name="ShipmentGid" type="GLogXMLGidType">
                     <xsd:annotation>
                        <xsd:documentation>
                           ID of Shipment associated with this driver work invoice.
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Shipment" type="ShipmentType">
                     <xsd:annotation>
                        <xsd:documentation>
                           Shipment Details. It include shipment details like header, source location,
                           destination location, SU's, Lines, Stops, Cost etc.
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:choice>
               <xsd:element name="DateSent" type="GLogDateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Date this work invoice was send to payroll.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Revenue" type="GLogXMLFinancialAmountType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Total work invoice amount to be paid to driver.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="WorkInvoiceActivity" type="WorkInvoiceActivityType" minOccurs="0"
                            maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        Lines of Work Invoice. List of activities performed by driver for this shipment.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Status" type="StatusType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        This is used to indicate the internal or external Work Invoice Status.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="WorkInvoiceActivityType">
      <xsd:annotation>
         <xsd:documentation>
            Represents Lines of Work Invoice; List of activities performed by driver for the shipment.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ActivitySequence" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the work invoice activity sequence. Validation references the WORK_INVOICE_ACTIVITY table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ActivityDate" type="GLogDateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  ActivityDate indicate the start date of the activity. Activity like loading or unloading performed
                  at specific stop (i.e. activity having same source and destination location) is calculated
                  from shipment special service actual or planned date. Activity like deadhead or bobtail performed
                  between stops (i.e. activity having different source and destination location) is calculated from
                  shipment stop actual or planned date.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SpecialServiceGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  SpecialServiceGid specifies a special service.Validation: Reference to SPECIAL_SERVICE table.
                  Special Service represents Work Activity like Load, Unload etc.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SourceLocationGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the Activity Source Location. Activity having same source and destination location
                  indicate activity performed at the specific location. example of these are loading and unloading.
                  Activity having different source and destination location indicate transit activities like transport,
                  deadhead or bobtail etc.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DestLocationGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the Activity Source Location. Activity having same source and destination location
                  indicate activity performed at the specific location. example of these are loading and unloading.
                  Activity having different source and destination location indicate transit activities like transport,
                  deadhead or bobtail etc.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ActivityDistance" type="GLogXMLDistanceType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  ActivityDistance indicate the distance travelled in performing the activity. It captures bobtail,
                  deadhead or transport distance travelled.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ActivityDuration" type="GLogXMLDurationType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  ActivityDuration indicate the duration in performing the activity. It captures loading, unloading,
                  bodtail, deadhead or transport duration.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="WeightVolume" type="WeightVolumeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  WeightVolume inidicate the weight and volume involved in performing the activity.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ShipUnitCount" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  ShipUnitCount indicate the count of ship units involved in performing the activity. It captures
                  count of ship units during loading, unloading, bodtail, deadhead or transport.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ItemPackageCount" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  ItemPackageCount indicate the count of Item Packages involved in performing the activity. It captures
                  count of packaged items during loading, unloading, bodtail, deadhead or transport.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PayableIndicatorGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Indicates payable state of the activity. Values may include Not Payable, Payable, Requies Approval etc.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

</xsd:schema>