<?xml version="1.0" encoding="utf-8"?>
<xsd:schema targetNamespace="http://xmlns.oracle.com/apps/otm/transmission/v6.4" elementFormDefault="qualified"
            attributeFormDefault="unqualified"
            xmlns="http://xmlns.oracle.com/apps/otm/transmission/v6.4"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema">
   <xsd:annotation>
      <xsd:documentation>
         Release 6.4.2 Order Schema. Last modified Nov, 2016.

         Contains element, attribute and types definitions common to all Order interfaces.
      </xsd:documentation>
   </xsd:annotation>

   <xsd:include schemaLocation="TransmissionCommon.xsd"/>
   <xsd:include schemaLocation="LocationContact.xsd"/>
   <xsd:include schemaLocation="ShipUnit.xsd"/>
   <xsd:include schemaLocation="Item.xsd"/>

   <xsd:element name="TransOrder" type="TransOrderType" substitutionGroup="GLogXMLTransaction">
      <xsd:annotation>
         <xsd:documentation>
            A TransOrder represents demand for transportation services. This interface will create or modify
            the Puchase Order (Order Base) in OTM.
         </xsd:documentation>
      </xsd:annotation>
   </xsd:element>

   <xsd:element name="TransOrderLink" type="TransOrderLinkType" substitutionGroup="GLogXMLTransaction">
      <xsd:annotation>
         <xsd:documentation>
            Used to establish a link between Order Base objects. This provides the ability to maintain orders
            in various states along with their relationships.
         </xsd:documentation>
      </xsd:annotation>
   </xsd:element>

   <xsd:element name="TransOrderStatus" type="TransOrderStatusType" substitutionGroup="GLogXMLTransaction">
      <xsd:annotation>
         <xsd:documentation>
            The TransOrderStatus element is used to transmit order base status event or order release status
            information to OTM.
         </xsd:documentation>
      </xsd:annotation>
   </xsd:element>

   <xsd:element name="Release" type="ReleaseType" substitutionGroup="GLogXMLTransaction">
      <xsd:annotation>
         <xsd:documentation>
            The Release interface creates or modifies an Order Release when sent inbound to OTM. It can also be
            sent outbound in response to an Order Release event or contained in other interfaces e.g.
            PlannedShipment.
         </xsd:documentation>
      </xsd:annotation>
   </xsd:element>

   <xsd:element name="ReleaseInstruction" type="ReleaseInstructionType" substitutionGroup="GLogXMLTransaction">
      <xsd:annotation>
         <xsd:documentation>
            The ReleaseInstruction interface creates instructions for an Order Base to control how Order Releases
            should be
            constructed.
         </xsd:documentation>
      </xsd:annotation>
   </xsd:element>

   <xsd:element name="OBShipUnit" type="OBShipUnitType" substitutionGroup="GLogXMLTransaction">
      <xsd:annotation>
         <xsd:documentation>
            This interface can used to send out the TransOrder ShipUnit information.
         </xsd:documentation>
      </xsd:annotation>
   </xsd:element>

   <xsd:element name="OBLine" type="OBLineType" substitutionGroup="GLogXMLTransaction">
      <xsd:annotation>
         <xsd:documentation>
            This interface can used to send out the TransOrder Line information.
         </xsd:documentation>
      </xsd:annotation>
   </xsd:element>

   <xsd:element name="OrderMovement" type="OrderMovementType" substitutionGroup="GLogXMLTransaction">
      <xsd:annotation>
         <xsd:documentation>
            The OrderMovement interface is used to create or modify the routing for Ship Units associated with
            an Order Release.
         </xsd:documentation>
      </xsd:annotation>
   </xsd:element>

   <xsd:element name="Quote" type="QuoteType" substitutionGroup="GLogXMLTransaction" />

   <xsd:complexType name="TransOrderType">
      <xsd:annotation>
         <xsd:documentation>
            A TransOrder consists of header level information and detail information.
            The detail information consists of either line items, or ship units, but not both.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="OTMTransactionInOut">
            <xsd:sequence>
               <xsd:element name="TransOrderHeader" type="TransOrderHeaderType">
                  <xsd:annotation>
                     <xsd:documentation>
                        Describes header level information related to processing of the order base.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:choice minOccurs="0">
                  <xsd:element name="TransOrderLineDetail" type="TransOrderLineDetailType" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation>
                           Order item detail at the line item level.
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="ShipUnitDetail" type="ShipUnitDetailType" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation>
                           Order item detail at the ship unit level.
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:choice>
               <xsd:element name="Location" type="LocationType" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element name="PackagedItem" type="PackagedItemType" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="TransOrderHeaderType">
      <xsd:annotation>
         <xsd:documentation>
            Contains all header level information e.g. constraints, reference numbers, which apply
            to the entire Order Base.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="SendReason" type="SendReasonType" minOccurs="0"/>
         <xsd:element name="IntSavedQuery" type="IntSavedQueryType" minOccurs="0"/>
         <xsd:element name="TransOrderGid" type="GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>
                  Global unique identifier for a transportation order.Validation: Reference to OB_ORDER_BASE
                  table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ExternalSystemId" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  ExternalSystemId may be used to identify the system from which the TransOrder came.DataType:
                  A30Validation: None
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TransactionCode" type="TransactionCodeType"/>
         <xsd:element name="ReplaceChildren" type="ReplaceChildrenType" minOccurs="0"/>
         <xsd:element name="IsUpdateShipmentOnly" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Y, N. Flag used in Integration processing to indicate if that the TransOrder should be used to
                  update theShipment only, and not the Order Base related information. This flag is provided as a
                  convenience whichreduces the need to implement a separate SShipUnit or ActualShipment
                  interface.The interface is restricted as follows:- The original order should have been created
                  using the ShipUnitDetail (not the TransOrderLineDetail).- The information updated is restricted
                  to the SShipUnit element only. The TransOrderHeader is ignored, andnone of the other Shipment
                  related information will be updated.- The specific S_Ship_Unit(s) to be modified are identified
                  by using the ShipUnitGid in this TransOrder XMLand searching for the related Release.ShipUnit
                  (via the OB_SHIP_UNIT_GID on SHIP_UNIT table) and then theShipment.ShipUnit(s) (via the
                  SHIP_UNIT_GID field in the S_SHIP_UNIT table). The search requires thosereference pointers to
                  exist.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ProcessingCodeGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  ProcessingCodeGid is used to indicate how a TransOrder should be processed.Specify PLN to
                  indicate that the TransOrder should be planned automatically into Shipment(s) upon
                  insert.Specify NOPLN to indicate that the TransOrder should not be planned automatically, but
                  requires userintervention (e.g. Bulk Plan process, agent action) to build the Shipment(s).
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ReleaseMethodGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the type of releasing logic to be used. References RELEASE_METHOD table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="InvolvedParty" type="InvolvedPartyType" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element name="CommercialTerms" type="CommercialTermsType" minOccurs="0"/>
         <xsd:element name="PlanningGroupGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  TransOrders with a common PlanningGroupGid are processed together by the planning
                  logic.Validation: References PLANNING_GROUP table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="FixedItineraryGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  FixedItineraryGid allows the TransOrder to be preassigned to a given Itinerary.Validation:
                  References ITINERARY table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OrderTypeGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Order type gid, refrence ORDER_TYPE table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TimeWindowEmphasisGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  TimeWindowEmphasisGid indicates which components of the TransOrder time window should be
                  emphasised.Specify 'BOTH' if both early and late delivery dates must be honored.Specify 'SHIP'
                  if only ship window need by honored.Specify 'DELV' if only delivery window need by honored.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsAppointmentRequired" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  IsAppointmentRequired is a boolean flag indicating if an appointment is requiredin order to make
                  the delivery at the destination locations.Specify 'Y' if an appointment is required, 'N'
                  otherwise.DataType: A1Validation: (Y,N)See Also: ZZSchemaIssues
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsMakeAppointmentBeforePlan" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  IsMakeAppointmentBeforePlan is a boolean flag specifying if one must schedule an
                  appointmentprior to planning transportation for the TransOrder.Specify 'Y' is an appointment
                  must be made before the order can be shipped, 'N' otherwise.Can be specified at the order or
                  location level.The order level takes precedence.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:choice minOccurs="0">
            <xsd:element name="RateServiceGid" type="GLogXMLGidType">
               <xsd:annotation>
                  <xsd:documentation>
                     This field will be copied to the order created from this order base. RateServiceGid
                     specifies the rate service to be used when planning this order onto a buy side
                     shipment.Validation: Reference to RATE_SERVICE table. .
                  </xsd:documentation>
               </xsd:annotation>
            </xsd:element>
            <xsd:element name="RateServiceProfileGid" type="GLogXMLGidType">
               <xsd:annotation>
                  <xsd:documentation>
                     This field will be copied to the order created from this order base. RateServiceProfileGid
                     specifies the rate service to be used when planning this order onto a buy side
                     shipment.Validation: Reference to RATE_SERVICE_PROFILE table. .
                  </xsd:documentation>
               </xsd:annotation>
            </xsd:element>
         </xsd:choice>
         <xsd:choice minOccurs="0">
            <xsd:element name="ServiceProviderGid" type="GLogXMLGidType">
               <xsd:annotation>
                  <xsd:documentation>
                     This field will be copied to the order created from this order base. ServiceProviderGid
                     specifies the service provider to be used when planning this order onto a buy side
                     shipment.Validation: Reference to SERVPROV table.
                  </xsd:documentation>
               </xsd:annotation>
            </xsd:element>
            <xsd:element name="ServiceProviderProfileGid" type="GLogXMLGidType">
               <xsd:annotation>
                  <xsd:documentation>
                     This field will be copied to the order created from this order base.
                     ServiceProviderProfileGid specifies the service provider to be used when planning this order
                     onto a buy side shipment.Validation: Reference to SERVPROV_PROFILE table.
                  </xsd:documentation>
               </xsd:annotation>
            </xsd:element>
         </xsd:choice>
         <xsd:choice minOccurs="0">
            <xsd:element name="SellServiceProviderGid" type="GLogXMLGidType">
               <xsd:annotation>
                  <xsd:documentation>
                     This field will be copied to the order created from this order base. SellServiceProviderGid
                     specifies the service provider to be used when planning this order onto a sell side
                     shipment.Validation: Reference to SERVPROV table.
                  </xsd:documentation>
               </xsd:annotation>
            </xsd:element>
            <xsd:element name="SellServiceProviderProfileGid" type="GLogXMLGidType">
               <xsd:annotation>
                  <xsd:documentation>
                     This field will be copied to the order created from this order base.
                     SellServiceProviderProfileGid specifies the service provider to be used when planning this
                     order onto a sell side shipment.Validation: Reference to SERVPROV_PROFILE_GID table.
                  </xsd:documentation>
               </xsd:annotation>
            </xsd:element>
         </xsd:choice>
         <xsd:choice minOccurs="0">
            <xsd:element name="TransportModeGid" type="GLogXMLGidType">
               <xsd:annotation>
                  <xsd:documentation>
                     This field will be copied to the order created from this order base. TransportModeGid
                     specifies the mode to be used when planning this order onto a buy side shipment.Validation:
                     Reference to TRANSPORT_MODE table.
                  </xsd:documentation>
               </xsd:annotation>
            </xsd:element>
            <xsd:element name="ModeProfileGid" type="GLogXMLGidType">
               <xsd:annotation>
                  <xsd:documentation>
                     This field will be copied to the order created from this order base. ModeProfileGid
                     specifies the mode to be used when planning this order onto a buy side shipment.Validation:
                     Reference to TRANSPORT_MODE_PROFILE table.
                  </xsd:documentation>
               </xsd:annotation>
            </xsd:element>
         </xsd:choice>
         <xsd:choice minOccurs="0">
            <xsd:element name="EquipmentGroupGid" type="GLogXMLGidType">
               <xsd:annotation>
                  <xsd:documentation>
                     This field will be copied to the order created from this order base. EquipmentGroupGid
                     specifies the mode to be used when planning this order onto a buy side shipment.Validation:
                     Reference to EQUIPMENT_GROUP table.
                  </xsd:documentation>
               </xsd:annotation>
            </xsd:element>
            <xsd:element name="EquipmentGroupProfileGid" type="GLogXMLGidType">
               <xsd:annotation>
                  <xsd:documentation>
                     This field will be copied to the order created from this order base.
                     EquipmentGroupProfileGid specifies the equipment group to be used when planning this order
                     onto a buy side shipment.Validation: Reference to EQUIPMENT_GROUP_PROFILE table.
                  </xsd:documentation>
               </xsd:annotation>
            </xsd:element>
         </xsd:choice>
         <xsd:element name="OrderRefnum" type="OrderRefnumType" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element name="ShipWithGroup" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Global unique identifier for a transportation order.Validation: Reference to OB_ORDER_BASE
                  table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Remark" type="RemarkType" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element name="OBSpecialService" type="SpecialServiceType" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element name="AccessorialCodeGid" type="GLogXMLGidType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>
                  Global unique identifier for a transportation order.Validation: Reference to OB_ORDER_BASE
                  table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsFullyReleased" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Y, N, whether this order base is fully released.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="EffectiveDate" type="GLogDateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  When this order base become effective.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ExpirationDate" type="GLogDateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  When this order base become expired.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsTemplate" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Y, N, specifies whether this order base is a template or not.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RateOfferingGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  This field will be copied to the order created from this order base. RateOfferingGid specifies
                  the rate offering to be used when planning this order onto a buy side shipment.Validation:
                  Reference to RATE_OFFERING table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RateGeoGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  This field will be copied to the order created from this order base. RateGeoGid specifies the
                  rate geo to be used when planning this order onto a buy side shipment.Validation: Reference to
                  RATE_GEO table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SellRateOfferingGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  This field will be copied to the order created from this order base. RateOfferingGid specifies
                  the rate offering to be used when planning this order onto a sell side shipment.Validation:
                  Reference to RATE_OFFERING table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SellRateGeoGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  This field will be copied to the order created from this order base. RateGeoGid specifies the
                  rate geo to be used when planning this order onto a sell side shipment.Validation: Reference to
                  RATE_GEO table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="FixedSellItineraryGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  This field will be copied to the order created from this order base. FixedSellItineraryGid
                  specifies the itinerary to be used when planning this order onto a sell side
                  shipment.Validation: Reference to ITINERARY table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="MustShipDirect" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Y,N, This field will be copied to the order created from this order base. When planning this
                  order onto shipment, it determines whether it should go on direct shipment.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="MustShipThruXDock" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Y,N, This field will be copied to the order created from this order base. When planning this
                  order onto shipment, whether it must go thru xdock.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="MustShipThruPool" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Y,N, This field will be copied to the order created from this order base. When planning this
                  order onto shipment, whether it must go thru pool.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="BundlingType" type="xsd:string" minOccurs="0"/>
         <xsd:element name="IsShipperKnown" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Global unique identifier for a transportation order.Validation: Reference to OB_ORDER_BASE
                  table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TotalNetWeightVolume" type="WeightVolumeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Total net weight of the order base.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DimRateFactorGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  DimRateFactorGid uniquely identifies a dim factor.Validation: Reference to DIM_RATE_FACTOR
                  table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PickupRoutingSeqGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the pickup routing sequence. Validation references the ROUTING_SEQUENCE table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DropoffRoutingSeqGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the dropoff routing sequence. Validation references the ROUTING_SEQUENCE table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="StuffLocation" type="GLogXMLLocRefType" minOccurs="0"/>
         <xsd:element name="DestuffLocation" type="GLogXMLLocRefType" minOccurs="0"/>
         <xsd:element name="StowageModeGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Uniquely identifies a stowage mode for a voyage or flight.Validation: Reference to STOWAGE_MODE
                  table.Note: This element replaces the VoyageStowageMode element.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="UnitizationConditionName" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the Unitization Condition Name.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CustomerUnitizationRequest" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Y, N. Specifies if it is a customer has requested unitization for the order.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsIgnoreLocationCalendar" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Global unique identifier for a transportation order.Validation: Reference to OB_ORDER_BASE
                  table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ReleaseInstruction" type="ReleaseInstructionType" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element name="Text" type="TextType" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element name="OrderLegConstraint" type="OrderLegConstraintType" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element name="OrderPriority" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the Priority Of the Order
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="FlexFieldStrings" type="FlexFieldStringType" minOccurs="0"/>
         <xsd:element name="FlexFieldNumbers" type="FlexFieldNumberType" minOccurs="0"/>
         <xsd:element name="FlexFieldDates" type="FlexFieldDateType" minOccurs="0"/>
         <xsd:element name="FlexFieldCurrencies" type="FlexFieldCurrencyType" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="TransOrderLineType">
      <xsd:annotation>
         <xsd:documentation>
            A TransOrderLine is a request to move a specified quantity of a specified item from a
            given ShipFrom location to a given ShipTo location within a given time window.

            The ShipFromLocationRef and ShipToLocationRef elements are marked as optional for updates,
            but are requred on inserts.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="TransOrderLineGid" type="GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>
                  TransOrderLineGid uniquely identifies a TransOrderLine within the GLog database.Validation:
                  References OB_LINE.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IntSavedQuery" type="IntSavedQueryType" minOccurs="0"/>
         <xsd:element name="TransactionCode" type="TransactionCodeType" minOccurs="0"/>
         <xsd:element name="PackagedItemRef" type="PackagedItemRefType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies packaged item information.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="InitialItemGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  InitialItemGid can be used in lieue of specifying the PackagedItem, and allowing GC3 to
                  automatically assign thePackagedItem using the automated data assignment functionality based on
                  defined assignment rules.Validation: References ITEM table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PackagedItemSpecRef" type="GLogXMLShipUnitSpecRefType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Used to reference a ship unit spec for the Item and the Packaging.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PackagedItemSpecCount" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the count of the PackagedItemSpec. For example, if the PackagedItemSpecRef referred to
                  CASE,then this count would correspond to the number of cases.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TransportHandlingUnitRef" type="GLogXMLShipUnitSpecRefType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the transport handling unit (ship unit spec).
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ShipFromLocationRef" type="GLogXMLLocRefType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  ShipFromLocationRef specifies the ship-from location for a TransOrderLine.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ShipFromLoadPoint" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the load point for the ship-from or source location. References the LOAD_UNLOAD_POINT
                  table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ShipToLocationRef" type="GLogXMLLocRefType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  ShipToLocationRef specifies the ship-to location for a TransOrderLine.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ShipToUnloadPoint" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the unload point for the ship-to or destination location. References the
                  LOAD_UNLOAD_POINT table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ItemQuantity" type="ItemQuantityType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  ItemQuantity is a grouping of item related fields.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TimeWindow" type="TimeWindowType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  TimeWindow is a group of fields for specifying early/late pickup, early/late delivery, and
                  pickup/delivery appointments.The PickupIsAppt and DeliveryIsAppt elements are only applicable
                  when the TimeWindow is in the TransOrderLine,TransOrder.ShipUnitDetail.ShipUnit, and Release
                  elements.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ReleasedCount" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the number of items or ship units released.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="NumLayersPerShipUnit" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Number of layers per ship unit.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="QuantityPerLayer" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Quantity per layer.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ReleasedWeightVolume" type="WeightVolumeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the released weight and volume.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PlanFromLocationGid" type="GLogXMLLocGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Indicates an alternative location from which an order will be sourced for transportation
                  purposes.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PlanFromLoadPoint" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the load point for the plan from location. References the LOAD_UNLOAD_POINT table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PlanToLocationGid" type="GLogXMLLocGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Indicates an alternative location from which an order will be destined for transportation
                  purposes.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PlanToUnloadPoint" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the unload point for the plan to location. References the LOAD_UNLOAD_POINT table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RoundingInstruction" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies whether to round up, down, or not to round at all. Possible values are 'N' for no
                  rounding, 'U' for roundingup, and 'D' for rounding down. Default value is 'N'.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RoundTo" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies whether to round the packaging unit quantity (PUQ), layer (LAY), or transport handling
                  unit (THU).Possible values are PUQ, LAY, or THU.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="BufferLocationGid" type="GLogXMLLocGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the buffer location to be used during the releasing process if the release process
                  supports buffering.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ShipUnitSpecProfileGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Uniquely identifies a ship unit specification profile. Validation: References
                  SHIP_UNIT_SPEC_PROFILE table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OrderLineRefnum" type="OrderLineRefnumType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>
                  OrderLineRefnum is an alternate method for identifying an order line. It consists of a qualifier
                  and a value.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Remark" type="RemarkType" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element name="ItemAttributes" type="ItemAttributeType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>
                  Contains additional information about the line item.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="InvolvedParty" type="InvolvedPartyType" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element name="OBLineEquipRefUnit" type="EquipmentRefUnitType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>
                  Used to specify Ship Unit reference unit of a Order Base Line.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OBLinePackageRefUnit" type="PackageRefUnittype" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>
                  Used to specify Packaging Reference Unit of a Order Base Line.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="FlexFieldStrings" type="FlexFieldStringType" minOccurs="0"/>
         <xsd:element name="FlexFieldNumbers" type="FlexFieldNumberType" minOccurs="0"/>
         <xsd:element name="FlexFieldDates" type="FlexFieldDateType" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ReleaseType">
      <xsd:annotation>
         <xsd:documentation>
            Release is a structure for describing an order release. A release respresents a shippable subset of a
            TransOrder.
            It corresponds to the TransOrderLines which share common transportation requirements.
            It includes a common source/destination, time window, and multiple release lines which
            share the source/destination and time window.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="OTMTransactionInOut">
            <xsd:sequence>
               <xsd:element name="SendReason" type="SendReasonType" minOccurs="0"/>
               <xsd:element name="IntSavedQuery" type="IntSavedQueryType" minOccurs="0"/>
               <xsd:element name="ReleaseGid" type="GLogXMLGidType">
                  <xsd:annotation>
                     <xsd:documentation>
                        ID for the order release. If no Xid value entered is then a value is generated using the
                        BNG.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="TransactionCode" type="TransactionCodeType"/>
               <xsd:element name="ReplaceChildren" type="ReplaceChildrenType" minOccurs="0"/>
               <xsd:element name="IntCommand" type="IntCommandType" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element name="TransOrderGid" type="GLogXMLGidType" minOccurs="0"/>
               <xsd:element name="ReleaseHeader" type="ReleaseHeaderType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Header information related to the Order Release.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ShipFromLocationRef" type="GLogXMLLocRefType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Source location (or location ID) for the Order Release.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ShipFromLocOverrideRef" type="GLogXMLLocOverrideRefType" minOccurs="0"/>
               <xsd:element name="ShipFromLoadPoint" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Load point at the source location; this must be a valid load/unload point for the Source
                        Location.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ShipToLocationRef" type="GLogXMLLocRefType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Destination location (or location ID) for the Order Release.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ShipToLocOverrideRef" type="GLogXMLLocOverrideRefType" minOccurs="0"/>
               <xsd:element name="ShipToUnloadPoint" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Unload point at the destination location; this must be a valid load/unload point for the
                        Destination Location.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="TimeWindow" type="TimeWindowType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Pickup and delivery windows for the order.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="DeclaredValue" type="GLogXMLFinancialAmountType" minOccurs="0"/>
               <xsd:element name="ReleaseLine" type="ReleaseLineType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        Lines for the Order Release; OTM can be configured to create ship units from lines.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ShipUnit" type="ShipUnitType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        The actual shipping units used by planning logic when creating shipments from orders.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ReleaseTypeGid" type="GLogXMLGidType" minOccurs="0"/>
               <xsd:element name="PlanFromLocationGid" type="GLogXMLLocGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Location (or location ID) if an alternate source location should be used.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="PlanFromLocOverrideRef" type="GLogXMLLocOverrideRefType" minOccurs="0"/>
               <xsd:element name="PlanFromLoadPoint" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Load point at the PlanFromLocationGid; this must be a valid load/unload point for the
                        Plan From Location.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="PlanToLocationGid" type="GLogXMLLocGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Location (or location ID) if an alternate destination location should be used.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="PlanToLocOverrideRef" type="GLogXMLLocOverrideRefType" minOccurs="0"/>
               <xsd:element name="PlanToUnloadPoint" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Unload point at the PlanToLocationGid; this must be a valid load/unload point for the
                        Plan To Location.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="PriLegSourceLocationRef" type="GLogXMLLocRefType" minOccurs="0"/>
               <xsd:element name="PriLegSourceLocOverrideRef" type="GLogXMLLocOverrideRefType" minOccurs="0"/>
               <xsd:element name="PortOfLoadLocationRef" type="GLogXMLLocRefType" minOccurs="0"/>
               <xsd:element name="PortOfLoadLocOverrideRef" type="GLogXMLLocOverrideRefType" minOccurs="0"/>
               <xsd:element name="PortOfDisLocationRef" type="GLogXMLLocRefType" minOccurs="0"/>
               <xsd:element name="PortOfDisLocOverrideRef" type="GLogXMLLocOverrideRefType" minOccurs="0"/>
               <xsd:element name="PriLegDestLocationRef" type="GLogXMLLocRefType" minOccurs="0"/>
               <xsd:element name="PriLegDestLocOverrideRef" type="GLogXMLLocOverrideRefType" minOccurs="0"/>
               <xsd:element name="BulkPlanGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (outbound) Id for the bulk plan that planned this order into a buy side shipment.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="SellBulkPlanGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (outbound) Id for the bulk plan that planned this order into a sell side shipment.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="PlanPartitionGid" type="GLogXMLGidType" minOccurs="0"/>
               <xsd:element name="BestDirectBuyCost" type="GLogXMLFinancialAmountType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (outbound) Best buy side cost if this order were to be planned direct (i.e. not as part
                        of a bulk plan
                        or multi-stop); an action must be run to set this.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="BestDirectBuyRateOfferingGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (outbound) Best buy side rate offering if this order were to be planned direct (i.e. not
                        as part of a
                        bulk plan or multi-stop); an action must be run to set this.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="BestDirectSellCost" type="GLogXMLFinancialAmountType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (outbound) Best sell side cost if this order were to be planned direct (i.e. not as part
                        of a bulk plan
                        or multi-stop); an action must be run to set this.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="BestDirectSellRateOfferingGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (outbound) Best sell side rate offering if this order were to be planned direct (i.e.
                        not as part of a
                        bulk plan or multi-stop); an action must be run to set this.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="BuyGeneralLedgerGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Buy side General Ledger code for the order release.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="SellGeneralLedgerGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Sell side General Ledger code for the order release.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="TotalWeightVolume" type="GLogXMLWeightVolumeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (outbound) Total weight and volume of the order release.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="TotalNetWeightVolume" type="WeightVolumeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (outbound) Total net weight and net volume of the order release.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="TotalPackagedItemSpecCount" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (outbound) .
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="TotalPackagedItemCount" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (outbound) Total packaged item count.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ReleaseRefnum" type="ReleaseRefnumType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        Reference numbers on the order release.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ReleaseStatus" type="StatusType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (outbound) Status types and values for the order release.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Remark" type="RemarkType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        Remarks on the order release.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="REquipment" type="REquipmentType" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element name="InvolvedParty" type="InvolvedPartyType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        Involved parties on the order release.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="CommercialInvoice" type="CommercialInvoiceType" minOccurs="0"
                            maxOccurs="unbounded"/>
               <xsd:element name="Text" type="TextType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        Text attached to the order release.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ReleaseService" type="ReleaseServiceType" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element name="ReleaseShipmentInfo" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (outbound) Information about the shipments related to this order release.
                     </xsd:documentation>
                  </xsd:annotation>
                  <xsd:complexType>
                     <xsd:sequence>
                        <xsd:element name="LatestEstDeliveryDt" type="GLogDateTimeType">
                           <xsd:annotation>
                              <xsd:documentation>
                                 (outbound) Expected or actual delivery date from the final delivery stop of
                                 the order.
                              </xsd:documentation>
                           </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="EarliestEstPickupDt" type="GLogDateTimeType">
                           <xsd:annotation>
                              <xsd:documentation>
                                 (outbound) Expected or actual pickup date from the first pickup stop of the
                                 order.
                              </xsd:documentation>
                           </xsd:annotation>
                        </xsd:element>
                     </xsd:sequence>
                  </xsd:complexType>
               </xsd:element>
               <xsd:element name="ReleaseAllocationInfo" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Specifies the allocation information for the order release.
                     </xsd:documentation>
                  </xsd:annotation>
                  <xsd:complexType>
                     <xsd:sequence>
                        <xsd:element name="ReleaseAllocByType" type="ReleaseAllocType" minOccurs="0"
                                     maxOccurs="unbounded"/>
                     </xsd:sequence>
                  </xsd:complexType>
               </xsd:element>
               <xsd:element name="TransOrder" type="TransOrderType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (outbound) Order Base related to this order release.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="OrStop" type="OrStopType" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element name="OrderMovement" type="OrderMovementType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (outbound) Order movement for this order release.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="AllocationGroupGid" type="GLogXMLGidType" minOccurs="0"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="ReleaseHeaderType">
      <xsd:annotation>
         <xsd:documentation>
            The ReleaseHeader contains various order related attributes and control information.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ReleaseName" type="xsd:string" minOccurs="0"/>
         <xsd:element name="ExternalSystemId" type="xsd:string" minOccurs="0"/>
         <xsd:element name="ReleaseMethodGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  ID of the Order Configuration for how the order lines and ship units should be processed.
                  Applies to new
                  Releases only. The change of Release Method of existing Release is not supported.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="QuoteGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="MovePerspectiveGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="CommercialTerms" type="CommercialTermsType" minOccurs="0"/>
         <xsd:element name="FinalCommercialTerms" type="FinalCommercialTermsType" minOccurs="0"/>
         <xsd:element name="CommercialInvoiceTerms" type="CommercialInvoiceTermsType" minOccurs="0"/>
         <xsd:element name="PlanningGroupGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="TimeWindowEmphasisGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:choice minOccurs="0">
            <xsd:element name="RateServiceGid" type="GLogXMLGidType">
               <xsd:annotation>
                  <xsd:documentation>
                     ID of the rate service that must be used on the main leg when this order is planned into a
                     buy shipment.
                  </xsd:documentation>
               </xsd:annotation>
            </xsd:element>
            <xsd:element name="RateServiceProfileGid" type="GLogXMLGidType">
               <xsd:annotation>
                  <xsd:documentation>
                     Main leg rate service must be compatible with this profile.
                  </xsd:documentation>
               </xsd:annotation>
            </xsd:element>
         </xsd:choice>
         <xsd:choice minOccurs="0">
            <xsd:element name="ServiceProviderGid" type="GLogXMLGidType">
               <xsd:annotation>
                  <xsd:documentation>
                     ID of the service provider that must be used on the main leg when this order is planned into
                     a buy shipment.
                  </xsd:documentation>
               </xsd:annotation>
            </xsd:element>
            <xsd:element name="ServiceProviderProfileGid" type="GLogXMLGidType">
               <xsd:annotation>
                  <xsd:documentation>
                     Buy side main leg service provider must be compatible with this profile.
                  </xsd:documentation>
               </xsd:annotation>
            </xsd:element>
         </xsd:choice>
         <xsd:choice minOccurs="0">
            <xsd:element name="SellServiceProviderGid" type="GLogXMLGidType">
               <xsd:annotation>
                  <xsd:documentation>
                     ID of the service provider that must be used on the main leg when this order is planned into
                     a sell shipment.
                  </xsd:documentation>
               </xsd:annotation>
            </xsd:element>
            <xsd:element name="SellServiceProviderProfileGid" type="GLogXMLGidType">
               <xsd:annotation>
                  <xsd:documentation>
                     Sell side main leg service provider must be compatoible with this profile.
                  </xsd:documentation>
               </xsd:annotation>
            </xsd:element>
         </xsd:choice>
         <xsd:choice minOccurs="0">
            <xsd:element name="TransportModeGid" type="GLogXMLGidType">
               <xsd:annotation>
                  <xsd:documentation>
                     Transport mode that must be used on the main leg when this order is planned into a buy
                     shipment.
                  </xsd:documentation>
               </xsd:annotation>
            </xsd:element>
            <xsd:element name="ModeProfileGid" type="GLogXMLGidType">
               <xsd:annotation>
                  <xsd:documentation>
                     Buy side main leg transport mode must be compatible with this profile.
                  </xsd:documentation>
               </xsd:annotation>
            </xsd:element>
         </xsd:choice>
         <xsd:choice minOccurs="0">
            <xsd:element name="EquipmentGroupGid" type="GLogXMLGidType"/>
            <xsd:element name="EquipmentGroupProfileGid" type="GLogXMLGidType">
               <xsd:annotation>
                  <xsd:documentation>
                     Buy side main leg equipment group must be compatible with this profile.
                  </xsd:documentation>
               </xsd:annotation>
            </xsd:element>
         </xsd:choice>
         <xsd:element name="ShipWithGroup" type="xsd:string" minOccurs="0"/>
         <xsd:element name="IsTemplate" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Indicates if this is a template order; template orders cannot be planned into shipments.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TemplateType" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Used to specify whether to hide template created for customer from other order UI. Possible
                  values are 'Customer' and 'Standard'.'Customer' is for customer template.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RateOfferingGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  RateOfferingGid is a reference to a record in the RATE_OFFERING table,which specifies
                  contractual information for a particular rate offering.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RateGeoGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  RateGeoGid uniquely identifies a RateGeo element.Validation: References RATE_GEO table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SellRateOfferingGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  SellRateOfferingGid is used to indicate the contractual information for a particular rate
                  offering for the sell-side perspective (e.g. used to create a bill for the
                  order/shipment)Validation: References RATE_OFFERING table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SellRateGeoGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  SellRateGeoGid is used to indicate a specific Rate Record within a Rate Offering for the
                  sell-side perspective.Validation: References RATE_GEO table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="FixedItineraryGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  FixedItineraryGid allows the TransOrder to be preassigned to a given Itinerary.Validation:
                  References ITINERARY table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="FixedSellItineraryGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  FixedSellItineraryGid allows the TransOrder to be preassigned to a given Itinerary for the
                  sell-side perspective.The distinction between using Buy and Sell Fixed Itineraries is important
                  in situations when you want to create bills that reflect point-to-point transportation services
                  instead of intermediary stop-offs or multiple legs.To use these fields effectively, you have to
                  create the appropriate itineraries that properly reflect how youplan to buy transportation
                  services versus how you will ultimately bill for those services.Validation: References ITINERARY
                  table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="MustShipDirect" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Y/N, Used to indicate that order releases from this order base must ship direct from source to
                  destination.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="MustShipThruXDock" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Y/N, Indicates that order release from this order base must ship thru a x-dock. It is mutually
                  exclusive with MustShipDirect.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="MustShipThruPool" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Y/N, indicates that order release from this order base must ship thru a pool. It is mutually
                  exclusive from MustShipDirect.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="BundlingType" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies whether the order should be bundled with any other orders.Possible values are
                  'AUTOMATIC', 'MANUAL ONLY', 'DO NOT BUNDLE'. Default value is 'AUTOMATIC'.AUTOMATIC or MANUAL
                  ONLY allow bundling, and they are distinguished only by user defined queries.DO NOT BUNDLE means
                  the order will never be bundled with other orders.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsSplitAllowed" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Y, N. Specifies if the order release may be split or not. When set to Y in the ReleaseHeader,
                  the order can be split based on the individual split settings on the ShipUnit(s). When set to N
                  in the ReleaseHeader, the order cannot be split.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsShipperKnown" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Y, N. Flag indicating if the location represents a KNOWN shipper. This field is used in air
                  deliveries.Air deliveries CANNOT be put on a commercial (passenger) flight, if the shipper is an
                  'Unknown' shipper. (ie. does NOT apply to cargo flights.)If the SHIPPER involved party location
                  is Unknown, then the flight finder is limited to cargo flights.This field will default to 'N' if
                  missing.Within the TransOrder object, the IsShipperKnow field is used to indicate if the Shipper
                  was known at the time the order is placed. This preserves the historical value if the status of
                  the Shipper is changed at a later date.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DimRateFactorGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  DimRateFactorGid uniquely identifies a dim factor.Validation: Reference to DIM_RATE_FACTOR
                  table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PickupRoutingSeqGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the pickup routing sequence. Validation references the ROUTING_SEQUENCE table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DropoffRoutingSeqGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the dropoff routing sequence. Validation references the ROUTING_SEQUENCE table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ItineraryProfileGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Uniquely identifies an Itinerary Profile. Validation: Reference to ITINERARY_PROFILE
                  table.Within the ReleaseHeader, it specifies a constraint where only the itineraries compatible
                  with the profile will be considered when building buy shipments.Within RIQQuery, it specifies
                  the itinerary profile for rate inquiry input.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SellItineraryProfileGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Uniquely identifies an Itinerary Profile. Validation: Reference to ITINERARY_PROFILE
                  table.Within the ReleaseHeader, it specifies a constraint where only the itineraries compatible
                  with the profile will be considered when building sell shipments.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="InspectionAndSurveyInfo" type="InspectionAndSurveyInfoType" minOccurs="0"/>
         <xsd:element name="LetterOfCreditInfo" type="LetterOfCreditInfoType" minOccurs="0"/>
         <xsd:element name="ImportLicenseInfo" type="ImportLicenseInfoType" minOccurs="0"/>
         <xsd:element name="ConsolidationTypeGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Uniquely identifies a user defined qualifier for orders and job types.Validation: Reference to
                  CONSOLIDATION_TYPE table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="StuffLocation" type="GLogXMLLocRefType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the location where the stuffing occurs.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DestuffLocation" type="GLogXMLLocRefType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the location where the destuffing occurs.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="StowageModeGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Uniquely identifies a stowage mode for a voyage or flight.Validation: Reference to STOWAGE_MODE
                  table.Note: This element replaces the VoyageStowageMode element.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="UnitizationConditionName" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the Unitization Condition Name.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CustomerUnitizationRequest" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Y, N. Specifies if it is a customer has requested unitization for the order.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsIgnoreLocationCalendar" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Y, N. Specifies if the location calendar will be ignored when this order is planned onto a
                  shipment. Default value is N.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsConsolidateREquipment" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Y, N. Specifies whether to consolidate order release equipment when bundling. Default value is
                  'N'.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="UltimateDestCountryCode3Gid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the ultimate destination country code. Validation refers to COUNTRY_CODE table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="BufferType" type="xsd:string" minOccurs="0"/>
         <xsd:element name="DutyPaid" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies if the duty has been paid. Possible values are 'YES', 'NO', and 'NA'.When a Shipment
                  is being built from the order release, the DutyPaid flag on the Shipment will be determined
                  based on the following logic:- If any orders on the shipment have duty paid then the shipment
                  will be marked as duty paid.- If no orders have duty paid, then if any order is duty paid 'NO'
                  then the shipment will be valued as 'NO'.- If all orders are null or 'NA' then the shipment will
                  be valued as null or 'NA'.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OnRouteTempExec" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Y, N. Specifies if the shipment built from this order is assigned to a route instance leg.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsPreEnteredPU" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Y/N. Used to indicated if the order is a pre-entered pickup order.A pre-entered pickup order is
                  an order which has a pickup order movement existed for this order prior to this order is entered
                  into OTM.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AccessorialCodeGid" type="GLogXMLGidType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>
                  AccessorialCodeGid specifies something extra, such as a piece of equipment,required when
                  servicing a particular location.Examples: (AP = Air Pump, AD = Air Dryer, PU = pump).Validation:
                  References ACCESSORIAL_CODE table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ReleaseSpecialService" type="ReleaseSpecialServiceType" minOccurs="0"
                      maxOccurs="unbounded"/>
         <xsd:element name="ReleaseLegConstraint" type="ReleaseLegConstraintType" minOccurs="0"
                      maxOccurs="unbounded"/>
         <xsd:element name="OrderPriority" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the Priority Of the Order
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="EquipmentTypeGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  References EQUIPMENT_TYPE table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PickupRailCarrier" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="DeliveryRailCarrier" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="RailRouteCodeGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Uniquely identifies a RouteCode. References the ROUTE_CODE table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="EmergPhoneNum" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Emergency phone number when shipping Hazardous Materials. The 24 hours contact for the shipment.
                  DataType: A80
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="FlexFieldStrings" type="FlexFieldStringType" minOccurs="0"/>
         <xsd:element name="FlexFieldNumbers" type="FlexFieldNumberType" minOccurs="0"/>
         <xsd:element name="FlexFieldDates" type="FlexFieldDateType" minOccurs="0"/>
         <xsd:element name="FlexFieldCurrencies" type="FlexFieldCurrencyType" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ReleaseLineType">
      <xsd:annotation>
         <xsd:documentation>
            ReleaseLine specifies and item and quantity that is released.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ReleaseLineGid" type="GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>
                  ID of the release line.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PackagedItemRef" type="PackagedItemRefType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies packaged item information.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OrderBaseGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  OrderBaseGid is the gid for the Order Base.Validation: References Ob_Order_Base table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OrderBaseLineGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  OrderBaseLineGid is the gid for the Order Base Line.Validation: References Ob_Line table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="InitialItemGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  InitialItemGid can be used in lieu of specifying the PackagedItem, and allowing GC3 to
                  automatically assign the PackagedItem using the automated data assignment functionality based on
                  defined assignment rules.Validation: References ITEM table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ItemQuantity" type="ItemQuantityType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  ItemQuantity is a grouping of item related fields.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PackageDimensions" type="PackageDimensionsType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the package dimensions on the order release line that are used to override the
                  corresponding fields on the packaged item.For example, if the packaged item has dimensions of
                  3x3x3, but the height could vary, users can set the height for the order line.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ManufacturedCountryCode3Gid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the country where the product was manufactured. Validation: References COUNTRY_CODE
                  table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsDrawback" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Y, N. Specifies if item is drawback.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PackagedItemSpecRef" type="GLogXMLShipUnitSpecRefType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Used to reference a ship unit spec for the Item and the Packaging.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PackagedItemSpecCount" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the count of the PackagedItemSpec. For example, if the PackagedItemSpecRef referred to
                  CASE,then this count would correspond to the number of cases.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="NumLayersPerShipUnit" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Number of layers per ship unit.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="QuantityPerLayer" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Quantity per layer.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TransportHandlingUnitRef" type="GLogXMLShipUnitSpecRefType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the transport handling unit (ship unit spec).
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="BuyGeneralLedgerGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Buy side general ledger gid for order line.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SellGeneralLedgerGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Sell side general ledger gid for order line.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CommercialInvoiceGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  CommercialInvoiceGid uniquely identifies a commercial invoice in GC3.Validation: References
                  COMMERCIAL_INVOICE table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CommercialInvoiceDesc" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Commercial Invoice Description.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SecondaryWeightVolume" type="WeightVolumeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Secondary weight and volume.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ReleaseLineHazmatInfo" type="ReleaseLineHazmatInfoType" minOccurs="0"/>
         <xsd:element name="BilledQuantity" type="GLogXMLFlexQuantityType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Billed quantity.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PricePerUnit" type="PricePerUnitType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Price per unit.Note: The UOM is only valid when the PricePerUnit is in the ReleaseLine element.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TotalBilledAmount" type="GLogXMLFinancialAmountType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Total billed amount.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="FreeAlongSide" type="GLogXMLFinancialAmountType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Free Along Side is the cost of the materials and freight up to the point the goods are being
                  loaded onto the vessel.It never includes ocean freight. It would include the cost of the goods,
                  inland transportation, wharfage, warehouse,and packaging fees to get the goods to the point.
                  This value is used by the Census Department along with the Schedule Bnumber to keep track of the
                  value of goods that are being exported out of the country.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="BrandName" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Brand Name is used to provide specific brand information for the item. This is for informational
                  purposes only.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsSplitAllowed" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Y, N. Specifies if the order line may be split or not.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ShipUnitSpecProfileGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Uniquely identifies a ship unit specification profile. Validation: References
                  SHIP_UNIT_SPEC_PROFILE table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ItemAttributes" type="ItemAttributeType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>
                  Contains additional information about the the line item.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Refnum" type="RefnumType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>
                  Refnum is a reference number for an object. It consists of a qualifier and a value.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Remark" type="RemarkType" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element name="OrLineSpecialService" type="SpecialServiceType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>
                  Defines special services required on the Order Release Line
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="InvolvedParty" type="InvolvedPartyType" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element name="ReleaseLineIntlClassification" type="ReleaseLineIntlClassificationType" minOccurs="0"
                      maxOccurs="unbounded"/>
         <xsd:element name="Text" type="TextType" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element name="ReleaseLineAllocationInfo" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Specifies the allocation information for the order release line.
               </xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="ReleaseLineAllocByType" type="ReleaseAllocType" minOccurs="0"
                               maxOccurs="unbounded"/>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="OrLineEquipRefUnit" type="THUCapacityEquipRefUnitType" minOccurs="0"
                      maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>
                  Used to specify Ship Unit reference unit of a Order Release Line.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OrLinePackageRefUnit" type="THUCapacityPackageRefUnitType" minOccurs="0"
                      maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>
                  Used to specify Packaging Reference Unit of a Order Release Line.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="FlexFieldStrings" type="FlexFieldStringType" minOccurs="0"/>
         <xsd:element name="FlexFieldNumbers" type="FlexFieldNumberType" minOccurs="0"/>
         <xsd:element name="FlexFieldDates" type="FlexFieldDateType" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ReleaseLineIntlClassificationType">
      <xsd:annotation>
         <xsd:documentation>Specifies the international classification for the order release line.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="SequenceNumber" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Sequence number.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ImportExportType" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  When ImportExportType is within the ReleaseLineIntlClassification element,Possible values are
                  IMPORT or EXPORT. Default value is (null).
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="HTSGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  HTSGid. Validation: References HTS Table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PrimaryRQ" type="GLogXMLFlexQuantityType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Primary Reportable Quantity.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SecondaryRQ" type="GLogXMLFlexQuantityType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Secondary Reportable Quantity.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TertiaryRQ" type="GLogXMLFlexQuantityType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Tertiary Reportable Quantity.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="OrderRefnumType">
      <xsd:annotation>
         <xsd:documentation>An OrderRefnum is an alternate way of referring to a TransOrder.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="OrderRefnumQualifierGid" type="GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>
                  OrderRefnumQualifierGid is an element of an OrderRefnum which indicates the source of the
                  information.Example: Specify an xid of PO to indicate that the OrderRefnumValue is a purchase
                  order.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OrderRefnumValue" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>
                  OrderRefnumValue is the value portion of an order refnum.Example: Specify a purchase order
                  number.DataType: A30
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ReleaseInstructionType">
      <xsd:annotation>
         <xsd:documentation>ReleaseInstruction is used to identify the line item or ship unit and the quantity to
            release in the base order.
            It provides further instructions for creating order releases.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="OTMTransactionIn">
            <xsd:sequence>
               <xsd:choice minOccurs="0">
                  <xsd:element name="ReleaseInstructionGid" type="GLogXMLGidType" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation>
                           Global unique identifier for a release instruction.Validation: Reference to
                           OB_RELEASE_INSTRUCTION or OB_SU_RELEASE_INSTRUCTION table.
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="SequenceNumber" type="xsd:string" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation>
                           Sequence number.
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:choice>
               <xsd:element name="TransactionCode" type="TransactionCodeType" minOccurs="0"/>
               <xsd:element name="TransOrderGid" type="GLogXMLGidType">
                  <xsd:annotation>
                     <xsd:documentation>
                        Global unique identifier for a transportation order.Validation: Reference to
                        OB_ORDER_BASE table.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:choice>
                  <xsd:sequence>
                     <xsd:element name="TransOrderLineGid" type="GLogXMLGidType">
                        <xsd:annotation>
                           <xsd:documentation>
                              TransOrderLineGid uniquely identifies a TransOrderLine within the GLog
                              database.Validation: References OB_LINE.
                           </xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="QuantityToRelease">
                        <xsd:annotation>
                           <xsd:documentation>QuantityToRelease is used to indicate the quantity that should be
                              released.
                              It specifies either the weight, volume, or item count.
                           </xsd:documentation>
                        </xsd:annotation>
                        <xsd:complexType>
                           <xsd:choice>
                              <xsd:element name="Weight" type="WeightType"/>
                              <xsd:element name="Volume" type="VolumeType"/>
                              <xsd:element name="PackagedItemCount" type="xsd:string"/>
                           </xsd:choice>
                        </xsd:complexType>
                     </xsd:element>
                     <xsd:element name="LineRelInstInfo" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Specifies the TransOrderLine Release Instruction information. The
                              ShipUnitCount corresponds to the number of Ship Units for this Release
                              Instruction.
                           </xsd:documentation>
                        </xsd:annotation>
                        <xsd:complexType>
                           <xsd:sequence>
                              <xsd:element name="ShipUnitCount" type="xsd:string" minOccurs="0"/>
                           </xsd:sequence>
                        </xsd:complexType>
                     </xsd:element>
                  </xsd:sequence>
                  <xsd:sequence>
                     <xsd:element name="ShipUnitGid" type="GLogXMLGidType"/>
                     <xsd:element name="ShipUnitReleaseCount" type="xsd:string"/>
                     <xsd:element name="ShipUnitRelInstInfo" type="ShipUnitRelInstInfoType" minOccurs="0"/>
                  </xsd:sequence>
               </xsd:choice>
               <xsd:element name="TotalWeightVolume" type="GLogXMLWeightVolumeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (outbound) Total weight and volume of the order release.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="TransportHandlingUnitGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Specifies the transport handling unit (ship unit spec).
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="SourceLocationGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Identifies the source location Gid. References the LOCATION table.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="DestLocationGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Identifies the destination location Gid. References the LOCATION table.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="TimeWindow" type="TimeWindowType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        TimeWindow is a group of fields for specifying early/late pickup, early/late delivery,
                        and pickup/delivery appointments.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ReleaseDate" type="GLogDateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Specifies the order release date.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ItemTag1" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        ItemTag1 is one of four free form text fields for tagging an item for tracing
                        purposes.Examples: batch#, lot#, serial#, etc.DataType: A30
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ItemTag2" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        ItemTag2 is one of four free form text fields for tagging an item for tracing
                        purposes.Examples: batch#, lot#, serial#, etc.DataType: A30
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ItemTag3" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        ItemTag3 is one of four free form text fields for tagging an item for tracing
                        purposes.Examples: batch#, lot#, serial#, etc.DataType: A30
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ItemTag4" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        ItemTag4 is one of four free form text fields for tagging an item for tracing
                        purposes.Examples: batch#, lot#, serial#, etc.DataType: A30
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="OrderMovementType">
      <xsd:annotation>
         <xsd:documentation>
            An Order Movement represents part of the routing of an order. It is also a collection of ship units that
            are
            unplanned for certain part of a route.

            The ShipUnit in the OrderMovement correspond to the same ShipUnit object in the Shipment (i.e. the
            S_SHIP_UNIT table).
         </xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="OTMTransactionInOut">
            <xsd:sequence>
               <xsd:element name="IntSavedQuery" type="IntSavedQueryType" minOccurs="0"/>
               <xsd:element name="OrderMovementGid" type="GLogXMLGidType">
                  <xsd:annotation>
                     <xsd:documentation>
                        An OrderMovementGid uniquely identifies an Order Movement.Validation: Reference to
                        ORDER_MOVEMENT table.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="TransactionCode" type="TransactionCodeType"/>
               <xsd:element name="ReplaceChildren" type="ReplaceChildrenType" minOccurs="0"/>
               <xsd:element name="OrderReleaseGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Unique identifier for an Order Release. Validation: References ORDER_RELEASE table.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="IsTemporary" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Specifies if the object is temporary or not. Possible values are 'Y' or 'N'. Default
                        value is 'N'. Within the OrderMovement, it indicates whether this order movement can
                        coexist with shipments. If this flag is true,the order movement will be removed once the
                        order movement is planned on shipments. Otherwise the order movement'sstatus is set to
                        PLANNED-FINAL and stays in the system.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="CreationProcessType" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Specifies how the Order Movement came into existence. Possible values are 'USER
                        ENTERED', 'SYSTEM-ACTIONS',and 'SYSTEM-ITINERARY'.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="OrderPriority" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Specifies the Priority Of the Order
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="TotalShipUnitCount" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Indicates the total number of Ship Units in an order movement.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="TotalWeightVolume" type="GLogXMLWeightVolumeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (outbound) Total weight and volume of the order movement.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="SequenceNumber" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Sequence number.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Perspective" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        The perspective element is used to ensure that the order and shipment data shown to the
                        end user makes sense from that user's perspective or point of reference.Possible values
                        are B = Buy Side, and S = Sell Side.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ShipFromLocationRef" type="GLogXMLLocRefType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        ShipFromLocationRef specifies the ship-from location for an order movement.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="IsFixedSource" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Y, N. Specifies whether the source location on this order movement is the real source
                        location or is derived from the source location of its upstream shipments. Default value
                        is 'Y'.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="IsTemplate" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Y, N, specifies whether this order movement is a template or not.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ShipToLocationRef" type="GLogXMLLocRefType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        ShipToLocationRef specifies the ship-to location for an order movement.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="IsFixedDest" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Y, N. Specifies whether the destination location on this order movement is the real
                        destination location or is derived from the downstream shipments. Default value is 'Y'.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="EarlyPickupDt" type="GLogDateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        EarlyPickupDt is the earliest date/time for picking up.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="OperationEarlyPickupDt" type="GLogDateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        OperationEarlyPickupDate is calculated from upstream shipments' end time.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="LatePickupDt" type="GLogDateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        LatePickupDt is the latest date/time for pickup.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="EarlyDeliveryDt" type="GLogDateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        EarlyDeliveryDt is the earliest date/time for delivering.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="LateDeliveryDt" type="GLogDateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        LateDeliveryDt is the latest date/time for delivering.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="OperationLateDeliveryDt" type="GLogDateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        OperationLateDeliveryDate is calculated from downstream shipments' start time.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ReuseEquipment" type="xsd:string" minOccurs="0"/>
               <xsd:element name="ParentSourceLocGid" type="GLogXMLGidType" minOccurs="0"/>
               <xsd:element name="ParentDestLocGid" type="GLogXMLGidType" minOccurs="0"/>
               <xsd:element name="OrigLegGid" type="GLogXMLGidType" minOccurs="0"/>
               <xsd:element name="OrigLegPosition" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        The position of the original leg. Main leg's leg position is 0. Legs left to main leg
                        have positions -1, -2, etc. Legs right to the main leg have positions 1, 2, etc.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="BulkPlanGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        (outbound) A Gid that uniquely identifies a BulkPlan. References BULK_PLAN table.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="OrigSellOMGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Original Sell Order Movement Gid.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ShipUnitLengthWidthHeight" type="GLogXMLLWHType" minOccurs="0"/>
               <xsd:element name="ShipUnitDiameter" type="GLogXMLDiameterType" minOccurs="0"/>
               <xsd:element name="UserDefIconInfo" type="UserDefIconInfoType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Specifies the user defined icon info.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="TransportHandlingUnitGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Specifies the transport handling unit (ship unit spec).
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Indicator" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Indicator for Order Movement.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="PickupRoutingSeqGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Specifies the pickup routing sequence. Validation references the ROUTING_SEQUENCE table.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="DropoffRoutingSeqGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Specifies the dropoff routing sequence. Validation references the ROUTING_SEQUENCE
                        table.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="SourceTag" type="xsd:string" minOccurs="0"/>
               <xsd:element name="DestTag" type="xsd:string" minOccurs="0"/>
               <xsd:element name="ShipmentGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        ShipmentGid is a shipment global identifier.It uniquely identifies a shipment within the
                        OTM database.Validation: Reference to SHIPMENT table.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ExpectedTransitTime" type="GLogXMLDurationType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Specifies the expected time for this leg of the shipment to take.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ExpectedCost" type="GLogXMLFinancialAmountType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Specifies the expected cost for the order movement.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="CalculateContractedRate" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Y, N. Specifies whether the system should calculate the cost associated with a
                        particular leg. Default value is 'Y'.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="CalculateServiceTime" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Y, N. Specifies whether the system should calculate the service time for a particular
                        leg. Default value is 'Y'.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="RateOfferingGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        RateOfferingGid is a reference to a record in the RATE_OFFERING table,which specifies
                        contractual information for a particular rate offering.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="RateGeoGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        RateGeoGid uniquely identifies a RateGeo element.Validation: References RATE_GEO table.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="EquipmentGroupGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        EquipmentGroupGid is used to specify that a given equipment group is required for an
                        order movement. It is used to indicate the group an Equipment belongs. Reference to
                        EQUIPMENT_GROUP table.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="EquipmentGroupProfileGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        EquipmentGroupProfileGid is used to specify that one of a given set of equipment groups
                        is required.Validation: Reference to EQUIPMENT_GROUP_PROFILE table.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="TransportModeGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        TransportModeGid is used to indicate the mode of transportation for the
                        shipment.Validation: Reference to TRANSPORT_MODE table.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ModeProfileGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        ModeProfileGid is used to specify that one of a given set of transport modes is required
                        or provided.Validation: Reference to MODE_PROFILE table.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ServiceProviderGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        ServiceProviderGid is used to specify that a given service provider is
                        required.Validation: Reference to SERVPROV table.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ServiceProviderProfileGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        ServiceProviderProfileGid is used to specify that one of a given set of service
                        providers is required.Validation: Reference to SERVPROV_PROFILE table.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="RateServiceProfileGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Rate service must be compatible with this profile. Validation: Reference to
                        RATE_SERVICE_PROFILE table.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ShipWithGroup" type="xsd:string" minOccurs="0"/>
               <xsd:element name="LegClassificationGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        A LegClassificationGid uniquely identifies a Leg Classification.Validation: Reference to
                        LEG_CLASSIFICATION table.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="OrderMovementD" type="OrderMovementDType" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element name="Refnum" type="RefnumType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        Refnum is a reference number for an object. It consists of a qualifier and a value.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Remark" type="RemarkType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        Remarks on the order movement.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Status" type="StatusType" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element name="InvolvedParty" type="InvolvedPartyType" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element name="FlexFieldStrings" type="FlexFieldStringType" minOccurs="0"/>
               <xsd:element name="FlexFieldNumbers" type="FlexFieldNumberType" minOccurs="0"/>
               <xsd:element name="FlexFieldDates" type="FlexFieldDateType" minOccurs="0"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="OrderLineRefnumType">
      <xsd:annotation>
         <xsd:documentation>OrderLineRefnum is an alternate method for identifying an order line. It consists of a
            qualifier and a value.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="OrderLineRefnumQualifierGid" type="GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>
                  OrderLineRefnumQualifierGid is the qualifier portion of OrderLineRefnum.Validation: References
                  OB_LINE_REFNUM_QUAL table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OrderLineRefnumValue" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>
                  Value portion of OrderLineRefnum. Validation: References OB_LINE table, but only when qualifier
                  = 'GLOG'
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ReleaseLegConstraintType">
      <xsd:annotation>
         <xsd:documentation>Specifies Order Leg level constraints.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="LegClassificationGid" type="GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>
                  A LegClassificationGid uniquely identifies a Leg Classification.Validation: Reference to
                  LEG_CLASSIFICATION table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RateOfferingGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  RateOfferingGid is a reference to a record in the RATE_OFFERING table,which specifies
                  contractual information for a particular rate offering.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SellRateOfferingGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  SellRateOfferingGid is used to indicate the contractual information for a particular rate
                  offering for the sell-side perspective (e.g. used to create a bill for the
                  order/shipment)Validation: References RATE_OFFERING table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RateGeoGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  RateGeoGid uniquely identifies a RateGeo element.Validation: References RATE_GEO table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SellRateGeoGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  SellRateGeoGid is used to indicate a specific Rate Record within a Rate Offering for the
                  sell-side perspective.Validation: References RATE_GEO table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="EquipmentGroupGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  EquipmentGroupGid is used to specify that a given equipment group is required for the order
                  release leg.It is used to indicate the group an Equipment belongs.Reference to EQUIPMENT_GROUP
                  table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="EquipmentGroupProfileGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  EquipmentGroupProfileGid is used to specify that one of a given set of equipment groups is
                  required.Validation: Reference to EQUIPMENT_GROUP_PROFILE table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TransportModeGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  TransportModeGid is used to indicate the mode of transportation required for the order release
                  leg.Validation: Reference to TRANSPORT_MODE table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ModeProfileGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  ModeProfileGid is used to specify that one of a given set of transport modes is required or
                  provided.Validation: Reference to MODE_PROFILE table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ServiceProviderGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  ServiceProviderGid is used to specify that a given service provider is required.Validation:
                  Reference to SERVPROV table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ServiceProviderProfileGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  ServiceProviderProfileGid is used to specify that one of a given set of service providers is
                  required.Validation: Reference to SERVPROV_PROFILE table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RateServiceProfileGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Rate service must be compatible with this profile. Validation: Reference to RATE_SERVICE_PROFILE
                  table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ShipWithGroup" type="xsd:string" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="TransOrderStatusType">
      <xsd:annotation>
         <xsd:documentation>
            When the interface is to apply to order release status event, the StatusLevel element must be set to
            'RELEASE'
            otherwise the interface is assumed to apply to an order base status event.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="OTMTransactionIn">
            <xsd:sequence>
               <xsd:element name="SendReason" type="SendReasonType" minOccurs="0"/>
               <xsd:element name="ServiceProviderAlias" type="ServiceProviderAliasType" minOccurs="0"/>
               <xsd:choice>
                  <xsd:element name="IntSavedQuery" type="IntSavedQueryType"/>
                  <xsd:element name="TransOrderGid" type="GLogXMLGidType">
                     <xsd:annotation>
                        <xsd:documentation>
                           Global unique identifier for a transportation order.Validation: Reference to
                           OB_ORDER_BASE table.
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="ReleaseGid" type="GLogXMLGidType">
                     <xsd:annotation>
                        <xsd:documentation>
                           ReleaseGid uniquely identifies a release within the OTM database.Validation:
                           References ORDER_RELEASE table.
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:choice>
               <xsd:element name="Description" type="xsd:string" minOccurs="0"/>
               <xsd:element name="StatusLevel" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        StatusLevel specifies the status message as a shipment level, container level, stop
                        level, or order level message.When in the TransOrderStatus element, it must be set to
                        'RELEASE' when the event is an order release event.DataType: A50.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="OrderRefnum" type="OrderRefnumType" minOccurs="0"/>
               <xsd:element name="StatusCodeGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        StatusCodeGid is a standard EDI status code.Validation: References BS_STATUS_CODE table.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="StatusReasonCodeGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        StatusReasonCodeGid specifies the reason for sending the status message.Validation:
                        References BS_STATUS_REASON_CODE table when within the TransOrderStatus.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="TimeZoneGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        TimeZoneGid is the time zone global identifier. Validation: References TIME_ZONE table.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="EventDt" type="GLogDateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        EventDate indicates the date and time when an event occurred.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="SSContact" type="SSContactType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        SSContact is a structure containing various contact elements, such as Contact Function
                        Code, Contact Name, Remarks.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="SSRemarks" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        SSRemarks is free form text used by the carrier to enter data.DataType: A4000.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="EventGroup" type="EventGroupType" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element name="StatusGroup" type="StatusGroupType" minOccurs="0"/>
               <xsd:element name="ReasonGroup" type="ReasonGroupType" minOccurs="0"/>
               <xsd:element name="QuickCodeGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Quick Codes are available to speed entering the reason entry. A Quick Code can be setup
                        to point to a combination of responsible party, status group, status code, reason group,
                        and reason code.Validation: References BS_QUICK_CODE table.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ResponsiblePartyGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Specifies the responsible party for the event reason.Validation: References
                        BS_RESPONSIBLE_PARTY table.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ReportingUser" type="xsd:string" minOccurs="0"/>
               <xsd:element name="SLine" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>Refers to either a TransOrderLine or a ReleaseLine for the Order Status
                        event.
                     </xsd:documentation>
                  </xsd:annotation>
                  <xsd:complexType>
                     <xsd:sequence>
                        <xsd:element name="LineGid" type="GLogXMLGidType">
                           <xsd:annotation>
                              <xsd:documentation>
                                 LineGid uniquely identifies a TransOrderLine or ReleaseLine within the OTM
                                 database.Validation: References OB_LINE or ORDER_RELEASE_LINE.
                              </xsd:documentation>
                           </xsd:annotation>
                        </xsd:element>
                     </xsd:sequence>
                  </xsd:complexType>
               </xsd:element>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="ReleaseLineHazmatInfoType">
      <xsd:annotation>
         <xsd:documentation>Hazardous material information for the release line.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="IsHazardous" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  IsHazardous indicates if the object is hazardous or not. DataType: A1. Validation: Y,N Within
                  the ReleaseLineHazmatInfo element, an empty element will default to null.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="HazmatGenericGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Used to identify Hazmat generic information.References HAZMAT_GENERIC table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="HazmatItemGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Identifies a Hazmat item in the GC3 database.References HAZMAT_ITEM table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IdentificationNumber" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Hazardous cargo classification number.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ProperShippingName" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  The proper shipping name used for an item.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="HazardousClass" type="HazardousClassType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the Hazardous Classification.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PackagingGroup" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  PackingGroup indicates the degree of danger of the item or category.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SubsidiaryHazard" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Similar to HazardClass (class of hazardous material).
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="EmergencyResponseInfo" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Emergency response information.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PSASingaporeGroup" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  PSA Singapore Group.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="HazmatCommonInfo" type="HazmatCommonInfoType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Contains common hazardous materials information.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ERG" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Emergency Response Guidebook.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ERGAir" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Emergency Response Guidebook.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="EMS" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Emergency Medical Service.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="HazCompatGroup" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the hazamt compatibility group. Possible values are A,B,C,D,E,F,G,H,J,K,L,N,S.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="HazSpecialProvisions" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Indicates Dangerous Goods (DG) are being shipped under one of the special provisions listed in
                  the IATA Dangerous Goods Regulation Guide, section 4.4. There appear to be 146 different
                  provisions, and multiple provisions may apply to a single item in some cases.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="HazApprovalExemptionGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  PreferenceGid specifies a user preference.Validation: References PREFERENCE table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ImportLicenseInfoType">
      <xsd:annotation>
         <xsd:documentation>Specifies the Import License information.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="LicenseNum" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Government assigned identification number for an exporter or importer. DataType:A12.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="LicenseRequired" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies whether the license is required. Possible values are 'REQUIRED', 'NOT REQUIRED', 'MAY
                  BE REQUIRED'.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IssueDt" type="GLogDateTimeType" minOccurs="0"/>
         <xsd:element name="ExpireDt" type="GLogDateTimeType" minOccurs="0"/>
         <xsd:element name="ReceivedDt" type="GLogDateTimeType" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="OrStopType">
      <xsd:annotation>
         <xsd:documentation>Defines intermediate stops on an order</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="OrStopGid" type="GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>
                  The OrderRelease Stop Gid.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TransactionCode" type="TransactionCodeType" minOccurs="0"/>
         <xsd:element name="OrStopSeq" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>
                  The OR Stop Sequence number.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:choice>
            <xsd:element name="Location" type="LocationType"/>
            <xsd:element name="LocationGid" type="GLogXMLGidType">
               <xsd:annotation>
                  <xsd:documentation>
                     A LocationGid is a Gid that uniquely identifies a location.Validation: Reference to LOCATION
                     table.
                  </xsd:documentation>
               </xsd:annotation>
            </xsd:element>
         </xsd:choice>
         <xsd:element name="EarlyArrivalTime" type="GLogDateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  When IsAppointment is Y, this field is early appointment time.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="LateArrivalTime" type="GLogDateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  When IsAppointment is Y, this field is late appointment time.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsAppointment" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>
                  IsAppointment indicates if an appointment has been made at a given stop.Validation: Y,N
                  DataType: A1
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsAppointmentRequired" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>
                  IsAppointmentRequired is a boolean flag indicating if an appointment is required in order to
                  make the delivery at the destination locations.Specify 'Y' if an appointment is required, 'N'
                  otherwise.DataType: A1Validation: (Y,N)See Also: ZZSchemaIssues
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsApptConfirmRequired" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>
                  A boolean flag indicating if an appointment confirmation is required. DataType: A1, Validation:
                  (Y,N)
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsApptConfirm" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>
                  A boolean flag indicating if an appointment is confirmed. DataType: A1, Validation: (Y,N)
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="LegPosition" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Define where to insert this stop in a multi-stop situation . Validation: (L,F)
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Remark" type="RemarkType" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element name="OrStopSpecialService" type="OrStopSpecialServiceType" minOccurs="0"
                      maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="OrStopSpecialServiceType">
      <xsd:annotation>
         <xsd:documentation>Defines special services required on the Order Stop</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:choice>
            <xsd:element name="SpclService" type="SpclServiceType"/>
            <xsd:element name="SpecialServiceGid" type="GLogXMLGidType"/>
         </xsd:choice>
         <xsd:element name="Time" type="GLogXMLDurationType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Duration Time required.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ReleaseSpecialServiceType">
      <xsd:annotation>
         <xsd:documentation>The special services required for the order release against a base order. The special
            services differ from the accessories in that the customer does not pay for special services.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:choice>
            <xsd:element name="SpclService" type="SpclServiceType"/>
            <xsd:element name="SpecialServiceGid" type="GLogXMLGidType"/>
         </xsd:choice>
         <xsd:element name="Time" type="GLogXMLDurationType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Duration Time required.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ForSourceDest" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Define if this special service is required for source and/or destination location of
                  this order
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="OrderMovementDType">
      <xsd:annotation>
         <xsd:documentation>Order Movement Details. It contains all the s ship units in an order movement
         </xsd:documentation>
      </xsd:annotation>
      <xsd:choice>
         <xsd:element name="ShipUnitGid" type="GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>
                  ShipUnitGid is a unique identifier for a ShipUnit in the OTM database. Validation: References
                  S_SHIP_UNIT table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ShipUnit" type="ShipUnitType"/>
      </xsd:choice>
   </xsd:complexType>

   <xsd:complexType name="REquipmentType">
      <xsd:annotation>
         <xsd:documentation>REquipment specifies equipments associated with a Release.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="REquipmentSequence" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>
                  REquipmentSequence identifies a particular equipment associated with an order release.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IntSavedQuery" type="IntSavedQueryType" minOccurs="0"/>
         <xsd:element name="TransactionCode" type="TransactionCodeType" minOccurs="0"/>
         <xsd:element name="EquipmentGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  EquipmentGid uniquely identifies an equipment record in the OTM database.Validation: References
                  EQUIPMENT table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="EquipmentGroupGid" type="GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>
                  EquipmentGroupGid is used to indicate the group an Equipment belongs.Reference to
                  EQUIPMENT_GROUP table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="EquipmentTypeGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  References EQUIPMENT_TYPE table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="EquipmentInitial" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Number identifying the equipment used.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="EquipmentNumber" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  EquipmentNumber.DataType: A30. For Payment: DataType is A10.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="EquipmentInitialNumber" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (outbound) This is a concatenation of the EquipmentInitial and the EquipmentNumber. This element
                  is supported on the outbound only.When using in an inbound interface, the two separate fields
                  should be used. GC3 will concatenate to create this field.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="EquipmentSeal" type="EquipmentSealType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>
                  EquipmentSeal consists of a SealNumber.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ReleaseRefnumType">
      <xsd:annotation>
         <xsd:documentation>ReleaseRefnum is an alternate method for identifying a Release. It consists of a
            qualifier and a value.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ReleaseRefnumQualifierGid" type="GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>
                  ReleaseRefnumQualifierGid is the qualifier portion of ReleaseRefnum.Validation: References
                  ORDER_REFNUM_QUAL table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ReleaseRefnumValue" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>
                  Value portion of ReleaseRefnum. Validation: References ORDER_RELEASE table, but only when
                  qualifier = 'GLOG'
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="TransOrderLineDetailType">
      <xsd:annotation>
         <xsd:documentation>TransOrderLineDetail consists of one or more TransOrderLine elements.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="TransOrderLine" type="TransOrderLineType" maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ShipUnitDetailType">
      <xsd:annotation>
         <xsd:documentation>ShipUnitDetail is set of one or more ship units.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ShipUnit" type="ShipUnitType" maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="OBShipUnitType">
      <xsd:annotation>
         <xsd:documentation>This element is supported on the outbound only.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="OTMTransactionOut">
            <xsd:sequence>
               <xsd:element name="SendReason" type="SendReasonType" minOccurs="0"/>
               <xsd:element name="ShipUnit" type="ShipUnitType"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="OBLineType">
      <xsd:annotation>
         <xsd:documentation>This is used to send out the TransOrderLine information. It is initiated via
            notification.
            This element is supported on the outbound only.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="OTMTransactionOut">
            <xsd:sequence>
               <xsd:element name="SendReason" type="SendReasonType" minOccurs="0"/>
               <xsd:element name="TransOrderLine" type="TransOrderLineType"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="ReleaseServiceType">
      <xsd:annotation>
         <xsd:documentation>Specifies the services for the order release.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="CustomerServiceGid" type="GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>
                  Uniquely identifies a customer service within the OTM database.Validation: References
                  CUSTOMER_SERVICE table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ExecutedDt" type="GLogDateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the latest date when the service template is applied.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsPriorityOverwrite" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Y, N. Used to indicate that the customer service should be treated as the highest priority. The
                  value is reset to N after it is applied.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="OrderLegConstraintType">
      <xsd:sequence>
         <xsd:element name="LegClassificationGid" type="GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>
                  A LegClassificationGid uniquely identifies a Leg Classification.Validation: Reference to
                  LEG_CLASSIFICATION table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RateOfferingGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  RateOfferingGid is a reference to a record in the RATE_OFFERING table,which specifies
                  contractual information for a particular rate offering.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SellRateOfferingGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  SellRateOfferingGid is used to indicate the contractual information for aparticularrate offering
                  for the sell-side perspective (e.g. used to create a bill for the order/shipment)Validation:
                  References RATE_OFFERING table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RateGeoGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  RateGeoGid uniquely identifies a RateGeo element.Validation: References RATE_GEO table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SellRateGeoGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  SellRateGeoGid is used to indicate a specific Rate Record within a Rate Offering for the
                  sell-side perspective.Validation: References RATE_GEO table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="EquipmentGroupGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  EquipmentGroupGid is used to specify that a given equipment group is required for an TransOrder
                  leg.It is used to indicate the group an Equipment belongs.Reference to EQUIPMENT_GROUP table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="EquipmentGroupProfileGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  EquipmentGroupProfileGid is used to specify that one of a given set of equipment groups is
                  required.Validation: Reference to EQUIPMENT_GROUP_PROFILE table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TransportModeGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  TransportModeGid is used to indicate the mode of transportation for the leg.Validation:
                  Reference to TRANSPORT_MODE table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ModeProfileGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  ModeProfileGid is used to specify that one of a given set of transport modes is required or
                  provided.Validation: Reference to MODE_PROFILE table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ServiceProviderGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  ServiceProviderGid is used to specify that a given service provider is required.Validation:
                  Reference to SERVPROV table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ServiceProviderProfileGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  ServiceProviderProfileGid is used to specify that one of a given set of service providers is
                  required.Validation: Reference to SERVPROV_PROFILE table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RateServiceProfileGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Rate service must be compatible with this profile. Validation: Reference to RATE_SERVICE_PROFILE
                  table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ReleaseAllocType">
      <xsd:sequence>
         <xsd:element name="AllocTypeQualGid" type="GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>
                  Validation:References ALLOC_TYPE_QUAL table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ReleaseAllocShipment" type="ReleaseAllocShipmentType" minOccurs="0"
                      maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ReleaseAllocShipmentType">
      <xsd:annotation>
         <xsd:documentation>Specifies the total allocation for an order release on a particular shipment.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ShipmentGid" type="GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>
                  ShipmentGid is a shipment global identifier.It uniquely identifies a shipment within the OTM
                  database.Validation: Reference to SHIPMENT table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TotalAllocCost" type="GLogXMLFinancialAmountType">
            <xsd:annotation>
               <xsd:documentation>
                  Total allocated cost for this order or order line(PrivateCost + BaseCost).
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ReleaseAllocShipmentDetail" type="ReleaseAllocShipmentDetailType" minOccurs="0"
                      maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ReleaseAllocShipmentDetailType">
      <xsd:annotation>
         <xsd:documentation>Specifies the total allocation for an order release on a particular shipment.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Cost" type="GLogXMLFinancialAmountType">
            <xsd:annotation>
               <xsd:documentation>
                  The cost.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CostTypeGid" type="GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>
                  The Cost Type Gid. References the COST_TYPE table
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CostDescription" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Shows the cost type.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AccessorialCodeGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  AccessorialCodeGid specifies something extra, such as a piece of equipment,required when
                  servicing a particular location.Examples: (AP = Air Pump, AD = Air Dryer, PU = pump).Validation:
                  References ACCESSORIAL_CODE table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="GeneralLedgerGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Used to reference a General Ledger code.References GENERAL_LEDGER_CODE table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ShipUnitRelInstInfoType">
      <xsd:annotation>
         <xsd:documentation>Specifies the Ship Unit Release Instruction information.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="PackagedItemCount" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Indicates the number of packaged items.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Weight" type="WeightType" minOccurs="0"/>
         <xsd:element name="Volume" type="VolumeType" minOccurs="0"/>
         <xsd:element name="Length" type="LengthType" minOccurs="0"/>
         <xsd:element name="Width" type="WidthType" minOccurs="0"/>
         <xsd:element name="Height" type="HeightType" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="TransOrderLinkType">
      <xsd:annotation>
         <xsd:documentation>
            The PrevObjectGid and NextObjectGid refer to TransOrderGid(s).
         </xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="OTMTransactionIn">
            <xsd:sequence>
               <xsd:element name="TransactionCode" type="TransactionCodeType"/>
               <xsd:element name="PrevObjectGid" type="GLogXMLGidType">
                  <xsd:annotation>
                     <xsd:documentation>
                        Within the TransOrderLink, it specifies a reference to the Previous
                        TransOrder.References OB_ORDER_BASE table.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="NextObjectGid" type="GLogXMLGidType">
                  <xsd:annotation>
                     <xsd:documentation>
                        Within the TransOrderLink, it specifies a reference to the Next TransOrder.References
                        OB_ORDER_BASE table.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="LetterOfCreditInfoType">
      <xsd:annotation>
         <xsd:documentation>Letter of credit information.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="LetterOfCreditNum" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Used to transmit letter of credit details. DataType:A40 within the LetterofCredit element.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsLetterOfCreditRequired" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Y, N. Specifies if the Letter of Credit is required.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IssueDt" type="GLogDateTimeType" minOccurs="0"/>
         <xsd:element name="ExpireDt" type="GLogDateTimeType" minOccurs="0"/>
         <xsd:element name="ReceivedDt" type="GLogDateTimeType" minOccurs="0"/>
         <xsd:element name="LatestShippingDt" type="GLogDateTimeType" minOccurs="0"/>
         <xsd:element name="NegotiatedDays" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Negotiated days.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="LetterOfCreditAmount" type="GLogXMLFinancialAmountType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Letter of credit amount.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AdvisingNumber" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Advising number.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ConfirmingNumber" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Confirming number.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsLetterOfCreditStale" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Y, N. Specifies if the letter of credit is stale. Default value is 'N'.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="InspectionAndSurveyInfoType">
      <xsd:annotation>
         <xsd:documentation>Inspection and survey information.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="InspectionNumber" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Inspection number.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="InspectionRequired" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies if an inspection will be required. Possible values are 'REQUIRED', 'NOT REQUIRED', and
                  'MAY BE REQUIRED'.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="InspectionScheduleDt" type="GLogDateTimeType" minOccurs="0"/>
         <xsd:element name="InspectionDt" type="GLogDateTimeType" minOccurs="0"/>
         <xsd:element name="SurveyLayCanDt" type="GLogDateTimeType" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="CommercialInvoiceType">
      <xsd:annotation>
         <xsd:documentation>
            The Commercial Invoice is one of the most common documents used in international trade.
            Primarily, the document contains details of what, how much, and the cost of what is being shipped.
            The cost of the goods can vary depending on payment terms.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="CommercialInvoiceGid" type="GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>
                  CommercialInvoiceGid uniquely identifies a commercial invoice in GC3.Validation: References
                  COMMERCIAL_INVOICE table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TransactionCode" type="TransactionCodeType"/>
         <xsd:element name="CommercialInvoiceType" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies if the invoice is related to an order release or a job. This is used to indicate if
                  the involved parties are taken from the Order Release or the Job. Possible values are 'JOB' and
                  'ORDER'. Default value is 'ORDER'.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CommercialInvoiceNum" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Commercial invoice number.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="InvoiceDate" type="GLogDateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  InvoiceDate: Date the Invoice was sent.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ExchangeRateValue" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  ExchangeRateValue specifies the exchange rate between the CurrencyFrom and CurrencyTo
                  currencies.DataType: N12.7
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="GlobalCurrencyCode" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  GlobalCurrencyCode specifies the currency type for a FinancialAmount.Validation: Reference to
                  CURRENCY table.DataType: A30
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ExchangeRateInfo" type="ExchangeRateInfoType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the set of exchange rate information to use for currency conversions. This element is
                  supported on the outbound only, except in the CommercialInvoice.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CurrencyFrom" type="CurrencyCodeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  CurrencyFrom specifies a currency code from which we are converting.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsOverrideExchangeRate" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Y, N. Default value is N. Indicates a specific conversion rate for this commercial invoice is
                  entered in the exchange rate field and should be used.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ProductAmount" type="GLogXMLFinancialAmountType">
            <xsd:annotation>
               <xsd:documentation>
                  Cost of the goods shipped.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsCalcProductAmt" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Y, N. Specifies if the Product Amount should be calculated by GC3. The Product Amount is the sum
                  of all relatedorder release line Total Billed Amount field. The default value is N.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OtherCharge" type="GLogXMLFinancialAmountType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Costs other than than base cost (e.g. Accessorial costs)
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsCalcTotalInvoiceAmt" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Y, N. Specifies if the Total Invoice Amount should be calculated by GC3.The Total Invoice Amount
                  is the sum of theProduct Amount and the Other Charges Total. The default value is N.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TotalInvoiceAmount" type="GLogXMLFinancialAmountType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Total Invoice Amount.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CommercialTerms" type="CommercialTermsType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  CommercialTerms specifies how the freight charges for the TransOrder will be paid.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="FinalCommercialTerms" type="FinalCommercialTermsType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  FinalCommercialTerms specifies the Final Terms of Sale the Commercial Invoice is based on.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CommercialInvoiceTerms" type="CommercialInvoiceTermsType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the commercial terms of payment between the shipper and the shippers customer.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CommercialInvoiceCharge" type="CommercialInvoiceChargeType" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="CommercialInvoiceChargeType">
      <xsd:annotation>
         <xsd:documentation>Used to specify additional/other charges for the commercial invoice.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="SequenceNumber" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Sequence number.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ChargeAmount" type="GLogXMLFinancialAmountType">
            <xsd:annotation>
               <xsd:documentation>
                  Charge amount.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CommercialInvChargeCodeGid" type="GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the commercial invoice charge code. Validation: References COMMERCIAL_INV_CHARGE_CODE
                  table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ChargeActivity" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies how to handle the charge. Possible values are 'ADD', 'SUBTRACT', and 'IGNORE'.If the
                  Activity is ADD/SUBTRACT, the value will be treated as a +/- against the Total Other Charges.Use
                  IGNORE when you want to list the charge but not have it impact the Total Invoice Amount.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="QuoteType">
      <xsd:annotation>
         <xsd:documentation>
            A Quote is an estimate given to a potential customer to move goods.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="OTMTransactionInOut">
            <xsd:sequence>
               <xsd:element name="IntSavedQuery" type="IntSavedQueryType" minOccurs="0"/>
               <xsd:element name="QuoteGid" type="GLogXMLGidType">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) QuoteGid uniquely identifies a Quote within GC3.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="TransactionCode" type="TransactionCodeType"/>
               <xsd:element name="ReplaceChildren" type="ReplaceChildrenType" minOccurs="0"/>
               <xsd:element name="QuoteHdr" type="QuoteHdrType">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Quote Header
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="QuoteCostOption" type="QuoteCostOptionType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) QuoteCostOption provides information related to the quoted move, including cost breakdowns,mode(s),equipment,and remarks.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="QuoteEquipmentGroup" type="QuoteEquipmentGroupType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Equipment group to the cost option.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="InvolvedParty" type="InvolvedPartyType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) An InvolvedParty specifies a contact that is in some way involved with a TransOrder, Shipment, or Release.Either the ContactRef is required
                        to be specified, or the InvolvedPartyLocationRef is required and the contact will beselected as the primary contact for the location.
                        The LocationOverrideInfo element is only applicable when the InvolvedParty is in the Shipment.ShipmentHeader or the Release elements.
                        The TransactionCode in the InvolvedParty is only supported in the Driver interface in 6.0.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Refnum" type="RefnumType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Reference Number of the Quote.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Remark" type="RemarkType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Remark allows one to specify structured or unstructured remarks.
                        Structured remarks may have a RemarkSequence,RemarkQualifierGid, and RemarkText.
                        Unstructured remarks may have just RemarkText.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="QuoteShipUnit" type="QuoteShipUnitType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Quote shipping information.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Status" type="StatusType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Indicates the status type and value for a Quote.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Text" type="TextType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Both) Used to specify the use of a text template.On inbound, either TextTemplateGid or TextTemplate can be specified.
                        On outbound, only TextTemplateGid is provided.TextPatternOverride is inbound only.TextInstance is outbound only.
                        TextPatternOverride and TextItem are currently applicable only to Documents.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Location" type="LocationType" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>
                        (Outbound) Specifies the location details for the contacts (e.g. NotifyPartyContact and LiablePartyContact).
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="QuoteHdrType">
      <xsd:annotation>
         <xsd:documentation>
            The QuoteHdr contains general information about the quote.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="RequestDt" type="GLogDateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Quote request date.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IssueDt" type="GLogDateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Quote issue date.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ExpireDt" type="GLogDateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Quote expiry date.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OriginLocation" type="QuoteLocationType">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Source location.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DestLocation" type="QuoteLocationType">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Destination location.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsHazardous" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) IsHazardous indicates if the object is hazardous or not.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Perspective" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) The perspective element is used to ensure thatthe order and shipment data shown to the end user makes sense from thatuser's perspective or point of
                  reference.
                  Possible values are B = Buy Side, and S = Sell Side.For the RIQQuery element, a value of 'A' can be used to query for both Buy Side and Sell Side.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="QuoteOption" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Quote option.
                  Possible values: O:optimize, E:Equipment Groups and C:Equipment Group configuration.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TransportModeGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) TransportModeGid is used to indicate the mode of transportation for the shipment.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ServiceProviderGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) ServiceProviderGid is used to specify that a given service provider is required.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AvailableDt" type="GLogDateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Available Date.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DeliveryByDt" type="GLogDateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Delivery Date.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OriginSearchValue" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Origin search value.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DestSearchValue" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Destination search value.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsCustomerRatesOnly" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Specifies if it is a customer rates only.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ConsolidationTypeGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Uniquely identifies a user defined qualifier for orders and job types.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IncoTermGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Uniquely identifies a IncoTerm.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="FlexFieldStrings" type="FlexFieldStringType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Flex Field columns for VARCHAR2 data type.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="FlexFieldNumbers" type="FlexFieldNumberType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Flex Field columns for NUMBER data type.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="FlexFieldDates" type="FlexFieldDateType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Flex Field columns for DATE data type.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="QuoteCostOptionType">
      <xsd:annotation>
         <xsd:documentation>provides information related to the quoted move, including cost breakdowns,mode(s),equipment,and remarks.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="IntSavedQuery" type="IntSavedQueryType" minOccurs="0"/>
         <xsd:element name="SequenceNumber" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Sequence Number.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TransactionCode" type="TransactionCodeType" minOccurs="0"/>
         <xsd:element name="SellCostOption">
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="CostOption" type="CostOptionType">
                     <xsd:annotation>
                        <xsd:documentation>
                           (Both) Specifies possible cost options saved with the quote.
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="BuyCostOption">
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="CostOption" type="CostOptionType">
                     <xsd:annotation>
                        <xsd:documentation>
                           (Both) Specifies possible cost options saved with the quote.
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="CostOptionEquip" type="CostOptionEquipType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) The equipment the shipment is being transported on.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Remark" type="RemarkType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Remark allows one to specify structured or unstructured remarks.Structured remarks may have a RemarkSequence,RemarkQualifierGid, and
                  RemarkText.Unstructured remarks may have just RemarkText.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CostOptionShip" type="CostOptionShipType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Shipping options.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="CostOptionType">
      <xsd:annotation>
         <xsd:documentation>
            Specifies possible cost options saved with the quote.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="TransCost" type="GLogXMLFinancialAmountType">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) The total cost of the shipment split between the buy side and the sell side.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsLate" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Flag indicating if is late.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsFeasible" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Flag indicating if result is feasible or infeasible.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ItineraryGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) ItineraryGid uniquely identifies an Itinerary within GC3.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="NonTransCost" type="GLogXMLFinancialAmountType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Specifies the total non-transportation cost of the shipment split between the buy side and the sell side.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="QuoteEquipmentGroupType">
      <xsd:annotation>
         <xsd:documentation>
            Equipment group to the cost option.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="EquipmentGroupGid" type="GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) EquipmentGroupGid is used to specify that a given equipment group is required for an TransOrder or a Shipment.It is used to indicate the group an Equipment
                  belongs.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="EquipGroupCount" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) The Equipment Group Count element is used to specify number of equipment groups.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TransactionCode" type="TransactionCodeType" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="QuoteShipUnitType">
      <xsd:annotation>
         <xsd:documentation>
            Quote shipping information
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="IntSavedQuery" type="IntSavedQueryType" minOccurs="0"/>
         <xsd:element name="SequenceNumber" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Sequence Number of the Quote Ship Unit
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TransactionCode" type="TransactionCodeType" minOccurs="0"/>
         <xsd:element name="CommodityType" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Specifies commodity type.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ItemGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) ItemGid is a item global identifier, which uniquely identifies the itemwithin the GLog database.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="NMFCClassGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) TheNMFCClassGid specifies an NMFC class code.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="STCCGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) STCCGid is a Standard Transportation Commodity Code global identifier.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="STTCGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) STCCGid is a Standard Transportation Commodity Code global identifier.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="HTSGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) HTSGid is a Harmonized Tariff System global identifier.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="UserDefinedCommodityGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Unique identifier for User-defined Commodity.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="UDCClassListGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) User Defined Commodity Classification List specifies a list of user defined commodities for the item.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="WeightVolume" type="WeightVolumeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) WeightVolume is a group of elements for specifying weight and volume.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsSplitAllowed" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Specifies if the order line, order release, or ship unit may be split or not.Ignored if ship unit is though TransOrderWhen set to Y in the ReleaseHeader,
                  the order can be split based on the individual split settings on the ShipUnit(s).When set to N in the ReleaseHeader, the order cannot be split.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TransportHandlingUnitGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Specifies the transport handling unit (ship unit spec).
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ShipUnitCount" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) ShipUnitCount is the number of items in terms of the shipping units.Example: When shipping soda by the case, one couldspecify ShipUnitCount = 10
                  cases.Assuming 24 cans to a case, the corresponding FundamentalUnitCount would be 240.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Length" type="LengthType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Length contains sub-elements for length value and length unit of measure.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Width" type="WidthType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Width contains sub-elements for width unit of measture and value.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Height" type="HeightType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Height contains sub-elements for height unit of measure and value.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DeclaredValue" type="GLogXMLFinancialAmountType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) DeclaredValue is the declared value of a ship unit, item quanity, or release.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IATAScrCodeGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Uniquely identifes an IATA SCR Code.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CommodityGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Unique identifes Commodity
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ExportHTSGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Harmonized Tariff System global identifier.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="NmfcClassCode" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Used to specify NmfcClassCode for NmfcClass.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="CostOptionEquipType">
      <xsd:annotation>
         <xsd:documentation>
            The equipment the shipment is being transported on.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="IntSavedQuery" type="IntSavedQueryType" minOccurs="0"/>
         <xsd:element name="SequenceNumber" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Sequence number.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="EquipmentGroupGid" type="GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) EquipmentGroupGid is used to specify that a given equipment group is required for an TransOrder or a Shipment.It is used to indicate the group an Equipment
                  belongs.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="EquipGroupCount" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) The Equipment Group Count element is used to specify number of equipment groups.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TransactionCode" type="TransactionCodeType" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="CostOptionShipType">
      <xsd:annotation>
         <xsd:documentation>
            Shipping options.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="IntSavedQuery" type="IntSavedQueryType" minOccurs="0"/>
         <xsd:element name="SequenceNumber" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Sequence number.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TransactionCode" type="TransactionCodeType" minOccurs="0"/>
         <xsd:element name="IsPrimary" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Specifies if the Leg/Shipment is the primary. For a multi-leg Itinerary, the Shipment that is generated fromthe primary Leg will be designated as the
                  primary Shipment. When the Shipment XML is sent outbound, the referenceinformation (refnum, remark, etc.) from the primary Shipment are included.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TotalAllocCost" type="GLogXMLFinancialAmountType">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Total allocated cost for this order or order line(PrivateCost + BaseCost).
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="StartDt" type="GLogDateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Start Date/Start Time.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="EndDt" type="GLogDateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) End Date/End Time.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ServiceProviderGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) ServiceProviderGid is used to specify that a given service provider is required.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TransportModeGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) TransportModeGid is used to indicate the mode of transportation for the shipment.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RateOfferingGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) RateOfferingGid is a reference to a record in the RATE_OFFERING table,which specifies contractual information for a particular rate offering.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RateGeoGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) RateGeoGid uniquely identifies a RateGeo element.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="FlightGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) The FlightGid element uniquely identifies a given flight.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RateFromLocGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) The source location.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RateToLocGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) The destination location.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SrcViaLocGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Source via location.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DestViaLocGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Destination via location.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="VoyageGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) The VoyageGid element uniquely identifies a given voyage.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Distance" type="DistanceType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Distance is a structure for describing distance.It includes unit of measure and value.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Perspective" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) The perspective element is used to ensure that the order and shipment data shown to the end user makes sense from thatuser's perspective or point of
                  reference.
                  Possible values are B = Buy Side, and S = Sell Side.For the RIQQuery element, a value of 'A' can be used to query for both Buy Side and Sell Side.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ExpireDt" type="GLogDateTimeType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Expiry date.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SrcLoc" minOccurs="0">
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="QuoteLocInfo" type="QuoteLocInfoType">
                     <xsd:annotation>
                        <xsd:documentation>
                           (Both) If the location ids are not available, then for quoting purpose this element can be used to define the general shipping point.
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="LocationGid" type="GLogXMLGidType" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation>
                           (Both) A LocationGid is a Gid that uniquely identifies a location.
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="SourceLocationName" type="xsd:string" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation>
                           (Both) Sorce location name.
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="DestLoc" minOccurs="0">
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="QuoteLocInfo" type="QuoteLocInfoType">
                     <xsd:annotation>
                        <xsd:documentation>
                           (Both) If the location ids are not available, then for quoting purpose this element can be used to define the general shipping point.
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="LocationGid" type="GLogXMLGidType" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation>
                           (Both) A LocationGid is a Gid that uniquely identifies a location.
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="DestLocationName" type="xsd:string" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation>
                           (Both) Destination location name.
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="ShipmentTypeGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Used to specify the type of shipment.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="NFRCRuleGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Uniquely identifies the NFRC Rule.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CostOptionShipCost" type="CostOptionShipCostType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Shipping cost.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="CostOptionShipCostType">
      <xsd:annotation>
         <xsd:documentation>
            Shipping cost.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="IntSavedQuery" type="IntSavedQueryType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Integration Saved Queries provide a means to query for Gid(s) using a combination of SQL and XPathexpressions referencing information in the XML.
                  A basic example of an Integration Saved Query thatuses the ShipUnit Content Tracking Tag number in the XML to lookup the Gid in the database
                  would resemble the following:select distinct s_ship_unit_gid from s_ship_unit_line where tracking_tag1 = '{ShipUnit/ShipUnitContent[1]/ItemQuantity/ItemTag1}'The
                  Xpath expression in the braces,
                  which referes to the ItemQuantity/ItemTag element in the firstShipUnitContent element of the ShipUnit, will be replaced with the value in the XML document
                  beforethe query is initiated.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SequenceNumber" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Sequence number.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CostType" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Indicates the type of cost. Possible values include the following:B (Base), A (Accessorial), O (Other), D (Discount), S (Stopoff), and C (Circuity).When
                  the CostType is within the ShipmentCost element, a value of L (Delta) is also supported.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AccessorialCodeGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) AccessorialCodeGid specifies something extra, such as a piece of equipment,required when servicing a particular location.Examples: (AP = Air Pump, AD = Air
                  Dryer, PU = pump).
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Cost" type="GLogXMLFinancialAmountType">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) The cost.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Description" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Description is a free form text string.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Explanation" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Explanation.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PaymentMethodCodeGid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) PaymentMethodCodeGid indicates how freight will be paid for. Examples: CC = Collect, CD = Collect on Delivery, MX = Mixed, NC = Service Freight no
                  Charge,PP = Prepaid, TP = ThirdParty
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TransactionCode" type="TransactionCodeType" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="QuoteLocationType">
      <xsd:choice>
         <xsd:element name="LocationGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:sequence minOccurs="0">
            <xsd:element name="Type" type="xsd:string"/>
            <xsd:element name="QuoteLocInfo" type="QuoteLocInfoType"/>
            <xsd:element name="ViaLocation" type="GLogXMLLocGidType" minOccurs="0"/>
         </xsd:sequence>
      </xsd:choice>
   </xsd:complexType>

   <xsd:complexType name="QuoteLocInfoType">
      <xsd:annotation>
         <xsd:documentation>
            If the location ids are not available, then for quoting purpose this element can be used to define the general shipping point.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="AddressLines" type="AddressLinesType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Contains the sequence number and address line.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="City" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) City.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Province" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) Province is a long description corresponding to ProvinceCode.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CountryCode3Gid" type="GLogXMLGidType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) CountryCode3Gid is the three character ISO country code global identifier.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PostalCode" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Both) ProvinceCode is a two character province code.In the United States, ProviceCode corresponds to a two character state code such as PA.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

</xsd:schema>