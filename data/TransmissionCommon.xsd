<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns="http://xmlns.oracle.com/apps/otm/transmission/v6.4" attributeFormDefault="unqualified" elementFormDefault="qualified"
            targetNamespace="http://xmlns.oracle.com/apps/otm/transmission/v6.4" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

   <xsd:annotation>
      <xsd:documentation>
         Release 6.4.2 Transmission Common structures Schema. Last modified Nov, 2016.
         This schema will contain all element, attribute and types definitions common to all Transmission XML schemas.
      </xsd:documentation>
   </xsd:annotation>

   <xsd:complexType name="TransmissionDocumentType" abstract="true">
      <xsd:annotation>
         <xsd:documentation>
            This type is essentially a 'marker' type used to identify the top level XML documents that can be sent to, or received
            from OTM and GTM.
         </xsd:documentation>
      </xsd:annotation>
   </xsd:complexType>

   <xsd:complexType name="TransmissionReportType">
      <xsd:annotation>
         <xsd:documentation>
            TransmissionReport consists of a TransmissionNo followed by 0 or more IntegrationLogMessage elements,
            followed by a TransmissionSummary element.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="OTMTransactionInOut">
            <xsd:sequence>
               <xsd:element name="UserName" type="xsd:string" minOccurs="0"/>
               <xsd:element name="Password" type="PasswordType" minOccurs="0"/>
               <xsd:element name="SendReason" type="SendReasonType" minOccurs="0"/>
               <xsd:element name="TransmissionNo" type="xsd:string"/>
               <xsd:element name="SenderTransmissionNo" type="xsd:string" minOccurs="0"/>
               <xsd:element name="ReportStatus" type="xsd:string" minOccurs="0"/>
               <xsd:element name="Refnum" type="RefnumType" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element name="IntegrationLogMessage" type="IntegrationLogMessageType" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element name="TransmissionSummary" type="TransmissionSummaryType" minOccurs="0"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="TransactionReportType">
      <xsd:annotation>
         <xsd:documentation>A TransactionReport contains a transaction number followed by 0 or more integration log messages.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ITransactionNo" type="xsd:string"/>
         <xsd:element name="IntegrationLogMessage" type="IntegrationLogMessageType" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="TransmissionSummaryType">
      <xsd:annotation>
         <xsd:documentation>TransmissionSummary is the summary component of a TransmissionReport.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="TransmissionNo" type="xsd:string" minOccurs="0"/>
         <xsd:element name="FirstTransactionNo" type="xsd:string" minOccurs="0"/>
         <xsd:element name="LastTransactionNo" type="xsd:string" minOccurs="0"/>
         <xsd:element name="ApplicationReport" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>
                  The ApplicationReport summarizes the activity associated with a given GLog integration application
                  during the processing of a given transmission.
               </xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="ApplicationName" type="xsd:string"/>
                  <xsd:element name="InsertCount" type="xsd:string"/>
                  <xsd:element name="UpdateCount" type="xsd:string"/>
                  <xsd:element name="InsertUpdateCount" type="xsd:string"/>
                  <xsd:element name="DeleteCount" type="xsd:string"/>
                  <xsd:element name="ErrorCount" type="xsd:string"/>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="GLogXMLElementType">
      <xsd:annotation>
         <xsd:documentation>
            A GLogXMLElement represents an OTM or GTM Integration transaction.
            The GLogXMLTransaction element is used a Substitution Group head element. All valid Transaction interface elements will
            declare that they can substitute for the GLogXMLTransaction. For example, the TransOrder interface is a valid
            inbound and outbound transaction and can be used in place of the GLogXMLTransaction element:

            GLogXMLElement start tag
               TransactionHeader (optional)
               TransOrder
            GLogXMLElement end tag

         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="TransactionHeader" type="TransactionHeaderType" minOccurs="0"/>
         <xsd:element ref="GLogXMLTransaction"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:element name="GLogXMLTransaction" type="GLogXMLTransactionType" abstract="true"/>

   <xsd:complexType name="GLogXMLTransactionType" abstract="true">
      <xsd:complexContent>
         <xsd:extension base="TransmissionDocumentType"/>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="OTMTransactionIn" abstract="true">
      <xsd:complexContent>
         <xsd:extension base="GLogXMLTransactionType"/>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="OTMTransactionOut" abstract="true">
      <xsd:complexContent>
         <xsd:extension base="GLogXMLTransactionType"/>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="OTMTransactionInOut" abstract="true">
      <xsd:complexContent>
         <xsd:extension base="GLogXMLTransactionType"/>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="TransactionHeaderType">
      <xsd:annotation>
         <xsd:documentation>
            TransactionHeader contains the header level information for the Transaction (GLogXMLElement).
            The TimeZoneGid is used as an overide for the times specified in that Transaction.
            Typically, the times are assumed to be in the time zone of the corresponding location.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="SenderTransactionId" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Specifies the sender transaction id. Used as a reference for the Transaction (GLogXMLElement).
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ProcessInfo" type="ProcessInfoType" minOccurs="0"/>
         <xsd:element name="TimeZoneGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="ObjectModInfo" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  (Outbound) Specifies the attributes for when the object was created of updated.
               </xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="InsertDt" type="GLogDateTimeType" minOccurs="0"/>
                  <xsd:element name="UpdateDt" type="GLogDateTimeType" minOccurs="0"/>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="SendReason" type="SendReasonType" minOccurs="0"/>
         <xsd:element name="Refnum" type="RefnumType" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ProcessInfoType">
      <xsd:annotation>
         <xsd:documentation>
            ProcessInfo specifies information pertaining to the processing of GLogXMLElements.
            Refer to the comments for the individual elements for more information.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ProcessGroup" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  Name used in defining a group of transactions for processing. Each group can be processed sequentially or in parallel.
                  If not specified, it defaults to the i_transmission_no assigned to the GLogXMLElement when it is persisted to the
                  i_transaction table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ProcessSequence" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
                  The ProcessSequence defaults to the i_transaction_no assigned to the GLogXMLElement when it is persisted to the
                  i_transaction table. You may override this default value if desired.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="GLogDateTimeType">
      <xsd:sequence>
         <xsd:element name="GLogDate" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>GLogDates are of the format YYYYMMDDHHMMSS.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TZId" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Time Zone ID.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TZOffset" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Time Zone Offset.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="GLogXMLGidType">
      <xsd:sequence>
         <xsd:element name="Gid" type="GidType"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="GidType">
      <xsd:annotation>
         <xsd:documentation>Gid stands for global identifier.
            Components of a gid include the DomainName, Xid, and XidQualifier.
            The DomainName and Xid are combined to form a globally unique identifier.
            Examples: Given a domain name of ROSENBLOOMS.COM, and an xid of MYORDER,
            the gid would be formed as ROSENBLOOMS.COM.MYORDER.
            Given a domain name of PUBLIC and an xid of PORT, the gid would be just PORT.
            Thus, when referring to PUBLIC data, the domain name may be omitted.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="DomainName" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>The DomainName is combined with the Xid to form a globally unique identifier.
                  DataType: A50
                  Validation: Must be a DomainName that has been previously registered with glog.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Xid" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>The Xid is combined with a DomainName to form a globally unique identifier.
                  DataType: A50
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="UOMContextType">
      <xsd:sequence>
         <xsd:element name="UOMType" type="xsd:string" minOccurs="0"/>
         <xsd:element name="UOM" type="xsd:string" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="GLogXMLLengthType">
      <xsd:sequence>
         <xsd:element name="Length" type="LengthType"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="LengthType">
      <xsd:annotation>
         <xsd:documentation>Length contains sub-elements for length value and length unit of measure.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="LengthValue" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>LengthValue is a number specifying length in a particular unit of measure.</xsd:documentation>
               <xsd:documentation>DataType: N10.2</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="LengthUOMGid" type="GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>LengthUOMGid is the length unit of measure global identifier.
                  Validation: References UOM table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="GLogXMLWidthType">
      <xsd:sequence>
         <xsd:element name="Width" type="WidthType"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="WidthType">
      <xsd:annotation>
         <xsd:documentation>Width contains sub-elements for width unit of measture and value.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="WidthValue" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>WeigthValue is a number specifying width in a particular unit of measure.
                  DataType: N10.2
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="WidthUOMGid" type="GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>WidthUOMGid is the width unit of measure global identifier.
                  Validation: References UOM table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="GLogXMLHeightType">
      <xsd:sequence>
         <xsd:element name="Height" type="HeightType"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="HeightType">
      <xsd:annotation>
         <xsd:documentation>Height contains sub-elements for height unit of measure and value.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="HeightValue" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>HeightValue is a number specifying height in a particular unit of measure.
                  DataType: N10.2
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="HeightUOMGid" type="GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>HeightUOMGid is the height unit of measure global identifier.
                  Validation: References UOM table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="GLogXMLLWHType">
      <xsd:sequence>
         <xsd:element name="LengthWidthHeight" type="LengthWidthHeightType"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="LengthWidthHeightType">
      <xsd:sequence>
         <xsd:element name="Length" type="LengthType"/>
         <xsd:element name="Width" type="WidthType"/>
         <xsd:element name="Height" type="HeightType"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="GLogXMLWeightVolumeType">
      <xsd:sequence>
         <xsd:element name="WeightVolume" type="WeightVolumeType"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="WeightVolumeType">
      <xsd:annotation>
         <xsd:documentation>WeightVolume is a group of elements for specifying weight and volume.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Weight" type="WeightType"/>
         <xsd:element name="Volume" type="VolumeType"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="GLogXMLWeightType">
      <xsd:sequence>
         <xsd:element name="Weight" type="WeightType"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="WeightType">
      <xsd:annotation>
         <xsd:documentation>Weight is a group of fields for specifying weight.
            It includes sub-elements for weight value and unit of measure.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="WeightValue" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>WeightValue is a number specifying how much something weights in a particular unit of measure.
                  DataType: N10.2
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="WeightUOMGid" type="GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>WeightUOMGid is the weight unit of measture global identifier.
                  It specifies the unit of measure for the WeightValue element.
                  Validation: References UOM table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="GLogXMLVolumeType">
      <xsd:sequence>
         <xsd:element name="Volume" type="VolumeType"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="VolumeType">
      <xsd:annotation>
         <xsd:documentation>Volume is a group of fields for specifying volume.
            It includes sub-elements for specifying unit of measure and a volume value.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="VolumeValue" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>VolumeValue is a number specifying how much space something fills in a particular unit of measure.
                  DataType: N10.2
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="VolumeUOMGid" type="GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>VolumeUOMGid is the volume unit of measure global identifier.
                  Validation: References UOM table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="GLogXMLDurationType">
      <xsd:sequence>
         <xsd:element name="Duration" type="DurationType"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="DurationType">
      <xsd:annotation>
         <xsd:documentation>Duration is the amount of time spent doing an activity, such as a stop.
            Includes a duration value and unit of measure.
            Example: To specify 30 seconds you would specify DurationValue=30 and DurationUOMGid=S.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="DurationValue" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>DurationValue is an amount of time in a particular unit of measure.
                  DataType: N10.2
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DurationUOMGid" type="GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>DurationUOMGid stands for duration unit of measure global identifier.
                  Validation: References UOM table.
                  Example: xid = S for seconds.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="GLogXMLDistanceType">
      <xsd:sequence>
         <xsd:element name="Distance" type="DistanceType"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="DistanceType">
      <xsd:annotation>
         <xsd:documentation>Distance is a structure for describing distance.
            It includes unit of measure and value.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="DistanceValue" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>DistanceValue is the value component of Distance element.
                  DataType: N10.2
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DistanceUOMGid" type="GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>DistanceUOMGid is the distance unit of measure global identifier.
                  Validation: References UOM table. Example: MI = miles.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="GLogXMLTemperatureType">
      <xsd:sequence>
         <xsd:element name="Temperature" type="TemperatureType"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="TemperatureType">
      <xsd:annotation>
         <xsd:documentation>Temperature is a structure for specifying temperature in a particular unit of measure.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="TemperatureValue" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>TemperatureValue specifies the value component of temperature.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TemperatureUOMGid" type="GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>TemperatureUOMGid stands for temperature unit of measure global identifier.
                  Its used to specify the unit of measure for an associated temperature value.
                  Validation: References UOM table where type = TEMPERATURE.
                  Example: C = celcius, K = Kelvin, F = Farenheit
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="GLogXMLDiameterType">
      <xsd:sequence>
         <xsd:element name="Diameter" type="DiameterType"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="DiameterType">
      <xsd:annotation>
         <xsd:documentation>Specifies the diameter. It includes sub-elements for diameter value and unit of measure.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="DiameterValue" type="xsd:string"/>
         <xsd:element name="UOMGid" type="GLogXMLGidType"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="GLogXMLFinancialAmountType">
      <xsd:sequence>
         <xsd:element name="FinancialAmount" type="FinancialAmountType"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="FinancialAmountType">
      <xsd:annotation>
         <xsd:documentation>FinancialAmount includes a currency code and an amount of money in that currency.
            Note: Functional currency will only be set for a domain if and when the exchange rate gid/date is set.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="GlobalCurrencyCode" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>GlobalCurrencyCode specifies the currency type for a FinancialAmount.
                  Validation: Reference to CURRENCY table.
                  DataType: A30
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="MonetaryAmount" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>MonetaryAmount is an amount of money in a given currency.
                  DataType: N10.2
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RateToBase" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Specifies the exchange rate used to convert the financial amount to the GC3 base amount. The element is supported on the outbound only.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="FuncCurrencyCode" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>A functional currency gid selected for the domain setting. References the CURRENCY table</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="FuncCurrencyAmount" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>The functional currency amount.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="CurrencyCodeType">
      <xsd:sequence>
         <xsd:element name="GlobalCurrencyCode" type="xsd:string"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="GLogXMLFlexQuantityType">
      <xsd:sequence>
         <xsd:element name="FlexQuantity" type="FlexQuantityType"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="FlexQuantityType">
      <xsd:annotation>
         <xsd:documentation>Used to indicate the quantity.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="UOM" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Unit of measure.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="UOMType" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Unit of Measure type.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Quantity" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Quantity value.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="GLogXMLLocProfileType">
      <xsd:sequence>
         <xsd:element name="LocationProfileGid" type="GLogXMLGidType"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="GLogXMLLocRoleType">
      <xsd:sequence>
         <xsd:element name="LocationRoleGid" type="GLogXMLGidType"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="GLogXMLRegionGidType">
      <xsd:sequence>
         <xsd:element name="RegionGid" type="GLogXMLGidType"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ExchangeRateInfoType">
      <xsd:annotation>
         <xsd:documentation>Specifies the set of exchange rate information to use for currency conversions. This element is supported on the outbound only, except in the
            CommercialInvoice.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ExchangeRateGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="ExchangeRateDt" type="GLogDateTimeType" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="CommercialTermsType">
      <xsd:annotation>
         <xsd:documentation>CommercialTerms specifies how the freight charges for the TransOrder will be paid.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="PaymentMethodCodeGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="IncoTermGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="TermLocationText" type="xsd:string" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="PricePerUnitType">
      <xsd:annotation>
         <xsd:documentation>Price per unit.
            Note: The UOM is only valid when the PricePerUnit is in the ReleaseLine element.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="FinancialAmount" type="FinancialAmountType"/>
         <xsd:element name="UOM" type="xsd:string" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="CommercialInvoiceTermsType">
      <xsd:sequence>
         <xsd:element name="PaymentMethodCodeGid" type="GLogXMLGidType" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="PasswordType">
      <xsd:annotation>
         <xsd:documentation>GLog Password.
            DataType: A128
