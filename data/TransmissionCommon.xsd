            <xsd:annotation>
               <xsd:documentation>FSACCode is an element of Station which specifies the Freight Station
                  Accounting Code of the rail carrier. DataType:A5.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="City" type="xsd:string"/>
         <xsd:element name="ProvinceCode" type="xsd:string" minOccurs="0"/>
         <xsd:element name="CountryCode3Gid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="PostalCode" type="xsd:string" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="SEquipmentGidQueryType">
      <xsd:annotation>
         <xsd:documentation>Used to query for SEquipmentGid to assign the Shipment.ShipUnit.</xsd:documentation>
      </xsd:annotation>
      <xsd:choice>
         <xsd:element name="SEquipGidMatchOption" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Specifies the logic to use for selecting the SEquipment on the Shipment. Currently support the following values:
                  Any - Select any of the original SEquipment on the Shipment
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IntSavedQuery" type="IntSavedQueryType"/>
      </xsd:choice>
   </xsd:complexType>

   <xsd:complexType name="StopRefnumType">
      <xsd:annotation>
         <xsd:documentation>StopRefnum is a qualifier/value pair used to refer to a particular stop.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="StopRefnumQualifierGid" type="GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>StopRefnumQualifierGid is a component of a StopRefnum used to qualify the
                  associated StopRefnumValue. Validation:References INVOICE_STOP_REFNUM_QUAL table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="StopRefnumValue" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>StopRefnumValue is the value portion of a StopRefnum. DataType:A30.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ServiceProviderAliasType">
      <xsd:annotation>
         <xsd:documentation>ServiceProviderAlias is an alternate identifier for a service provider.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ServiceProviderAliasQualifierGid" type="GLogXMLGidType">
            <xsd:annotation>
               <xsd:documentation>ServiceProviderAliasQualifierGid indicates the type of value in the ServiceProviderAliasValue element.
                  Validation: References SERVPROV_ALIAS_QUAL table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ServiceProviderAliasValue" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>ServiceProviderAliasValue is an alternate identifier for a service provider.
                  DataType: A101.
                  Validation: References SERVPROV table, but only if when the qualifier is 'GLOG'.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="TimeWindowType">
      <xsd:annotation>
         <xsd:documentation>TimeWindow is a group of fields for specifying early/late pickup, early/late delivery, and pickup/delivery appointments.
            The PickupIsAppt and DeliveryIsAppt elements are only applicable when the TimeWindow is in the TransOrderLine,
            TransOrder.ShipUnitDetail.ShipUnit, and Release elements.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="EarlyPickupDt" type="GLogDateTimeType" minOccurs="0"/>
         <xsd:element name="LatePickupDt" type="GLogDateTimeType" minOccurs="0"/>
         <xsd:element name="EarlyDeliveryDt" type="GLogDateTimeType" minOccurs="0"/>
         <xsd:element name="LateDeliveryDt" type="GLogDateTimeType" minOccurs="0"/>
         <xsd:element name="PickupIsAppt" type="xsd:string" minOccurs="0"/>
         <xsd:element name="DeliveryIsAppt" type="xsd:string" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ItemQuantityType">
      <xsd:annotation>
         <xsd:documentation>ItemQuantity is a grouping of item related fields.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ItemTag1" type="xsd:string" minOccurs="0"/>
         <xsd:element name="ItemTag2" type="xsd:string" minOccurs="0"/>
         <xsd:element name="ItemTag3" type="xsd:string" minOccurs="0"/>
         <xsd:element name="ItemTag4" type="xsd:string" minOccurs="0"/>
         <xsd:element name="IsShippable" type="xsd:string" minOccurs="0"/>
         <xsd:element name="IsSplitAllowed" type="xsd:string" minOccurs="0"/>
         <xsd:element name="WeightVolume" type="WeightVolumeType" minOccurs="0"/>
         <xsd:element name="PackagedItemCount" type="xsd:string" minOccurs="0"/>
         <xsd:element name="DeclaredValue" type="GLogXMLFinancialAmountType" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="UserDefIconInfoType">
      <xsd:annotation>
         <xsd:documentation>Specifies the user defined icon info. Validation: References ICON table.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="UserDef1IconGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="UserDef2IconGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="UserDef3IconGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="UserDef4IconGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="UserDef5IconGid" type="GLogXMLGidType" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="SpecialServiceType">
      <xsd:choice>
         <xsd:element name="SpclService" type="SpclServiceType"/>
         <xsd:element name="SpecialServiceGid" type="GLogXMLGidType"/>
      </xsd:choice>
   </xsd:complexType>

   <xsd:complexType name="SpclServiceType">
      <xsd:annotation>
         <xsd:documentation>Indicates the special services required to make a delivery. Used to deprecate SpecialService element</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="IntSavedQuery" type="IntSavedQueryType" minOccurs="0"/>
         <xsd:element name="SpecialServiceGid" type="GLogXMLGidType"/>
         <xsd:element name="TransactionCode" type="TransactionCodeType"/>
         <xsd:element name="SpecialServiceDesc" type="xsd:string" minOccurs="0"/>
         <xsd:element name="SpclServiceGroupGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="ModeProfileGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="IsArbitrary" type="xsd:string" minOccurs="0"/>
         <xsd:element name="IsForDriverChk" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Designates this special service is to be checked during driver assignment.Default : N</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsForEquipChk" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Designates this special service is to be checked during equipment assignment. Default : N</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsForPUChk" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Designates this special service is to be checked during power unit assignment. Default : N</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsForRating" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Designates this special service is to be used for rating. Default : N</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PULevel" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Designates this special service is to apply at the power unit or power unit type level.Validation : POWER_UNIT,POWER_UNIT_TYPE
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DriverLevel" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Designates this special service is to apply at the driver or driver type level.Validation : DRIVER,DRIVER_TYPE</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="EquipLevel" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Designates this special service is to apply at the equipment or equipment type level.Validation : EQUIPMENT,EQUIPMENT_TYPE
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ForSourceDest" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Define if this special service is required for source and/or destination location of this order</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ActivityTypeGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="SequenceNumber" type="xsd:string" minOccurs="0"/>
         <xsd:element name="IsTimeBased" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Specify if planning needs to get/calculate the time duration for this special service. Default : N</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IsIgnorable" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Specify if this special service will be ignored. Default : N</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PayableIndicatorGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="BillableIndicatorGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="DriverTimeType" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Indicates the type of time associated with this special service or does not count as driver time (when the value is null).Validation: DRIVER_WORK,
                  DRIVER_DRIVE,
                  DRIVER_REST, DRIVER_WAIT
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="FlexFieldStringType">
      <xsd:annotation>
         <xsd:documentation>Common type for tables which contain Flex Field columns for VARCHAR2 data type</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Attribute1" type="xsd:string" minOccurs="0"/>
         <xsd:element name="Attribute2" type="xsd:string" minOccurs="0"/>
         <xsd:element name="Attribute3" type="xsd:string" minOccurs="0"/>
         <xsd:element name="Attribute4" type="xsd:string" minOccurs="0"/>
         <xsd:element name="Attribute5" type="xsd:string" minOccurs="0"/>
         <xsd:element name="Attribute6" type="xsd:string" minOccurs="0"/>
         <xsd:element name="Attribute7" type="xsd:string" minOccurs="0"/>
         <xsd:element name="Attribute8" type="xsd:string" minOccurs="0"/>
         <xsd:element name="Attribute9" type="xsd:string" minOccurs="0"/>
         <xsd:element name="Attribute10" type="xsd:string" minOccurs="0"/>
         <xsd:element name="Attribute11" type="xsd:string" minOccurs="0"/>
         <xsd:element name="Attribute12" type="xsd:string" minOccurs="0"/>
         <xsd:element name="Attribute13" type="xsd:string" minOccurs="0"/>
         <xsd:element name="Attribute14" type="xsd:string" minOccurs="0"/>
         <xsd:element name="Attribute15" type="xsd:string" minOccurs="0"/>
         <xsd:element name="Attribute16" type="xsd:string" minOccurs="0"/>
         <xsd:element name="Attribute17" type="xsd:string" minOccurs="0"/>
         <xsd:element name="Attribute18" type="xsd:string" minOccurs="0"/>
         <xsd:element name="Attribute19" type="xsd:string" minOccurs="0"/>
         <xsd:element name="Attribute20" type="xsd:string" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="FlexFieldNumberType">
      <xsd:annotation>
         <xsd:documentation>Common type for tables which contain Flex Field columns for NUMBER data type</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="AttributeNumber1" type="xsd:string" minOccurs="0"/>
         <xsd:element name="AttributeNumber2" type="xsd:string" minOccurs="0"/>
         <xsd:element name="AttributeNumber3" type="xsd:string" minOccurs="0"/>
         <xsd:element name="AttributeNumber4" type="xsd:string" minOccurs="0"/>
         <xsd:element name="AttributeNumber5" type="xsd:string" minOccurs="0"/>
         <xsd:element name="AttributeNumber6" type="xsd:string" minOccurs="0"/>
         <xsd:element name="AttributeNumber7" type="xsd:string" minOccurs="0"/>
         <xsd:element name="AttributeNumber8" type="xsd:string" minOccurs="0"/>
         <xsd:element name="AttributeNumber9" type="xsd:string" minOccurs="0"/>
         <xsd:element name="AttributeNumber10" type="xsd:string" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="FlexFieldDateType">
      <xsd:annotation>
         <xsd:documentation>Common type for tables which contain Flex Field columns for DATE data type</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="AttributeDate1" type="GLogDateTimeType" minOccurs="0"/>
         <xsd:element name="AttributeDate2" type="GLogDateTimeType" minOccurs="0"/>
         <xsd:element name="AttributeDate3" type="GLogDateTimeType" minOccurs="0"/>
         <xsd:element name="AttributeDate4" type="GLogDateTimeType" minOccurs="0"/>
         <xsd:element name="AttributeDate5" type="GLogDateTimeType" minOccurs="0"/>
         <xsd:element name="AttributeDate6" type="GLogDateTimeType" minOccurs="0"/>
         <xsd:element name="AttributeDate7" type="GLogDateTimeType" minOccurs="0"/>
         <xsd:element name="AttributeDate8" type="GLogDateTimeType" minOccurs="0"/>
         <xsd:element name="AttributeDate9" type="GLogDateTimeType" minOccurs="0"/>
         <xsd:element name="AttributeDate10" type="GLogDateTimeType" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="FlexFieldCurrencyType">
      <xsd:annotation>
         <xsd:documentation>Common type for tables which contain Flex Field columns for CURRENCY data type</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="AttributeCurrency1" type="GLogXMLFinancialAmountType" minOccurs="0"/>
         <xsd:element name="AttributeCurrency2" type="GLogXMLFinancialAmountType" minOccurs="0"/>
         <xsd:element name="AttributeCurrency3" type="GLogXMLFinancialAmountType" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="IntegrationLogMessageType">
      <xsd:annotation>
         <xsd:documentation>An IntegrationLogMessage describes an error found in a transmission, transaction or any other type (like Message).</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ILogSeqNo" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>ILogSeqNo is a unique identifier for GLog Integration log message.
                  Validation: References I_LOG table.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ITransactionNo" type="xsd:string"/>
         <xsd:element name="ObjectGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="WrittenBy" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>WrittenBy specifies the GLog integration application that wrote the log message to the I_LOG table.
                  DataType: A30
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IMessageClass" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>IMessageClass specifies if the log message is an (E)rror, and (I)nformational message, or a (W)arning message.
                  DataType: A1
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IMessageCode" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>IMessageCode specifies the short name of the message.
                  DataType: A30 Example: PK_NOT_FOUND_ERROR
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IMessageText" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>IMessageText specifies the text of the log message.
                  DataType: LONG (Text)
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ProcessingErrorCodeGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="DateTimeStamp" type="GLogDateTimeType"/>
         <xsd:element name="DomainName" type="xsd:string"/>
         <xsd:element name="IntegrationLogDetail" type="IntegrationLogDetailType" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="IntegrationLogDetailType">
      <xsd:annotation>
         <xsd:documentation>IntegrationLogDetail is a structure containing the details of the log message - a qualifier/value pair</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ILogDetailQualGid" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>ILogDetailQualGid is reserved for future use.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ILogDetailValue" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>ILogDetailValue is reserved for future use.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="TextType">
      <xsd:annotation>
         <xsd:documentation>Used to specify the use of a text template.
            On inbound, either TextTemplateGid or TextTemplate can be specified.
            On outbound, only TextTemplateGid is provided.
            TextPatternOverride is inbound only.
            TextInstance is outbound only.
            TextPatternOverride and TextItem are currently applicable only to Documents.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:choice minOccurs="0">
            <xsd:element name="TextTemplateGid" type="GLogXMLGidType" minOccurs="0"/>
            <xsd:element name="TextTemplate" type="TextTemplateType" minOccurs="0"/>
         </xsd:choice>
         <xsd:element name="TextDocumentDefinition" type="TextDocumentDefinitionType" minOccurs="0"/>
         <xsd:element name="TextPatternOverride" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>The text pattern for a document, which overrides the text pattern of the containing text template.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TextItem" type="TextItemType" minOccurs="0"/>
         <xsd:element name="TextInstance" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Represents the result of applying the business object to the text pattern.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TransactionCode" type="TransactionCodeType" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="TextTemplateType">
      <xsd:annotation>
         <xsd:documentation>Used to specify a pre-formatted selection of text.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="TextTemplateGid" type="GLogXMLGidType"/>
         <xsd:element name="TextTemplatePattern" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>The text pattern for a text template, possibly containing embedded parameters.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DataQueryTypeGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="TextTemplateIsModifiable" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>If Y, the text template can be overridden by a document user.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TextTemplateDocumentDef" type="TextTemplateDocumentDefType" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element name="TransactionCode" type="TransactionCodeType" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="TextItemType">
      <xsd:annotation>
         <xsd:documentation>If the text was retrieved from an item-level template, this represents item context
            information needed by downstream systems.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="TextItemContextType" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Defines the type of item specifying the template (e.g. order release line).</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TextItemContextGid" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Based on the context type, a GID which identifies the line item (e.g. OR_LINE gid).</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TextItemContextSeq" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Based on the context type, a sequence # which identifies the line item.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="TextTemplateDocumentDefType">
      <xsd:annotation>
         <xsd:documentation>Represents the use of a text template for a particular document type.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="DocumentDefinitionGid" type="GLogXMLGidType"/>
         <xsd:element name="TextTemplateIsMandatory" type="xsd:string" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>If Y, the text template must be included with the document type. Otherwise,
                  it is only included if on the transaction. Defaults to N.In 6.3, this element has been deprecated. In may be removed in future releases.
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="TextDocumentDefinitionType">
      <xsd:annotation>
         <xsd:documentation>Specifies which documents should include a text template..</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="DocumentDefinitionGid" type="GLogXMLGidType"/>
         <xsd:element name="TextPatternOverride" type="xsd:string" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ProtectiveSvcType">
      <xsd:annotation>
         <xsd:documentation>ProtectiveSvc is an element of RailDetail used to specify protective service
            and ventilation instructions on a rail specific invoice.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ProtectiveSvcCode" type="xsd:string" minOccurs="0"/>
         <xsd:element name="ProtectiveSvcRuleCode" type="xsd:string" minOccurs="0"/>
         <xsd:element name="Temperature" type="TemperatureType" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="EquipMarksType">
      <xsd:annotation>
         <xsd:documentation>Specifies the list of equipment marks.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="EquipMarkGid" type="GLogXMLGidType" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="MarksType">
      <xsd:annotation>
         <xsd:documentation>Marks is a repeating structure containing MarksNumberQualifierGid and MarksNumber elements</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="MarksNumberQualifier" type="xsd:string"/>
         <xsd:element name="MarksNumber" type="xsd:string"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="SSContactType">
      <xsd:annotation>
         <xsd:documentation>SSContact is a structure containing various contact elements, such as Contact Function Code, Contact Name, Remarks.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="SSContactFunctionCode" type="xsd:string" minOccurs="0"/>
         <xsd:element name="SSContactName" type="xsd:string" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="PortType">
      <xsd:annotation>
         <xsd:documentation>port associated with an ocean stop.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="PortName" type="xsd:string" minOccurs="0"/>
         <xsd:element name="PortLocationFunctionCode" type="xsd:string"/>
         <xsd:element name="PortIdentifier" type="PortIdentifierType" minOccurs="0"/>
         <xsd:element name="ProvinceCode" type="xsd:string" minOccurs="0"/>
         <xsd:element name="CountryCode3Gid" type="GLogXMLGidType" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="PortIdentifierType">
      <xsd:annotation>
         <xsd:documentation>Used to identify the location of a port.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="PortIdentifierValue" type="xsd:string"/>
         <xsd:element name="PortIdentifierQualifier" type="xsd:string"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="ShipUnitCriteriaType">
      <xsd:annotation>
         <xsd:documentation>Refers to either a ShipUnit or a ObShipUnit for the Trans Order Status events.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:choice>
            <xsd:element name="IntSavedQuery" type="IntSavedQueryType"/>
            <xsd:element name="ShipUnitGid" type="GLogXMLGidType"/>
         </xsd:choice>
      </xsd:sequence>
   </xsd:complexType>

</xsd:schema>