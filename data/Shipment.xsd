                     <xsd:documentation>
                        Ship Unit Count of the Outbound Shipment.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="OutWeight" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Weight of the First Item of the Outbound Shipment.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="OutWeightUomCode" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Weight UOM of the First Item of the Outbound Shipment.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="OutWeightBase" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Weight in Base UOM of the First Item of the Outbound Shipment.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="OutPackagedItemGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Package Item ID of the First Item of the Outbound Shipment.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="OutItemGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Item Gid of the First Item of the Outbound Shipment.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="OutTHUGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Transport Handling Unit Gid of the First THU of the Outbound Shipment.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>

               <xsd:element name="DmEventDate1" type="GLogDateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Demurrage Transaction Event Date 1. Its a de-normalized Event Date 1 that gets populated via
                        status events.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="DmEventDate2" type="GLogDateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Demurrage Transaction Event Date 2. Its a de-normalized Event Date 2 that gets populated via
                        status events.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="DmEventDate3" type="GLogDateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Demurrage Transaction Event Date 3. Its a de-normalized Event Date 3 that gets populated via
                        status events.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="DmEventDate4" type="GLogDateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Demurrage Transaction Event Date 4. Its a de-normalized Event Date 4 that gets populated via
                        status events.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="DmEventDate5" type="GLogDateTimeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
