               <xsd:annotation>
                  <xsd:documentation>The driver's home location. References the LOCATION table.</xsd:documentation>
               </xsd:annotation>
               <xsd:complexType>
                  <xsd:sequence>
                     <xsd:element name="Location" type="LocationType"/>
                  </xsd:sequence>
               </xsd:complexType>
            </xsd:element>
         </xsd:choice>
         <xsd:element name="WorkRegionGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="DedAcctLocnProfileGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="CorporationProfileGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="ContactGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="RateOfferingGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="PrefConstraintConfigGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="AdjHireDate" type="GLogDateTimeType" minOccurs="0"/>
         <xsd:element name="EndDate" type="GLogDateTimeType" minOccurs="0"/>
         <xsd:element name="DriverTypeGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="ResourceType" type="xsd:string" minOccurs="0"/>
         <xsd:element name="UseHosHistory" type="xsd:string" minOccurs="0"/>
         <xsd:element name="IsActive" type="xsd:string" minOccurs="0"/>
         <xsd:element name="SightingLocGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="SightingDate" type="GLogDateTimeType" minOccurs="0"/>
         <xsd:element name="PrevSightingLocGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="PrevSightingDate" type="GLogDateTimeType" minOccurs="0"/>
         <xsd:element name="UserDefIconInfo" type="UserDefIconInfoType" minOccurs="0"/>
         <xsd:element name="Indicator" type="xsd:string" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="DriverTeamType">
      <xsd:annotation>
         <xsd:documentation>Defines Driver's Team information.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="IntSavedQuery" type="IntSavedQueryType" minOccurs="0"/>
         <xsd:element name="DriverTeamSeqNo" type="xsd:string" minOccurs="0"/>
         <xsd:element name="TransactionCode" type="TransactionCodeType" minOccurs="0"/>
         <xsd:element name="SecondaryDriverGid" type="GLogXMLGidType"/>
         <xsd:element name="EffectiveDate" type="GLogDateTimeType" minOccurs="0"/>
         <xsd:element name="ExpirationDate" type="GLogDateTimeType" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="DriverAssignmentType">
      <xsd:sequence>
         <xsd:element name="DriverAssignmtSeqNo" type="xsd:string"/>
         <xsd:element name="TransactionCode" type="TransactionCodeType"/>
         <xsd:element name="IntSavedQuery" type="IntSavedQueryType" minOccurs="0"/>
         <xsd:element name="Description" type="xsd:string"/>
         <xsd:element name="ShipmentGid" type="GLogXMLGidType"/>
         <xsd:element name="NextAvailLocGid" type="GLogXMLGidType"/>
         <xsd:element name="NextAvailTime" type="GLogDateTimeType"/>
         <xsd:element name="PrevDriverAssignmtSeqNo" type="xsd:string"/>
         <xsd:element name="IsSystemGenerated" type="xsd:string"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="DriverSpecialServiceType">
      <xsd:annotation>
         <xsd:documentation>Defines Driver's Special Service information.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="SpecialServiceGid" type="GLogXMLGidType"/>
         <xsd:element name="TransactionCode" type="TransactionCodeType" minOccurs="0"/>
         <xsd:element name="EffectiveDate" type="GLogDateTimeType" minOccurs="0"/>
         <xsd:element name="ExpirationDate" type="GLogDateTimeType" minOccurs="0"/>
         <xsd:element name="PayableIndicatorGid" type="GLogXMLGidType" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="DriverCdlType">
      <xsd:annotation>
         <xsd:documentation>Defines Driver's Commercial driver license information.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="TransactionCode" type="TransactionCodeType" minOccurs="0"/>
         <xsd:element name="CdlClass" type="xsd:string" minOccurs="0"/>
         <xsd:element name="CdlNum" type="xsd:string"/>
         <xsd:element name="CdlExpDate" type="GLogDateTimeType"/>
         <xsd:element name="CdlIssueCountryGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="CdlIssueState" type="xsd:string"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="DriverCalendarType">
      <xsd:annotation>
         <xsd:documentation>Defines Driver's Calendar information.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="HomeLocGid" type="GLogXMLGidType"/>
         <xsd:element name="ActivityCalendarGid" type="GLogXMLGidType"/>
         <xsd:element name="EffectiveDate" type="GLogDateTimeType"/>
         <xsd:element name="ExpirationDate" type="GLogDateTimeType"/>
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="DriverCalendarEventType">
      <xsd:annotation>
         <xsd:documentation>Describes a calender event for a specific driver</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="OTMTransactionIn">
            <xsd:sequence>
               <xsd:element name="IntSavedQuery" type="IntSavedQueryType" minOccurs="0"/>
               <xsd:element name="DriverCalEventGid" type="GLogXMLGidType" minOccurs="0"/>
               <xsd:element name="TransactionCode" type="TransactionCodeType"/>
               <xsd:element name="DriverGid" type="GLogXMLGidType"/>
               <xsd:element name="EventStartTime" type="GLogDateTimeType"/>
               <xsd:element name="EventEndTime" type="GLogDateTimeType"/>
               <xsd:element name="EventLocGid" type="GLogXMLGidType" minOccurs="0"/>
               <xsd:element name="CalendarEventTypeGid" type="GLogXMLGidType"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="DriverPowerUnitType">
      <xsd:sequence>
         <xsd:element name="SequenceNumber" type="xsd:string"/>
         <xsd:element name="PowerUnitGid" type="GLogXMLGidType"/>
         <xsd:element name="EffectiveDate" type="GLogDateTimeType"/>
         <xsd:element name="ExpirationDate" type="GLogDateTimeType"/>
      </xsd:sequence>
   </xsd:complexType>

</xsd:schema>