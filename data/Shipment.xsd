         <xsd:element name="DriverAssignBulkPlanGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="EquipmentAssignBulkPlanGid" type="GLogXMLGidType" minOccurs="0"/>
         <xsd:element name="BulkContMoveGid" type="GLogXMLGidType" minOccurs="0"/>
