                     <xsd:documentation>
                        Item Gid of the First Item of the Outbound Shipment.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="OutShipUnitCount" type="xsd:string" minOccurs="0">
