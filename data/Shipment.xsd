   <xsd:complexType name="DemurrageTransactionType">
      <xsd:annotation>
         <xsd:documentation>
            The demurrage trasaction is built based on events (like CLM, EDI322 etc) and help manage resouces (equipments) that
            are incuring costs. It includes charges (that are time based) called demurrage, detention and storage with no clear
            definition of terms.
         </xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="OTMTransactionOut">
            <xsd:sequence>
               <xsd:element name="ReportingScac" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        SCAC ID of the Service Provider.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>

               <xsd:element name="DmLocationGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Demurrage Transaction Location Unique Identifier. Reference table: LOCATION.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="DmTimeZoneGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Demurrage Transaction Location - Time Zone. Reference table: TIME_ZONE.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="DmCity" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Demurrage Transaction Location - City.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="DmState" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Demurrage Transaction Location - State.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="DmCountry" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Demurrage Transaction Location - Country.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="DmSplc" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Demurrage Transaction Location - SPLC Code.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>

               <xsd:element name="CpLocationGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Constructive Placement - Location. Constructive placement represent
                        place outside of shipper or consignees facility because of no access to deliver the resource.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="CpTimeZoneGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Constructive Placement - Time Zone. Reference table: TIME_ZONE.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="CpCity" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Constructive Placement - City.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="CpState" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Constructive Placement - State.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="CpCountry" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Constructive Placement - Country.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="CpSplc" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Constructive Placement - SPLC Code.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>

               <xsd:element name="EquipmentInitial" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        EquipmentInitial is typically the carriers standard alpha code.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="EquipmentNumber" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        The number identifier for the equipment for a particular carrier. This is used in conjunction
                        with a equipment initial specific for carrier.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="EquipmentInitialNumber" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        This should be a concatenation of the EquipmentInitial and the EquipmentNumber.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="EquipmentGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        EquipmentGid uniquely identifies an equipment record in the OTM.
                        Validation: References EQUIPMENT table.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="EquipmentTypeGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        EquipmentTypeGid uniquely identifies an equipment type record in the OTM.
                        Validation: References EQUIPMENT_TYPE table.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="EquipmentRefnumQualifierGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        EquipmentRefnumQualifierGid is a component of an EquipmentRefnum which qualifies the associated
                        EquipmentRefnumValue element. Validation:References EQUIPMENT_REFNUM_QUAL table.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="EquipmentRefnumValue" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        EquipmentRefnumValue is the value component of an EquipmentRefnum.
                        This is used as alternative way to identify unique equipment.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="AARCarType" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        The Association of American Railroads Car Type.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ChassisInitial" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Number identifying the chassis equipment used.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ChassisNumber" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Chassis number.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ChassisInitialNumber" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        This should be a concatenation of the ChassisInitial and the ChassisNumber.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>

               <xsd:element name="StartEventStatusCodeGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Demurrage Transaction Start Event ID. Reference table: BS_STATUS_CODE. It represents the event
                        that captured/started the demurrage transaction for a particular movement of resource
                        (equipment) or more like demurrage start clock event. Typically it would be constructive
                        placement event.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="StartLocationGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Start Event Location- Unique Identifier. Reference table: LOCATION.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="StartScacGid" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        SCAC code for the service provider associated with the Start Event.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="StartTimeZoneGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Start Event Location - Time Zone. Reference table: TIME_ZONE.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="StartCity" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Start Event Location - City.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="StartState" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Start Event Location - State.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="StartCountry" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Start Event Location - Country.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="StartSplc" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Start Event Location - SPLC Code.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="StartIsCarLoaded" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Is the equipment loaded or empty at the Start Event.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>

               <xsd:element name="EndEventStatusCodeGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Demurrage Transaction End Event ID. Reference table: BS_STATUS_CODE. It represents the event
                        that captured/ended the demurrage transaction for a particular movement of resource
                        (equipment) or more like demurrage end clock event. Typically it would be released event.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="EndLocationGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        Location associated with the End Event. Reference table: LOCATION.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="EndScacGid" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        SCAC Code of the service provider associated with the End Event.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="EndTimeZoneGid" type="GLogXMLGidType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        End Event Location - Time Zone. Reference table: TIME_ZONE.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="EndCity" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        End Event Location - City.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="EndState" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        End Event Location - State.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="EndCountry" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        End Event Location - Country.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="EndSplc" type="xsd:string" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>
                        End Event Location - SPLC Code.
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="EndIsCarLoaded" type="xsd:string" minOccurs="0">
