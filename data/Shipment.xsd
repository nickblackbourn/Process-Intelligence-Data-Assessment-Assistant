         <xsd:element name="TransactionCode" type="TransactionCodeType" minOccurs="0"/>
         <xsd:element name="StopDuration" type="GLogXMLDurationType" minOccurs="0"/>
         <xsd:element name="IsAppointment" type="xsd:string" minOccurs="0"/>
         <xsd:element name="LocationRef" type="LocationRefType" minOccurs="0"/>
         <xsd:element name="LocationOverrideRef" type="LocationOverrideRefType" minOccurs="0"/>
         <xsd:element name="ParentLocationRef" type="GLogXMLLocRefType" minOccurs="0"/>
         <xsd:element name="ArbitraryType" type="xsd:string" minOccurs="0"/>
